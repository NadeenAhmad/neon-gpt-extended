@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix ns1: <http://example.org/SewerNet#> .
@prefix ns2: <http://example.org/sewer#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:Flow a rdfs:Class ;
    rdfs:label "Flow" ;
    rdfs:comment "Represents the flow of sewage or water." .

ns1:SewerComponent a rdfs:Class,
        owl:Class ;
    rdfs:subClassOf ns1:NetworkComponent,
        ns1:SewerNetwork .

ns1:SewerNetwork a rdfs:Class,
        owl:Class,
        schema:Network ;
    rdfs:label "Sewer Network" ;
    ns1:hasComponent ns1:SewerComponent ;
    ns1:hasPart ns1:CatchmentArea,
        ns1:Manhole,
        ns1:Pipe ;
    rdfs:comment "A network of sewer pipes and associated components." ;
    rdfs:subClassOf <http://example.org/RAEPA#Network>,
        ns1:Network,
        owl:Thing ;
    schema:description "A network of pipes and sewer systems." .

ns1:SewerPipe a rdfs:Class,
        schema:Pipe ;
    rdfs:label "Sewer Pipe" ;
    rdfs:comment "A pipe in the sewer system." ;
    schema:description "A pipe used for sewer systems." .

<http://example.org/SewerNet> a owl:Ontology ;
    rdfs:label "SewerNet Ontology"@en ;
    rdfs:comment "The SewerNet Ontology provides a structured framework for representing the components and management processes of wastewater and stormwater networks."@en ;
    owl:versionInfo "1.0" .

ns1:BlockageDate a ns1:TemporalProperty ;
    rdfs:comment "Specifies the date on which a blockage occurred." ;
    rdfs:domain ns1:BlockageEvent ;
    rdfs:range xsd:date .

ns1:FlowRate a ns1:HydraulicProperty ;
    rdfs:comment "Represents the rate of flow through a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:decimal .

ns1:GeospatialReferenceGSR1 a ns1:GeospatialReference ;
    ns1:conformsTo ns1:RAEPAStandardV2 ;
    ns1:isUsedBy ns1:SewerNetwork1 ;
    rdfs:comment "Geospatial Reference GSR1 used by SewerNetwork1, conforming to RAEPA Standard V2." .

ns1:INSPIREDirective2020 a ns1:Directive ;
    ns1:hasPublicationDate "2020-01-01"^^xsd:date ;
    ns1:isImplementedBy ns1:SewerNetwork1 ;
    rdfs:comment "INSPIRE Directive published on January 1, 2020, implemented by SewerNetwork1." .

ns1:InletElevation a ns1:SpatialProperty ;
    rdfs:comment "Describes the elevation of an inlet relative to a reference datum." ;
    rdfs:domain ns1:Inlet ;
    rdfs:range xsd:decimal .

ns1:InletI1 a ns1:Inlet ;
    ns1:hasFlowRate 500 ;
    ns1:isConnectedTo ns1:PipeA1 ;
    rdfs:comment "Inlet I1 connected to Pipe A1 with a flow rate of 500 cubic meters per hour." .

ns1:Inspection1 a ns1:Inspection ;
    ns1:affects ns1:Pipe1 ;
    ns1:performs ns1:MaintenanceTeam1 ;
    time:hasTime ns1:TimeInterval1 .

ns1:InspectionEvent2023 a ns1:Inspection ;
    ns1:hasDate "2023-02-15"^^xsd:date ;
    ns1:occursAt ns1:Manhole456 ;
    rdfs:comment "Inspection event that occurred at Manhole 456 on February 15, 2023." .

ns1:InspectionResult a ns1:OperationalProperty ;
    rdfs:comment "Describes the outcome or result of an inspection event." ;
    rdfs:domain ns1:InspectionEvent ;
    rdfs:range xsd:string .

ns1:LastInspectionDate a ns1:TemporalProperty ;
    rdfs:comment "The date of the most recent inspection of a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:date .

ns1:MaintenanceActivityMA1 a ns1:Maintenance ;
    ns1:hasDescription "Cleaning and inspection of Pipe A1" ;
    ns1:isPerformedOn ns1:PipeA1 ;
    rdfs:comment "Maintenance activity performed on Pipe A1 for cleaning and inspection." .

ns1:MaintenanceCost a ns1:OperationalProperty ;
    rdfs:comment "Indicates the financial cost associated with a maintenance activity." ;
    rdfs:domain ns1:MaintenanceActivity ;
    rdfs:range xsd:decimal .

ns1:Manhole1 a ns1:Manhole ;
    ns1:hasLocation "POINT(48.8566 2.3522)"^^<http://inspire.ec.europa.eu/ontology#Geometry> .

ns1:ManholeDepth a ns1:StructuralProperty ;
    rdfs:comment "Represents the depth of a manhole." ;
    rdfs:domain ns1:Manhole ;
    rdfs:range xsd:decimal .

ns1:NextMaintenanceDate a ns1:TemporalProperty ;
    rdfs:comment "The scheduled date for the next maintenance activity on a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:date .

ns1:OutletElevation a ns1:SpatialProperty ;
    rdfs:comment "Describes the elevation of an outlet relative to a reference datum." ;
    rdfs:domain ns1:Outlet ;
    rdfs:range xsd:decimal .

ns1:OverflowVolume a ns1:OperationalProperty ;
    rdfs:comment "Represents the volume of wastewater or stormwater that overflowed." ;
    rdfs:domain ns1:OverflowEvent ;
    rdfs:range xsd:decimal .

ns1:PipeAge a ns1:StructuralProperty ;
    rdfs:comment "The age of a pipe, calculated from its installation date." ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:integer .

ns1:PipeLength a ns1:StructuralProperty ;
    rdfs:comment "Represents the length of a pipe." ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:PipeRoughnessCoefficient a ns1:HydraulicProperty ;
    rdfs:comment "Represents the roughness coefficient of a pipe, used in hydraulic modeling." ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:PipeSlope a ns1:HydraulicProperty ;
    rdfs:comment "Represents the slope of a pipe, affecting flow characteristics." ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:Pressure a ns1:HydraulicProperty ;
    rdfs:comment "Represents the water pressure within a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:decimal .

ns1:PumpingStationCapacity a ns1:HydraulicProperty ;
    rdfs:comment "Represents the maximum capacity of a pumping station." ;
    rdfs:domain ns1:PumpingStation ;
    rdfs:range xsd:decimal .

ns1:PumpingStationPS1 a ns1:PumpingStation ;
    ns1:hasCapacity 1000 ;
    ns1:isLocatedAt ns1:Location456 ;
    rdfs:comment "Pumping Station PS1 with a capacity of 1000 cubic meters, located at Location 456." .

ns1:RemainingServiceLife a ns1:StructuralProperty ;
    rdfs:comment "The estimated remaining service life of a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:integer .

ns1:RepairActivityRA1 a ns1:Repair ;
    ns1:hasDate "2023-03-01"^^xsd:date ;
    ns1:isDoneFor ns1:BlockageB1 ;
    rdfs:comment "Repair activity RA1 done for Blockage B1 on March 1, 2023." .

ns1:SeweNetOntology a owl:Ontology ;
    rdfs:label "SewerNet Ontology"@en ;
    rdfs:comment "An ontology for representing sewer network components, management processes, and related concepts."@en .

ns1:SewerFlow a owl:Class ;
    rdfs:subClassOf ns1:SewerNetworkComponent .

ns1:SewerNetOntology a owl:Ontology ;
    owl:imports rdf:_blank ;
    owl:versionInfo "1.0" .

ns1:SewerNetworkOntology a owl:Ontology ;
    owl:versionInfo "1.0" .

ns1:StructuralConditionScore a ns1:StructuralProperty ;
    rdfs:comment "Represents a score or rating of the structural condition of a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:integer .

ns1:affects a owl:ObjectProperty ;
    rdfs:label "affects"@en ;
    rdfs:comment "Relates an operational event to the network component it affects."@en ;
    rdfs:domain ns1:OperationalEvent ;
    rdfs:range ns1:NetworkComponent .

ns1:exampleSewerNetwork a ns1:SewerNetwork .

ns1:hasCapacity a owl:DatatypeProperty ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range xsd:float .

ns1:hasComponent a owl:ObjectProperty ;
    rdfs:label "has Component"@en ;
    rdfs:comment "Relates a network to one of its components."@en ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range ns1:NetworkComponent .

ns1:hasDescription a owl:DatatypeProperty .

ns1:hasDiameter a owl:DatatypeProperty ;
    rdfs:label "has Diameter"@en ;
    rdfs:comment "The diameter of a pipe."@en ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:hasFlowCapacity a owl:DatatypeProperty ;
    rdfs:label "has Flow Capacity"@en ;
    rdfs:comment "The flow capacity of a pipe or pumping station."@en ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:decimal .

ns1:hasFlowRate a owl:DatatypeProperty ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range xsd:float .

ns1:hasInlet a owl:ObjectProperty .

ns1:hasLocation a owl:DatatypeProperty ;
    rdfs:label "has Location"@en ;
    rdfs:comment "The geographic location of a network component."@en ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range <http://inspire.ec.europa.eu/ontology#Geometry> ;
    rdfs:subPropertyOf <http://inspire.ec.europa.eu/ontology#geometry> .

ns1:hasMaterial a owl:DatatypeProperty ;
    rdfs:label "has Material"@en ;
    rdfs:comment "The material a pipe is made of."@en ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:string .

ns1:hasNetworkComponent a owl:ObjectProperty ;
    rdfs:label "has network component"@en ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range ns1:SewerComponent .

ns1:hasOutlet a owl:ObjectProperty .

ns1:hasSlope a owl:DatatypeProperty ;
    rdfs:label "has Slope"@en ;
    rdfs:comment "The slope of a pipe."@en ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:hasSubNetwork a owl:ObjectProperty ;
    rdfs:label "has sub-network"@en ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range ns1:SewerNetwork ;
    rdfs:subPropertyOf ns1:hasPart .

ns1:isConnectedTo a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:label "is Connected To"@en,
        "is connected to"@en ;
    rdf:object ns1:SewerNetwork ;
    rdf:predicate ns1:connectedTo ;
    rdf:subject ns1:SewerNetwork ;
    rdfs:comment "A property indicating that two sewer components are connected."@en,
        "Indicates a connection between two network components."@en ;
    rdfs:domain ns1:NetworkComponent,
        ns1:SewerComponent ;
    rdfs:range ns1:NetworkComponent,
        ns1:SewerComponent .

ns1:performs a owl:ObjectProperty ;
    rdfs:label "performs"@en ;
    rdfs:comment "Relates an operational event to the entity performing it."@en ;
    rdfs:domain ns1:OperationalEvent ;
    rdfs:range <http://www.loa-cnr.it/ontologies/DOLCE-Lite#Agent> .

<http://example.org/SewerNet#sewer:hasPart> a owl:ObjectProperty ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range ns1:SewerComponent .

ns1:sewerNetworkStatus a owl:DatatypeProperty,
        owl:NamedIndividual .

ns1:sewer_node a rdf:Property ;
    rdfs:label "sewer node" ;
    rdfs:comment "A node in the sewer network, e.g., a manhole or a junction." .

<http://example.org/SewerNetOntology#hasSameFunctionAs> a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Indicates that a component serves the same function as another or itself."@en .

<http://example.org/SewerNetOntology#hasSameMaterialAs> a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Indicates that a component has the same material as another or itself."@en .

<http://example.org/SewerNetOntology#hasSameStructuralConditionAs> a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Indicates that a component has the same structural condition as another or itself."@en .

<http://example.org/SewerNetOntology#isAdjacentTo> a owl:ObjectProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Describes the adjacency relationship between network components."@en .

<http://example.org/SewerNetOntology#isAssociatedWith> a owl:ObjectProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "A general property indicating association between entities."@en .

<http://example.org/SewerNetOntology#isConnectedTo> a owl:ObjectProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Indicates that a network component is connected to itself or another component."@en .

<http://example.org/SewerNetOntology#isInSameGeographicLocationAs> a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Indicates that a component is located in the same geographic location as another or itself."@en .

<http://example.org/SewerNetOntology#isManagedBySameAuthorityAs> a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Indicates that a network component or event is managed by the same authority as another or itself."@en .

<http://example.org/SewerNetOntology#isPartOfSameNetworkAs> a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Denotes that a component is part of the same network as another or itself."@en .

<http://example.org/SewerNetOntology#isRelatedTo> a owl:ObjectProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "A general property for relating entities."@en .

<http://example.org/SewerNetOntology#isSimilarTo> a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Denotes similarity between network components or events."@en .

ns2:Blockage a owl:Class ;
    rdfs:label "Blockage"@en .

ns2:Fault a owl:Class ;
    rdfs:label "Fault"@en .

ns2:Inspection a owl:Class ;
    rdfs:label "Inspection"@en .

ns2:Maintenance a owl:Class ;
    rdfs:label "Maintenance"@en .

ns2:Repair a owl:Class ;
    rdfs:label "Repair"@en .

ns2:SewerNetwork a ns2:Network ;
    rdfs:label "Sewer Network"@en .

ns2:SewerSystem a ns2:Network ;
    rdfs:label "Sewer System"@en .

ns2:consistencyCheck a owl:AnnotationProperty ;
    rdfs:label "consistency check"@en .

ns2:creationDate a owl:AnnotationProperty ;
    rdfs:label "creation date"@en .

ns2:dataset a ns2:Dataset ;
    rdfs:label "Sewer dataset"@en .

ns2:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description"@en ;
    rdfs:domain ns2:Network ;
    rdfs:range xsd:string .

ns2:hasFunctionalProperty a owl:ObjectProperty ;
    rdfs:label "has functional property"@en .

ns2:hasIdentifier a owl:DatatypeProperty ;
    rdfs:label "has identifier"@en ;
    rdfs:domain ns2:Network ;
    rdfs:range xsd:string .

ns2:hasInverse a owl:ObjectProperty ;
    rdfs:label "has inverse"@en .

ns2:isAdjacentTo a owl:SymmetricProperty ;
    rdfs:comment "Indicates that two network components are adjacent to each other." ;
    rdfs:domain ns2:NetworkComponent ;
    rdfs:range ns2:NetworkComponent .

ns2:isConnectedTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "is connected to"@en ;
    rdfs:comment "Indicates that two network components are connected to each other." ;
    rdfs:domain ns2:NetworkComponent ;
    rdfs:range ns2:NetworkComponent ;
    owl:inverseOf ns2:connectedTo .

ns2:isInterconnectedWith a owl:SymmetricProperty ;
    rdfs:comment "Indicates that two pumping stations are interconnected." ;
    rdfs:domain ns2:PumpingStation ;
    rdfs:range ns2:PumpingStation .

ns2:isPartOf a owl:ObjectProperty ;
    rdfs:label "is part of"@en ;
    rdfs:comment "Relates a component to the network it belongs to."@en ;
    rdfs:domain ns2:Component ;
    rdfs:range ns2:Network ;
    owl:inverseOf ns2:hasPart .

<http://example.org/sewer-net#Inlet> a <http://example.org/sewer-net#NetworkComponent> ;
    rdfs:comment "A point where wastewater or stormwater enters the sewer network." .

<http://example.org/sewer-net#Outlet> a <http://example.org/sewer-net#NetworkComponent> ;
    rdfs:comment "A point where wastewater or stormwater exits the sewer network." .

<http://example.org/sewer-net#PipeDiameter> rdfs:comment "The diameter of a pipe." .

<http://example.org/sewer-net#PipeMaterial> rdfs:comment "The material of which a pipe is made." .

<http://example.org/sewer-net#addresses> a owl:ObjectProperty ;
    rdfs:label "addresses"@en ;
    rdfs:comment "Relates a maintenance activity to the structural condition it addresses."@en ;
    rdfs:domain <http://example.org/sewer-net#MaintenanceActivity> ;
    rdfs:range <http://example.org/sewer-net#StructuralCondition> .

<http://example.org/sewer-net#alignsWith> a owl:ObjectProperty ;
    rdfs:label "aligns with"@en ;
    rdfs:comment "Indicates that a sewer network aligns with a specific INSPIRE directive."@en ;
    rdfs:domain <http://example.org/sewer-net#SewerNetwork> ;
    rdfs:range <http://example.org/sewer-net#INSPIREDirective>,
        <http://inspire.ec.europa.eu/directive/INSPIREDirective> .

<http://example.org/sewer-net#blockageEvent> a <http://example.org/sewer-net#OperationalEvent> ;
    rdfs:comment "An event representing a blockage in the sewer network." .

<http://example.org/sewer-net#compliesWith> a owl:ObjectProperty ;
    rdfs:label "complies with"@en ;
    rdfs:comment "Indicates that a sewer network complies with a specific RAEPA standard."@en ;
    rdfs:domain <http://example.org/sewer-net#SewerNetwork> ;
    rdfs:range <http://example.org/RAEPA#RAEPAStandard>,
        <http://example.org/sewer-net#RAEPAStandard> .

<http://example.org/sewer-net#compliesWithINSPIRE> a <http://example.org/sewer-net#DataProperty> ;
    rdfs:comment "Indicates compliance with the INSPIRE European directive." .

<http://example.org/sewer-net#compliesWithRAEPA> a <http://example.org/sewer-net#DataProperty> ;
    rdfs:comment "Indicates compliance with the French RAEPA v1.2 geostandard." .

<http://example.org/sewer-net#conveys> a <http://example.org/sewer-net#ObjectProperty> ;
    rdfs:comment "The relationship between a pipe and the wastewater or stormwater it conveys." .

<http://example.org/sewer-net#flowCapacity> a owl:DatatypeProperty ;
    rdfs:domain <http://example.org/sewer-net#Pipe> ;
    rdfs:range xsd:float .

<http://example.org/sewer-net#hasBlockage> rdfs:comment "Inverse property of isBlockageOf, linking a blockage event to the component affected." ;
    owl:inverseOf <http://example.org/sewer-net#isBlockageOf> .

<http://example.org/sewer-net#hasDiameter> a <http://example.org/sewer-net#DataProperty> ;
    rdfs:comment "The diameter of a pipe, which is a critical attribute for hydraulic calculations." .

<http://example.org/sewer-net#hasFlowCapacity> a <http://example.org/sewer-net#DataProperty> ;
    rdfs:comment "The capacity of a pipe to convey flow." .

<http://example.org/sewer-net#hasInspectionDate> a <http://example.org/sewer-net#DataProperty> ;
    rdfs:comment "The date on which a network component was last inspected." .

<http://example.org/sewer-net#hasInspectionEvent> rdfs:comment "Inverse property of isInspectionEventOf, connecting an inspection event to the component inspected." ;
    owl:inverseOf <http://example.org/sewer-net#isInspectionEventOf> .

<http://example.org/sewer-net#hasMaintenanceActivity> rdfs:comment "Inverse property of isMaintenanceActivityOf, relating a maintenance activity to the component maintained." ;
    owl:inverseOf <http://example.org/sewer-net#isMaintenanceActivityOf> .

<http://example.org/sewer-net#hasMaintenanceDate> a <http://example.org/sewer-net#DataProperty> ;
    rdfs:comment "The date on which a network component was last maintained." .

<http://example.org/sewer-net#hasManhole> rdfs:comment "Inverse property of isManholeOf, linking a network to its manholes." ;
    owl:inverseOf <http://example.org/sewer-net#isManholeOf> .

<http://example.org/sewer-net#hasMaterial> a <http://example.org/sewer-net#DataProperty> ;
    rdfs:comment "The material of a pipe, which affects its durability and resistance to corrosion." .

<http://example.org/sewer-net#hasOverflow> rdfs:comment "Inverse property of isOverflowOf, connecting an overflow event to the component involved." ;
    owl:inverseOf <http://example.org/sewer-net#isOverflowOf> .

<http://example.org/sewer-net#hasPumpingStation> rdfs:comment "Inverse property of isPumpingStationOf, relating a network to its pumping stations." ;
    owl:inverseOf <http://example.org/sewer-net#isPumpingStationOf> .

<http://example.org/sewer-net#hasRepair> rdfs:comment "Inverse property of isRepairOf, relating a repair activity to the component repaired." ;
    owl:inverseOf <http://example.org/sewer-net#isRepairOf> .

<http://example.org/sewer-net#hasSlope> a <http://example.org/sewer-net#DataProperty> ;
    rdfs:comment "The slope of a pipe, which affects its hydraulic performance." .

<http://example.org/sewer-net#hasTime> a owl:ObjectProperty ;
    rdfs:domain <http://www.loa-cnr.it/ontologies/DOLCE-Lite#Event> ;
