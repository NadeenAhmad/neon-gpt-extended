<http://example.org/sewer-net#pipeDiameter> a owl:DatatypeProperty ;
    rdfs:domain <http://example.org/sewer-net#Pipe> ;
    rdfs:range xsd:float .

<http://example.org/sewer-net#pipeMaterial> a owl:DatatypeProperty ;
    rdfs:domain <http://example.org/sewer-net#Pipe> ;
    rdfs:range xsd:string .

