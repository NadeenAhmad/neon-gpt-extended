ns1:ChâteauLafiteRothschild2015 a ns1:Wine ;
    ns1:hasAcidity 6.0 ;
    ns1:hasAlcoholContent 13.0 ;
    ns1:hasResidualSugar 0.2 ;
    ns1:hasTanninLevel 4.0 .

ns1:DOCG a ns1:RegulatoryAspect ;
    rdfs:label "Denominazione di Origine Controllata e Garantita"@en ;
    rdfs:comment "The highest classification for Italian wines, guaranteeing their origin and quality."@en .

ns1:Flavor a ns1:WineAttribute ;
    rdfs:label "Flavor"@en ;
    rdfs:comment "The taste of a wine, encompassing its sweetness, acidity, tannins, and body."@en .

ns1:FortifiedWine a owl:Class ;
    rdfs:label "Fortified Wine"@en ;
    rdfs:subClassOf ns1:Wine .

ns1:Grape a owl:Class ;
    rdfs:label "Grape"@en ;
    rdfs:comment "A fruit used in winemaking."@en ;
    rdfs:subClassOf ns1:GrapeVariety .

ns1:GrilledSalmon a ns1:WinePairing ;
    rdfs:label "Grilled Salmon"@en ;
    rdfs:comment "A dish pairing option that goes well with certain white wines like Sauvignon Blanc."@en .

ns1:Irrigation a ns1:ViticulturalPractice ;
    rdfs:label "Irrigation"@en ;
    rdfs:comment "The application of controlled amounts of water to plants at needed intervals to support grape production."@en .

ns1:Merlot a ns1:GrapeVariety ;
    rdfs:label "Merlot"@en ;
    rdfs:comment "A red grape variety known for producing smooth, approachable wines with flavors of plum and blackberry."@en .

ns1:Montrachet a ns1:Wine ;
    ns1:classifiedAs <http://eurovoc.europa.eu/PDO_ProtectedDesignationOfOrigin> ;
    ns1:containsCompound ns1:Ethanol,
        ns1:TartaricAcid,
        <http://id.nlm.nih.gov/mesh/Ethanol> ;
    ns1:hasAcidity 6.2 ;
    ns1:hasAdditive ns1:AscorbicAcid,
        <http://id.nlm.nih.gov/mesh/AscorbicAcid> ;
    ns1:hasAlcoholContent 13.5 ;
    ns1:hasAroma <http://aims.fao.org/aos/agrovoc/Esters>,
        ns1:Esters,
        "Complex aromas of stone fruits, citrus, and toasted nuts."@en ;
    ns1:hasBitterness <http://aims.fao.org/aos/agrovoc/Tannin> ;
    ns1:hasBody <http://aims.fao.org/aos/agrovoc/Glycerol>,
        "Full-bodied with a rich texture."@en ;
    ns1:hasColor ns1:Anthocyanin ;
    ns1:hasComponent ns1:Glucose,
        ns1:Glycerol,
        ns1:LacticAcid,
        ns1:MalicAcid,
        ns1:Polysaccharides,
        ns1:Tannin,
        <http://id.nlm.nih.gov/mesh/TartaricAcid> ;
    ns1:hasFlavor <http://aims.fao.org/aos/agrovoc/Terpene>,
        ns1:Terpene ;
    ns1:hasIngredient ns1:Chardonnay ;
    ns1:hasPreservative ns1:SulfurDioxide,
        <http://id.nlm.nih.gov/mesh/SulfurDioxide> ;
    ns1:hasResidualSugar 0.5 ;
    ns1:hasSweetness <http://aims.fao.org/aos/agrovoc/Glucose> ;
    ns1:hasTanninLevel 3.5 ;
    ns1:hasVintage ns1:Vintage2015 ;
    ns1:hasViscosity <http://aims.fao.org/aos/agrovoc/Polysaccharides> ;
    ns1:hasWineAttribute ns1:Aroma ;
    ns1:isFrom ns1:Burgundy ;
    ns1:isMadeFrom ns1:Chardonnay ;
    ns1:isProducedBy ns1:DomaineDeLaRomanéeConti ;
    ns1:soldUnder <http://purl.obo.org/unspsc/50202201> .

ns1:OpusOne2018 a ns1:Wine ;
    ns1:hasAcidity 6.5 ;
    ns1:hasAlcoholContent 14.0 ;
    ns1:hasResidualSugar 0.3 ;
    ns1:hasTanninLevel 4.5 .

ns1:PinotNoir a ns1:GrapeVariety ;
    rdfs:label "Pinot Noir"@en ;
    ns1:isUsedIn ns1:Burgundy ;
    rdfs:comment "A red grape variety known for producing light-bodied wines with flavors of cherry and earth."@en .

ns1:RedWine a owl:Class ;
    rdfs:label "Red Wine"@en ;
    rdfs:subClassOf ns1:Wine .

ns1:ReverseOsmosis a ns1:WinemakingProcess ;
    rdfs:label "Reverse Osmosis"@en ;
    rdfs:comment "A winemaking technique used to adjust the concentration of grape must or wine."@en .

ns1:Riesling a ns1:GrapeVariety ;
    rdfs:label "Riesling"@en ;
    rdfs:comment "A white grape variety known for producing wines ranging from dry to sweet with high acidity."@en .

ns1:RoséWine a owl:Class ;
    rdfs:label "Rosé Wine"@en ;
    rdfs:subClassOf ns1:Wine .

ns1:SauvignonBlanc a ns1:GrapeVariety ;
    rdfs:label "Sauvignon Blanc"@en ;
    ns1:isUsedIn ns1:NapaValley ;
    rdfs:comment "A white grape variety known for crisp acidity and flavors of green apple and citrus."@en .

ns1:SparklingWine a owl:Class ;
    rdfs:label "Sparkling Wine"@en ;
    rdfs:subClassOf ns1:Wine .

ns1:SteakAuPoivre a ns1:WinePairing ;
    rdfs:label "Steak au Poivre"@en ;
    rdfs:comment "A pepper-crusted steak dish that pairs well with full-bodied red wines."@en .

ns1:Tuscany a ns1:GeographicalIndication ;
    rdfs:label "Tuscany"@en ;
    ns1:produces ns1:Chianti ;
    rdfs:comment "A region in central Italy known for producing Chianti and other renowned wines."@en .

ns1:Vintage2018 a ns1:Vintage ;
    rdfs:label "Vintage 2018"@en ;
    rdfs:comment "The harvest year 2018, notable for its weather conditions affecting grape quality and yield."@en .

ns1:Vintage2020 a ns1:Vintage ;
    rdfs:label "Vintage 2020"@en ;
    rdfs:comment "The harvest year 2020, characterized by its specific climatic conditions."@en .

ns1:WhiteWine a owl:Class ;
    rdfs:label "White Wine"@en ;
    rdfs:subClassOf ns1:Wine .

ns1:WineColor rdfs:domain ns1:Wine ;
    rdfs:range ns1:Color .

ns1:WineOntology a owl:Ontology ;
    rdfs:label "Wine Ontology"@en ;
    rdfs:comment "An ontology for describing wine-related concepts and relationships." .

ns1:WineRegion rdfs:subClassOf ns1:Region .

ns1:Winemaker rdfs:subClassOf ns1:Person .

ns1:classifiedAs a owl:ObjectProperty ;
    rdfs:label "classified as"@en ;
    rdfs:comment "The classification of a wine according to a protected designation of origin."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range <http://eurovoc.europa.eu/PDO_ProtectedDesignationOfOrigin> .

ns1:compliesWith a owl:ObjectProperty ;
    rdfs:label "complies with"@en ;
    rdfs:comment "The wine label complies with a specific labeling directive."@en ;
    rdfs:domain ns1:WineLabel ;
    rdfs:range <http://eurovoc.europa.eu/WineLabelingDirective> .

ns1:containsTannins a ns1:Tannin ;
    ns1:tanninLevel "high" .

ns1:hasAcidity a owl:DatatypeProperty ;
    rdfs:label "has acidity"@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:float .

ns1:hasAdditive a owl:ObjectProperty ;
    rdfs:label "has additive"@en ;
    rdfs:comment "An additive in the wine."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range <http://id.nlm.nih.gov/mesh/AscorbicAcid> ;
    rdfs:subPropertyOf ns1:containsCompound .

ns1:hasAgingVessel a owl:DatatypeProperty ;
    rdfs:label "has aging vessel"@en ;
    rdfs:comment "Describes the type of vessel used for aging the wine."@en ;
    rdfs:domain ns1:WinemakingProcess ;
    rdfs:range xsd:string .

ns1:hasAlcoholContent a owl:DatatypeProperty ;
    rdfs:label "has alcohol content"@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:float .

ns1:hasAppellation rdfs:subPropertyOf ns1:hasRegion .

ns1:hasAroma a owl:DatatypeProperty,
        owl:ObjectProperty ;
    rdfs:label "has aroma"@en ;
    rdfs:comment "Describes the aroma characteristics of a wine."@en,
        "The aroma profile of the wine."@en ;
    rdfs:domain ns1:Wine,
        ns1:WineAttribute ;
    rdfs:range <http://aims.fao.org/aos/agrovoc/Esters>,
        ns1:AromaComponent,
        xsd:string .

ns1:hasBitterness a owl:ObjectProperty ;
    rdfs:label "has bitterness"@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range <http://aims.fao.org/aos/agrovoc/Tannin> .

ns1:hasBody a owl:DatatypeProperty,
        owl:ObjectProperty ;
    rdfs:label "has body"@en ;
    rdfs:comment "Describes the body or weight of a wine in the mouth."@en ;
    rdfs:domain ns1:Wine,
        ns1:WineAttribute ;
    rdfs:range <http://aims.fao.org/aos/agrovoc/Glycerol>,
        ns1:WineBody,
        xsd:string .

ns1:hasClassificationSystem a owl:DatatypeProperty ;
    rdfs:label "has classification system"@en ;
    rdfs:comment "Describes the classification system used for wine under a regulatory aspect."@en ;
    rdfs:domain ns1:RegulatoryAspect ;
    rdfs:range xsd:string .

ns1:hasClimate a owl:DatatypeProperty ;
    rdfs:label "has climate"@en ;
    rdfs:comment "Describes the general climate characteristics of a geographical indication."@en ;
    rdfs:domain ns1:GeographicalIndication ;
    rdfs:range xsd:string .

ns1:hasClimaticConditions a owl:DatatypeProperty ;
    rdfs:label "has climatic conditions"@en ;
    rdfs:comment "Describes the climatic conditions during a vintage year."@en ;
    rdfs:domain ns1:Vintage ;
    rdfs:range xsd:string .

ns1:hasColor a owl:DatatypeProperty ;
    rdfs:label "has color"@en ;
    rdfs:comment "A property to describe the color of a wine." ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:string .

ns1:hasComponent a owl:ObjectProperty ;
    rdfs:label "has component"@en ;
    rdfs:comment "A component of the wine."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:Compound,
        <http://id.nlm.nih.gov/mesh/TartaricAcid> .

ns1:hasFermentationTechnique a owl:DatatypeProperty ;
    rdfs:label "has fermentation technique"@en ;
    rdfs:comment "Specifies the techniques used during the fermentation process."@en ;
    rdfs:domain ns1:WinemakingProcess ;
    rdfs:range xsd:string .

ns1:hasFlavor a owl:ObjectProperty ;
    rdfs:label "has flavor"@en ;
    rdfs:comment "A property to describe the flavor profile of a wine.",
        "The flavor profile of the wine."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range <http://aims.fao.org/aos/agrovoc/Terpene>,
        ns1:FlavorComponent .

ns1:hasFlavorProfile a owl:DatatypeProperty ;
    rdfs:label "has flavor profile"@en ;
    rdfs:comment "Describes the typical flavor characteristics associated with a grape variety."@en ;
    rdfs:domain ns1:GrapeVariety ;
    rdfs:range xsd:string .

ns1:hasGeneticLineage a owl:DatatypeProperty ;
    rdfs:label "has genetic lineage"@en ;
    rdfs:comment "Provides information on the genetic lineage or parentage of a grape variety."@en ;
    rdfs:domain ns1:GrapeVariety ;
    rdfs:range xsd:string .

ns1:hasGrapeVarietyInverse a owl:ObjectProperty ;
    rdfs:label "is grape variety of"@en ;
    rdfs:comment "The inverse property of hasGrapeVariety, indicating the wine that has a specific grape variety."@en ;
    rdfs:domain ns1:GrapeVariety ;
    rdfs:range ns1:Wine ;
    owl:inverseOf ns1:hasGrapeVariety .

ns1:hasHarvestDate a owl:DatatypeProperty ;
    rdfs:label "has harvest date"@en ;
    rdfs:comment "Specifies the date or period when the grapes were harvested for a vintage."@en ;
    rdfs:domain ns1:Vintage ;
    rdfs:range xsd:date .

ns1:hasHistory a owl:DatatypeProperty ;
    rdfs:label "has history"@en ;
    rdfs:comment "Provides historical information about a winery."@en ;
    rdfs:domain ns1:Winery ;
    rdfs:range xsd:string .

ns1:hasIngredient a owl:ObjectProperty ;
    rdfs:label "has ingredient"@en ;
    rdfs:comment "The grape variety used in the production of a wine."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:GrapeVariety .

ns1:hasLabelingRequirement a owl:DatatypeProperty ;
    rdfs:label "has labeling requirement"@en ;
    rdfs:comment "Specifies the labeling requirements for wine under a regulatory aspect."@en ;
    rdfs:domain ns1:RegulatoryAspect ;
    rdfs:range xsd:string .

ns1:hasPairingReason a owl:DatatypeProperty ;
    rdfs:label "has pairing reason"@en ;
    rdfs:comment "Provides reasons or rationale behind a specific wine and food pairing."@en ;
    rdfs:domain ns1:WinePairing ;
    rdfs:range xsd:string .

ns1:hasPestControlMethod a owl:DatatypeProperty ;
    rdfs:label "has pest control method"@en ;
    rdfs:comment "Describes the methods used for controlling pests in the vineyard."@en ;
    rdfs:domain ns1:ViticulturalPractice ;
    rdfs:range xsd:string .

ns1:hasPreservative a owl:ObjectProperty ;
    rdfs:label "has preservative"@en ;
    rdfs:comment "A preservative added to the wine."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range <http://id.nlm.nih.gov/mesh/SulfurDioxide> ;
    rdfs:subPropertyOf ns1:containsCompound .

ns1:hasProductionVolume a owl:DatatypeProperty ;
    rdfs:label "has production volume"@en ;
    rdfs:comment "Indicates the volume of wine produced by a winery."@en ;
    rdfs:domain ns1:Winery ;
    rdfs:range xsd:integer .

ns1:hasPruningMethod a owl:DatatypeProperty ;
    rdfs:label "has pruning method"@en ;
    rdfs:comment "Specifies the pruning technique used in vineyard management."@en ;
    rdfs:domain ns1:ViticulturalPractice ;
    rdfs:range xsd:string .

ns1:hasResidualSugar a owl:DatatypeProperty ;
    rdfs:label "has residual sugar"@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:float .

ns1:hasSameGrapeVariety a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "has same grape variety"@en ;
    rdfs:comment "Two wines have the same grape variety."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:Wine .

ns1:hasSameRegulatoryAspect a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "has same regulatory aspect"@en ;
    rdfs:comment "Two regulatory aspects are the same."@en ;
    rdfs:domain ns1:RegulatoryAspect ;
    rdfs:range ns1:RegulatoryAspect .

ns1:hasSameVintage a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "has same vintage"@en ;
    rdfs:comment "Two vintages are the same."@en ;
    rdfs:domain ns1:Vintage ;
    rdfs:range ns1:Vintage .

ns1:hasSameViticulturalPractice a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "has same viticultural practice"@en ;
    rdfs:comment "Two wineries have the same viticultural practice."@en ;
    rdfs:domain ns1:Winery ;
    rdfs:range ns1:Winery .

ns1:hasSameWinemakingProcess a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "has same winemaking process"@en ;
    rdfs:comment "Two wineries have the same winemaking process."@en ;
    rdfs:domain ns1:Winery ;
    rdfs:range ns1:Winery .

ns1:hasSoilType a owl:DatatypeProperty ;
    rdfs:label "has soil type"@en ;
    rdfs:comment "Specifies the types of soil found in a geographical indication."@en ;
    rdfs:domain ns1:GeographicalIndication ;
    rdfs:range xsd:string .

ns1:hasSugar rdfs:domain ns1:Wine ;
    rdfs:range ns1:SugarLevel .

ns1:hasSweetness a owl:ObjectProperty ;
    rdfs:label "has sweetness"@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range <http://aims.fao.org/aos/agrovoc/Glucose> .

ns1:hasTannin rdfs:domain ns1:Wine ;
    rdfs:range ns1:TanninLevel .

ns1:hasTanninLevel a owl:DatatypeProperty ;
    rdfs:label "has tannin level"@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:float .

ns1:hasVintageInverse a owl:ObjectProperty ;
    rdfs:label "is vintage of"@en ;
    rdfs:comment "The inverse property of hasVintage, indicating the wine that has a specific vintage."@en ;
    rdfs:domain ns1:Vintage ;
    rdfs:range ns1:Wine ;
    owl:inverseOf ns1:hasVintage .

ns1:hasViscosity a owl:ObjectProperty ;
    rdfs:label "has viscosity"@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range <http://aims.fao.org/aos/agrovoc/Polysaccharides> .

ns1:hasViticulturalPracticeInverse a owl:ObjectProperty ;
    rdfs:label "is practiced by"@en ;
    rdfs:comment "The inverse property of hasViticulturalPractice, indicating the winery that practices a specific viticultural practice."@en ;
    rdfs:domain ns1:ViticulturalPractice ;
    rdfs:range ns1:Winery ;
    owl:inverseOf ns1:hasViticulturalPractice .

ns1:hasWineAttributeInverse a owl:ObjectProperty ;
    rdfs:label "is attribute of"@en ;
    rdfs:comment "The inverse property of hasWineAttribute, indicating the wine that has a specific attribute."@en ;
    rdfs:domain ns1:WineAttribute ;
    rdfs:range ns1:Wine ;
    owl:inverseOf ns1:hasWineAttribute .

ns1:hasWinePairingInverse a owl:ObjectProperty ;
    rdfs:label "is pairing for"@en ;
    rdfs:comment "The inverse property of hasWinePairing, indicating the wine pairing that is associated with a wine."@en ;
    rdfs:domain ns1:WinePairing ;
    rdfs:range ns1:Wine ;
    owl:inverseOf ns1:hasWinePairing .

ns1:hasWinemakingProcessInverse a owl:ObjectProperty ;
    rdfs:label "is used by"@en ;
    rdfs:comment "The inverse property of hasWinemakingProcess, indicating the winery that uses a specific winemaking process."@en ;
    rdfs:domain ns1:WinemakingProcess ;
    rdfs:range ns1:Winery ;
    owl:inverseOf ns1:hasWinemakingProcess .

ns1:isAssociatedWith a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "is associated with"@en ;
    rdfs:comment "A geographical indication is associated with another."@en ;
    rdfs:domain ns1:GeographicalIndication ;
    rdfs:range ns1:GeographicalIndication .

ns1:isComparableTo a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "is comparable to"@en ;
    rdfs:comment "A wine pairing is comparable to another."@en ;
    rdfs:domain ns1:WinePairing ;
    rdfs:range ns1:WinePairing .

ns1:isFromInverse a owl:ObjectProperty ;
    rdfs:label "is origin of"@en ;
    rdfs:comment "The inverse property of isFrom, indicating the wine that originates from a geographical indication."@en ;
    rdfs:domain ns1:GeographicalIndication ;
    rdfs:range ns1:Wine ;
    owl:inverseOf ns1:isFrom .

ns1:isMadeFromInverse a owl:ObjectProperty ;
    rdfs:label "is used to make"@en ;
    rdfs:comment "The inverse property of isMadeFrom, indicating the wine made from a grape variety."@en ;
    rdfs:domain ns1:GrapeVariety ;
    rdfs:range ns1:Wine ;
    owl:inverseOf ns1:isMadeFrom .

ns1:isMature rdfs:domain ns1:Wine ;
    rdfs:range ns1:Maturity .

ns1:isPairedWith rdfs:inverseOf ns1:hasFoodPairing .

ns1:isProducedByInverse a owl:ObjectProperty ;
    rdfs:label "produces"@en ;
    rdfs:comment "The inverse property of isProducedBy, indicating the winery that produces a wine."@en ;
    rdfs:domain ns1:Winery ;
    rdfs:range ns1:Wine ;
    owl:inverseOf ns1:isProducedBy .

ns1:isRegulatedBy a owl:ReflexiveProperty ;
    rdfs:label "is regulated by"@en ;
    rdfs:comment "A regulatory aspect regulates itself."@en ;
    rdfs:domain ns1:RegulatoryAspect ;
    rdfs:range ns1:RegulatoryAspect .

ns1:isRelatedTo a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "is related to"@en ;
    rdfs:comment "A wine attribute is related to another."@en ;
    rdfs:domain ns1:WineAttribute ;
    rdfs:range ns1:WineAttribute .

ns1:isSimilarTo a owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "is similar to"@en ;
    rdfs:comment "A wine is similar to another wine."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:Wine .

ns1:isSubjectToInverse a owl:ObjectProperty ;
    rdfs:label "regulates"@en ;
    rdfs:comment "The inverse property of isSubjectTo, indicating the regulatory aspect that applies to a wine."@en ;
    rdfs:domain ns1:RegulatoryAspect ;
    rdfs:range ns1:Wine ;
    owl:inverseOf ns1:isSubjectTo .

ns1:madeFrom rdfs:comment "Relates a wine to its ingredients or the material from which it is made." .

<http://example.org/wine-ontology#owl:Class> rdfs:comment "The class of OWL classes." .

ns1:partOf a owl:ObjectProperty ;
    rdfs:label "part of"@en ;
    rdfs:comment "The vineyard is part of the broader domain of viticulture."@en ;
    rdfs:domain ns1:Vineyard ;
    rdfs:range <http://aims.fao.org/aos/agrovoc/Viticulture> .

<http://example.org/wine-ontology#rdfs:Class> rdfs:comment "The class of RDFS classes." .

ns1:soldUnder a owl:ObjectProperty ;
    rdfs:label "sold under"@en ;
    rdfs:comment "The UNSPSC code for the wine."@en ;
    rdfs:domain ns1:Wine ;
    rdfs:range <http://purl.obo.org/unspsc/50202201> .

rdf:type rdfs:comment "The subject's type or class." .

rdfs:label rdfs:comment "A human-readable label for the resource." .

ns1:Additive a owl:Class ;
    rdfs:label "Additive"@en ;
    rdfs:comment "A substance added to wine for a specific purpose."@en ;
    rdfs:subClassOf ns1:Compound .

ns1:AgingInOak a ns1:WinemakingProcess ;
    rdfs:label "Aging in Oak"@en ;
    rdfs:comment "The process of aging wine in oak barrels to impart flavors of vanilla and toast."@en .

ns1:Anthocyanin a ns1:Compound ;
    rdfs:label "Anthocyanin"@en .

ns1:Aroma a ns1:WineAttribute ;
    rdfs:label "Aroma"@en ;
    rdfs:comment "The smell of a wine, which can include a wide range of scents from fruits to spices."@en .

ns1:AscorbicAcid a ns1:Additive ;
    rdfs:label "Ascorbic Acid"@en .

ns1:Bordeaux a ns1:GeographicalIndication ;
