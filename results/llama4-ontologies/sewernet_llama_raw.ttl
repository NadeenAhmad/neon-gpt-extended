sn:isInspectionEventOf rdfs:domain sn:InspectionEvent ;
    rdfs:range sn:NetworkComponent ;
    rdfs:comment "Links an inspection event to the network component it involves." .

sn:hasMaintenanceActivity owl:inverseOf sn:isMaintenanceActivityOf ;
    rdfs:comment "Inverse property of isMaintenanceActivityOf, relating a maintenance activity to the component maintained." .

sn:isMaintenanceActivityOf rdfs:domain sn:MaintenanceActivity ;
    rdfs:range sn:NetworkComponent ;
    rdfs:comment "Connects a maintenance activity to the network component it is performed on." .

sn:hasBlockage owl:inverseOf sn:isBlockageOf ;
    rdfs:comment "Inverse property of isBlockageOf, linking a blockage event to the component affected." .

sn:isBlockageOf rdfs:domain sn:Blockage ;
    rdfs:range sn:NetworkComponent ;
    rdfs:comment "Relates a blockage to the network component where it occurs." .

sn:hasOverflow owl:inverseOf sn:isOverflowOf ;
    rdfs:comment "Inverse property of isOverflowOf, connecting an overflow event to the component involved." .

sn:isOverflowOf rdfs:domain sn:Overflow ;
    rdfs:range sn:NetworkComponent ;
    rdfs:comment "Links an overflow event to the network component it affects." .

sn:hasRepair owl:inverseOf sn:isRepairOf ;
    rdfs:comment "Inverse property of isRepairOf, relating a repair activity to the component repaired." .

sn:isRepairOf rdfs:domain sn:Repair ;
    rdfs:range sn:NetworkComponent ;
    rdfs:comment "Connects a repair activity to the network component it is performed on." .

sn:isLocatedNear owl:inverseOf sn:hasNearbyComponent ;
    rdfs:comment "Inverse property of hasNearbyComponent, indicating proximity between components." .

sn:hasNearbyComponent rdfs:domain sn:NetworkComponent ;
    rdfs:range sn:NetworkComponent ;
    rdfs:comment "Indicates that a network component has another component nearby." .

sn:isPartOfNetwork owl:inverseOf sn:hasNetworkComponent ;
    rdfs:comment "Inverse property of hasNetworkComponent, linking a component to its network." .

sn:hasNetworkComponent rdfs:domain sn:SewerNetwork ;
    rdfs:range sn:NetworkComponent ;
    rdfs:comment "Relates a sewer network to its components." .

sn:isManagedBy owl:inverseOf sn:manages ;
    rdfs:comment "Inverse property of manages, relating a network or component to its manager." .

sn:manages rdfs:domain sn:Operator ;
    rdfs:range sn:SewerNetwork ;
    rdfs:comment "Links an operator to the sewer network or components they manage." .

sn:isCompliantWith owl:inverseOf sn:complianceFor ;
    rdfs:comment "Inverse property of complianceFor, relating a standard to the components or networks compliant with it." .

sn:complianceFor rdfs:domain sn:Standard ;
    rdfs:range sn:SewerNetwork ;
    rdfs:comment "Connects a standard to the sewer networks or components that comply with it." .

