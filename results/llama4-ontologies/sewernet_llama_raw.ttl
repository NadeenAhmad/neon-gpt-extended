# Declare the namespace for the SewerNet Ontology
@prefix : <http://example.org/SewerNet#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dolce: <http://www.loa-cnr.it/ontologies/DOLCE-Lite#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix inspire: <http://inspire.ec.europa.eu/ontology#> .
@prefix raepa: <http://example.org/RAEPA#> .

# Define the ontology
:SeweNetOntology rdf:type owl:Ontology ;
    rdfs:label "SewerNet Ontology"@en ;
    rdfs:comment "An ontology for representing sewer network components, management processes, and related concepts."@en .

# Classes
:NetworkComponent rdf:type owl:Class ;
    rdfs:label "Network Component"@en ;
    rdfs:comment "A physical element within a sewer network."@en ;
    rdfs:subClassOf dolce:PhysicalObject .

:Pipe rdf:type owl:Class ;
    rdfs:label "Pipe"@en ;
    rdfs:comment "A tubular section conveying wastewater or stormwater."@en ;
    rdfs:subClassOf :NetworkComponent .

:Manhole rdf:type owl:Class ;
    rdfs:label "Manhole"@en ;
    rdfs:comment "An access point to a sewer network for maintenance."@en ;
    rdfs:subClassOf :NetworkComponent .

:Inlet rdf:type owl:Class ;
    rdfs:label "Inlet"@en ;
    rdfs:comment "A point of entry for water into the sewer network."@en ;
    rdfs:subClassOf :NetworkComponent .

:Outlet rdf:type owl:Class ;
    rdfs:label "Outlet"@en ;
    rdfs:comment "A point of discharge from the sewer network."@en ;
    rdfs:subClassOf :NetworkComponent .

:PumpingStation rdf:type owl:Class ;
    rdfs:label "Pumping Station"@en ;
    rdfs:comment "A facility that pumps wastewater or stormwater."@en ;
    rdfs:subClassOf :NetworkComponent .

:OperationalEvent rdf:type owl:Class ;
    rdfs:label "Operational Event"@en ;
    rdfs:comment "An event related to the management of the sewer network."@en ;
    rdfs:subClassOf dolce:Event .

:Inspection rdf:type owl:Class ;
    rdfs:label "Inspection"@en ;
    rdfs:comment "An examination of a network component."@en ;
    rdfs:subClassOf :OperationalEvent .

:MaintenanceActivity rdf:type owl:Class ;
    rdfs:label "Maintenance Activity"@en ;
    rdfs:comment "Work performed to maintain or repair the sewer network."@en ;
    rdfs:subClassOf :OperationalEvent .

# Object Properties
:hasComponent rdf:type owl:ObjectProperty ;
    rdfs:label "has Component"@en ;
    rdfs:comment "Relates a network to one of its components."@en ;
    rdfs:domain :SewerNetwork ;
    rdfs:range :NetworkComponent .

:isConnectedTo rdf:type owl:ObjectProperty ;
    rdfs:label "is Connected To"@en ;
    rdfs:comment "Indicates a connection between two network components."@en ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :NetworkComponent .

:performs rdf:type owl:ObjectProperty ;
    rdfs:label "performs"@en ;
    rdfs:comment "Relates an operational event to the entity performing it."@en ;
    rdfs:domain :OperationalEvent ;
    rdfs:range dolce:Agent .

:affects rdf:type owl:ObjectProperty ;
    rdfs:label "affects"@en ;
    rdfs:comment "Relates an operational event to the network component it affects."@en ;
    rdfs:domain :OperationalEvent ;
    rdfs:range :NetworkComponent .

# Data Properties
:hasDiameter rdf:type owl:DatatypeProperty ;
    rdfs:label "has Diameter"@en ;
    rdfs:comment "The diameter of a pipe."@en ;
    rdfs:domain :Pipe ;
    rdfs:range xsd:decimal .

:hasMaterial rdf:type owl:DatatypeProperty ;
    rdfs:label "has Material"@en ;
    rdfs:comment "The material a pipe is made of."@en ;
    rdfs:domain :Pipe ;
    rdfs:range xsd:string .

:hasSlope rdf:type owl:DatatypeProperty ;
    rdfs:label "has Slope"@en ;
    rdfs:comment "The slope of a pipe."@en ;
    rdfs:domain :Pipe ;
    rdfs:range xsd:decimal .

:hasFlowCapacity rdf:type owl:DatatypeProperty ;
    rdfs:label "has Flow Capacity"@en ;
    rdfs:comment "The flow capacity of a pipe or pumping station."@en ;
    rdfs:domain :NetworkComponent ;
    rdfs:range xsd:decimal .

:hasLocation rdf:type owl:DatatypeProperty ;
    rdfs:label "has Location"@en ;
    rdfs:comment "The geographic location of a network component."@en ;
    rdfs:domain :NetworkComponent ;
    rdfs:range inspire:Geometry .

# Axioms and Assertions
:Pipe1 rdf:type :Pipe ;
    :hasDiameter 0.5 ;
    :hasMaterial "Concrete" ;
    :hasSlope 0.02 .

:Manhole1 rdf:type :Manhole ;
    :hasLocation "POINT(48.8566 2.3522)"^^inspire:Geometry .

:Inspection1 rdf:type :Inspection ;
    :performs :MaintenanceTeam1 ;
    :affects :Pipe1 ;
    time:hasTime :TimeInterval1 .

:TimeInterval1 rdf:type time:Interval ;
    time:hasBeginning :TimeInstant1 ;
    time:hasEnd :TimeInstant2 .

:TimeInstant1 rdf:type time:Instant ;
    time:inXSDDateTime "2023-01-01T10:00:00"^^xsd:dateTime .

:TimeInstant2 rdf:type time:Instant ;
    time:inXSDDateTime "2023-01-01T11:00:00"^^xsd:dateTime .

# Align with RAEPA and INSPIRE
:NetworkComponent rdfs:subClassOf raepa:NetworkElement .
:SewerNetwork rdfs:subClassOf raepa:Network .
:hasLocation rdfs:subPropertyOf inspire:geometry .

