# Define the ontology
:Ontology rdf:type owl:Ontology ;
          rdfs:label "AquaDiva Ontology"@en ;
          rdfs:comment "Ontology for subsurface environmental microbiology"@en .

# Classes
:MicrobialTaxon rdf:type owl:Class ;
                rdfs:label "Microbial Taxon"@en ;
                rdfs:comment "A taxonomic classification of microorganisms"@en ;
                rdfs:subClassOf :Organism .

:FunctionalRole rdf:type owl:Class ;
                rdfs:label "Functional Role"@en ;
                rdfs:comment "A role played by microorganisms in an ecosystem"@en .

:SamplingEnvironment rdf:type owl:Class ;
                     rdfs:label "Sampling Environment"@en ;
                     rdfs:comment "An environment from which samples are taken"@en .

:MeasurementTechnique rdf:type owl:Class ;
                      rdfs:label "Measurement Technique"@en ;
                      rdfs:comment "A method used to measure environmental or biological parameters"@en .

:EnvironmentalVariable rdf:type owl:Class ;
                       rdfs:label "Environmental Variable"@en ;
                       rdfs:comment "A parameter that characterizes an environment"@en .

:BiogeochemicalProcess rdf:type owl:Class ;
                       rdfs:label "Biogeochemical Process"@en ;
                       rdfs:comment "A process involving the interaction of living organisms and the chemical and geological environment"@en .

:SpatialContext rdf:type owl:Class ;
                rdfs:label "Spatial Context"@en ;
                rdfs:comment "The spatial location and characteristics of a sampling environment"@en .

:TemporalContext rdf:type owl:Class ;
                 rdfs:label "Temporal Context"@en ;
                 rdfs:comment "The temporal characteristics of a sampling event or process"@en .

:Organism rdf:type owl:Class ;
          rdfs:label "Organism"@en ;
          rdfs:comment "A living organism"@en .

:Population rdf:type owl:Class ;
            rdfs:label "Population"@en ;
            rdfs:comment "A group of organisms of the same species living in a particular area"@en .

:EcosystemInteraction rdf:type owl:Class ;
                      rdfs:label "Ecosystem Interaction"@en ;
                      rdfs:comment "Interactions between organisms and their environment or between organisms"@en .

# Object Properties
:hasFunctionalRole rdf:type owl:ObjectProperty ;
                   rdfs:label "has Functional Role"@en ;
                   rdfs:comment "Relates a microbial taxon to its functional role in an ecosystem"@en ;
                   rdfs:domain :MicrobialTaxon ;
                   rdfs:range :FunctionalRole .

:isSampledIn rdf:type owl:ObjectProperty ;
             rdfs:label "is Sampled In"@en ;
             rdfs:comment "Relates a sampling event to the environment from which the sample was taken"@en ;
             rdfs:domain :SamplingEnvironment ;
             rdfs:range :SpatialContext .

:hasMeasurement rdf:type owl:ObjectProperty ;
                rdfs:label "has Measurement"@en ;
                rdfs:comment "Relates a measurement technique to the data it produces"@en ;
                rdfs:domain :MeasurementTechnique ;
                rdfs:range :EnvironmentalVariable .

:affects rdf:type owl:ObjectProperty ;
        rdfs:label "affects"@en ;
        rdfs:comment "Relates a biogeochemical process to the environmental variable it affects"@en ;
        rdfs:domain :BiogeochemicalProcess ;
        rdfs:range :EnvironmentalVariable .

:occursIn rdf:type owl:ObjectProperty ;
          rdfs:label "occurs In"@en ;
          rdfs:comment "Relates a biogeochemical process to the spatial context in which it occurs"@en ;
          rdfs:domain :BiogeochemicalProcess ;
          rdfs:range :SpatialContext .

:hasTemporalContext rdf:type owl:ObjectProperty ;
                    rdfs:label "has Temporal Context"@en ;
                    rdfs:comment "Relates a sampling event or process to its temporal context"@en ;
                    rdfs:domain :SamplingEnvironment ;
                    rdfs:range :TemporalContext .

:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:label "is Part Of"@en ;
          rdfs:comment "Relates a population to the ecosystem it is part of"@en ;
          rdfs:domain :Population ;
          rdfs:range :EcosystemInteraction .

# Data Properties
:haspH rdf:type owl:DatatypeProperty ;
       rdfs:label "has pH"@en ;
       rdfs:comment "The pH of an environment"@en ;
       rdfs:domain :EnvironmentalVariable ;
       rdfs:range xsd:float .

:hasTemperature rdf:type owl:DatatypeProperty ;
                 rdfs:label "has Temperature"@en ;
                 rdfs:comment "The temperature of an environment in degrees Celsius"@en ;
                 rdfs:domain :EnvironmentalVariable ;
                 rdfs:range xsd:float .

:hasOxygenLevel rdf:type owl:DatatypeProperty ;
                rdfs:label "has Oxygen Level"@en ;
                rdfs:comment "The oxygen level in an environment"@en ;
                rdfs:domain :EnvironmentalVariable ;
                rdfs:range xsd:float .

# Individuals
:Aquifer rdf:type :SamplingEnvironment ;
         rdfs:label "Aquifer"@en .

