# --- Turtle block from step_14_symmetric_properties ---
:isMadeFrom owl:inverseOf :hasGrapeVariety ;
           rdfs:comment "Inverse property of hasGrapeVariety, indicating the grape variety used to make a wine."@en ;
           rdf:type owl:SymmetricProperty .

:hasGrapeVariety rdf:type owl:SymmetricProperty ;
                 rdfs:comment "Indicates the grape variety used in the production of a wine, symmetric to isMadeFrom."@en .

:isProducedBy owl:inverseOf :producesWine ;
              rdfs:comment "Inverse property indicating the winery that produces a wine."@en .

:producesWine rdf:type owl:ObjectProperty ;
              rdfs:domain :Winery ;
              rdfs:range :Wine ;
              rdfs:label "produces wine"@en ;
              owl:inverseOf :isProducedBy ;
              rdfs:comment "Indicates the wines produced by a winery."@en .

:hasWinePairing rdf:type owl:SymmetricProperty ;
                rdfs:comment "Indicates a recommended pairing between a wine and a food item, symmetric in nature."@en .

:isFrom owl:inverseOf :producesWineFrom ;
        rdfs:comment "Inverse property indicating the geographical origin of a wine."@en .

:producesWineFrom rdf:type owl:ObjectProperty ;
                  rdfs:domain :GeographicalIndication ;
                  rdfs:range :Wine ;
                  rdfs:label "produces wine from"@en ;
                  owl:inverseOf :isFrom ;
                  rdfs:comment "Indicates wines originating from a specific geographical indication."@en .

:hasViticulturalPractice owl:inverseOf :isUsedIn ;
                         rdfs:comment "Inverse property indicating the viticultural practices employed by a winery."@en .

:isUsedIn rdf:type owl:ObjectProperty ;
          rdfs:domain :ViticulturalPractice ;
          rdfs:range :Winery ;
          rdfs:label "is used in"@en ;
          owl:inverseOf :hasViticulturalPractice ;
          rdfs:comment "Indicates the wineries that utilize a specific viticultural practice."@en .

:hasWinemakingProcess owl:inverseOf :isAppliedIn ;
                      rdfs:comment "Inverse property indicating the winemaking processes used by a winery."@en .

:isAppliedIn rdf:type owl:ObjectProperty ;
             rdfs:domain :WinemakingProcess ;
             rdfs:range :Winery ;
             rdfs:label "is applied in"@en ;
             owl:inverseOf :hasWinemakingProcess ;
             rdfs:comment "Indicates the wineries that apply a specific winemaking process."@en .

:hasWineAttribute owl:inverseOf :describesWine ;
                  rdfs:comment "Inverse property indicating the attributes that describe a wine."@en .

:describesWine rdf:type owl:ObjectProperty ;
               rdfs:domain :WineAttribute ;
               rdfs:range :Wine ;
               rdfs:label "describes wine"@en ;
               owl:inverseOf :hasWineAttribute ;
               rdfs:comment "Indicates the wines described by a specific wine attribute."@en .

:isSubjectTo owl:inverseOf :regulates ;
             rdfs:comment "Inverse property indicating the regulatory aspects that govern a wine."@en .

:regulates rdf:type owl:ObjectProperty ;
           rdfs:domain :RegulatoryAspect ;
           rdfs:range :Wine ;
           rdfs:label "regulates"@en ;
           owl:inverseOf :isSubjectTo ;
           rdfs:comment "Indicates the wines regulated by a specific regulatory aspect."@en .

# --- Turtle block from step_16_transitive_properties ---
:isMadeFrom rdf:type owl:TransitiveProperty ;
            rdfs:comment "If a wine is made from a grape variety, and that grape variety is a descendant of another, then the wine is also made from that ancestor grape variety."@en .

:hasViticulturalPractice rdf:type owl:TransitiveProperty ;
                         rdfs:comment "If a winery has a viticultural practice, and that practice involves another practice, then the winery also has that other practice."@en .

:hasWinemakingProcess rdf:type owl:TransitiveProperty ;
                       rdfs:comment "If a winery has a winemaking process, and that process involves another process, then the winery also has that other process."@en .

:isFrom rdf:type owl:TransitiveProperty ;
        rdfs:comment "If a wine is from a geographical indication, and that indication is part of a larger region, then the wine is also from that larger region."@en .

:isProducedBy rdf:type owl:TransitiveProperty ;
              rdfs:comment "If a wine is produced by a winery, and that winery is owned by another entity, then the wine is also produced by that other entity."@en .

:hasWineAttribute rdf:type owl:TransitiveProperty ;
                  rdfs:comment "If a wine has a certain attribute, and that attribute is related to another attribute, then the wine also has that other attribute."@en .

:hasWinePairing rdf:type owl:TransitiveProperty ;
                rdfs:comment "If a wine has a pairing suggestion, and that suggestion is related to another food or wine, then the wine also pairs with that other food or wine."@en .

:isSubjectTo rdf:type owl:TransitiveProperty ;
             rdfs:comment "If a wine is subject to a regulatory aspect, and that aspect is part of a larger regulatory framework, then the wine is also subject to that larger framework."@en .

