# Inverse Object Properties

:isMadeFromInverse rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isMadeFrom ;
                   rdfs:domain :GrapeVariety ;
                   rdfs:range :Wine ;
                   rdfs:label "is used to make"@en ;
                   rdfs:comment "The inverse property of isMadeFrom, indicating the wine made from a grape variety."@en .

:hasGrapeVarietyInverse rdf:type owl:ObjectProperty ;
                        owl:inverseOf :hasGrapeVariety ;
                        rdfs:domain :GrapeVariety ;
                        rdfs:range :Wine ;
                        rdfs:label "is grape variety of"@en ;
                        rdfs:comment "The inverse property of hasGrapeVariety, indicating the wine that has a specific grape variety."@en .

:hasViticulturalPracticeInverse rdf:type owl:ObjectProperty ;
                                owl:inverseOf :hasViticulturalPractice ;
                                rdfs:domain :ViticulturalPractice ;
                                rdfs:range :Winery ;
                                rdfs:label "is practiced by"@en ;
                                rdfs:comment "The inverse property of hasViticulturalPractice, indicating the winery that practices a specific viticultural practice."@en .

:hasWinemakingProcessInverse rdf:type owl:ObjectProperty ;
                             owl:inverseOf :hasWinemakingProcess ;
                             rdfs:domain :WinemakingProcess ;
                             rdfs:range :Winery ;
                             rdfs:label "is used by"@en ;
                             rdfs:comment "The inverse property of hasWinemakingProcess, indicating the winery that uses a specific winemaking process."@en .

:hasWineAttributeInverse rdf:type owl:ObjectProperty ;
                         owl:inverseOf :hasWineAttribute ;
                         rdfs:domain :WineAttribute ;
                         rdfs:range :Wine ;
                         rdfs:label "is attribute of"@en ;
                         rdfs:comment "The inverse property of hasWineAttribute, indicating the wine that has a specific attribute."@en .

:isFromInverse rdf:type owl:ObjectProperty ;
               owl:inverseOf :isFrom ;
               rdfs:domain :GeographicalIndication ;
               rdfs:range :Wine ;
               rdfs:label "is origin of"@en ;
               rdfs:comment "The inverse property of isFrom, indicating the wine that originates from a geographical indication."@en .

:isProducedByInverse rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isProducedBy ;
                     rdfs:domain :Winery ;
                     rdfs:range :Wine ;
                     rdfs:label "produces"@en ;
                     rdfs:comment "The inverse property of isProducedBy, indicating the winery that produces a wine."@en .

:hasVintageInverse rdf:type owl:ObjectProperty ;
                   owl:inverseOf :hasVintage ;
                   rdfs:domain :Vintage ;
                   rdfs:range :Wine ;
                   rdfs:label "is vintage of"@en ;
                   rdfs:comment "The inverse property of hasVintage, indicating the wine that has a specific vintage."@en .

:hasWinePairingInverse rdf:type owl:ObjectProperty ;
                       owl:inverseOf :hasWinePairing ;
                       rdfs:domain :WinePairing ;
                       rdfs:range :Wine ;
                       rdfs:label "is pairing for"@en ;
                       rdfs:comment "The inverse property of hasWinePairing, indicating the wine pairing that is associated with a wine."@en .

:isSubjectToInverse rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isSubjectTo ;
                    rdfs:domain :RegulatoryAspect ;
                    rdfs:range :Wine ;
                    rdfs:label "regulates"@en ;
                    rdfs:comment "The inverse property of isSubjectTo, indicating the regulatory aspect that applies to a wine."@en .

# --- Turtle block from step_13_reflexive_properties ---
:isSimilarTo rdf:type owl:SymmetricProperty, owl:ReflexiveProperty ;
             rdfs:domain :Wine ;
             rdfs:range :Wine ;
             rdfs:label "is similar to"@en ;
             rdfs:comment "A wine is similar to another wine."@en .

:hasSameGrapeVariety rdf:type owl:SymmetricProperty, owl:ReflexiveProperty ;
