@prefix vin: <http://example.org/vin#> .

wine:Merlot a wine:GrapeVariety ;
    rdfs:label "Merlot" .

vin:Chardonnay a wine:GrapeVariety ;
    rdfs:label "Chardonnay" .

wine:CabernetSauvignon a wine:GrapeVariety ;
    rdfs:label "Cabernet Sauvignon" .

