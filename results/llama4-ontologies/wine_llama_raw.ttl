                      rdfs:label "has same grape variety"@en ;
                      rdfs:comment "Two wines have the same grape variety."@en .

:hasSameViticulturalPractice rdf:type owl:SymmetricProperty, owl:ReflexiveProperty ;
                             rdfs:domain :Winery ;
                             rdfs:range :Winery ;
                             rdfs:label "has same viticultural practice"@en ;
                             rdfs:comment "Two wineries have the same viticultural practice."@en .

:hasSameWinemakingProcess rdf:type owl:SymmetricProperty, owl:ReflexiveProperty ;
                           rdfs:domain :Winery ;
                           rdfs:range :Winery ;
                           rdfs:label "has same winemaking process"@en ;
                           rdfs:comment "Two wineries have the same winemaking process."@en .

:isAssociatedWith rdf:type owl:SymmetricProperty, owl:ReflexiveProperty ;
                   rdfs:domain :GeographicalIndication ;
                   rdfs:range :GeographicalIndication ;
                   rdfs:label "is associated with"@en ;
                   rdfs:comment "A geographical indication is associated with another."@en .

:isRelatedTo rdf:type owl:SymmetricProperty, owl:ReflexiveProperty ;
             rdfs:domain :WineAttribute ;
             rdfs:range :WineAttribute ;
             rdfs:label "is related to"@en ;
             rdfs:comment "A wine attribute is related to another."@en .

:hasSameVintage rdf:type owl:SymmetricProperty, owl:ReflexiveProperty ;
