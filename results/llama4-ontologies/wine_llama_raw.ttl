# New Classes
:WineLabel rdf:type owl:Class ;
           rdfs:label "Wine Label"@en ;
           rdfs:comment "Information on the label of a wine."@en .

:Vineyard rdf:type owl:Class ;
          rdfs:label "Vineyard"@en ;
          rdfs:comment "A plot of land where grapes are cultivated."@en .

:Grape rdf:type owl:Class ;
       rdfs:subClassOf :GrapeVariety ;
       rdfs:label "Grape"@en ;
       rdfs:comment "A fruit used in winemaking."@en .

:Compound rdf:type owl:Class ;
          rdfs:label "Compound"@en ;
          rdfs:comment "A chemical substance present in wine."@en .

:Preservative rdf:type owl:Class ;
              rdfs:subClassOf :Compound ;
              rdfs:label "Preservative"@en ;
              rdfs:comment "A substance added to wine to prevent spoilage."@en .

:Additive rdf:type owl:Class ;
          rdfs:subClassOf :Compound ;
          rdfs:label "Additive"@en ;
          rdfs:comment "A substance added to wine for a specific purpose."@en .

:FlavorComponent rdf:type owl:Class ;
                 rdfs:subClassOf :Compound ;
                 rdfs:label "Flavor Component"@en ;
                 rdfs:comment "A compound contributing to the flavor of wine."@en .

:AromaComponent rdf:type owl:Class ;
                rdfs:subClassOf :Compound ;
                rdfs:label "Aroma Component"@en ;
                rdfs:comment "A compound contributing to the aroma of wine."@en .

# New Object Properties
:hasIngredient rdf:type owl:ObjectProperty ;
                rdfs:domain :Wine ;
                rdfs:range :GrapeVariety ;
                rdfs:label "has ingredient"@en ;
                rdfs:comment "The grape variety used in the production of a wine."@en .

:classifiedAs rdf:type owl:ObjectProperty ;
              rdfs:domain :Wine ;
              rdfs:range eurovoc:PDO_ProtectedDesignationOfOrigin ;
              rdfs:label "classified as"@en ;
              rdfs:comment "The classification of a wine according to a protected designation of origin."@en .

:containsCompound rdf:type owl:ObjectProperty ;
                  rdfs:domain :Wine ;
                  rdfs:range :Compound ;
                  rdfs:label "contains compound"@en ;
                  rdfs:comment "A chemical compound present in a wine."@en .

:compliesWith rdf:type owl:ObjectProperty ;
              rdfs:domain :WineLabel ;
              rdfs:range eurovoc:WineLabelingDirective ;
              rdfs:label "complies with"@en ;
              rdfs:comment "The wine label complies with a specific labeling directive."@en .

:hasComponent rdf:type owl:ObjectProperty ;
               rdfs:domain :Wine ;
               rdfs:range :Compound ;
               rdfs:label "has component"@en ;
               rdfs:comment "A component of the wine."@en .

:soldUnder rdf:type owl:ObjectProperty ;
           rdfs:domain :Wine ;
           rdfs:range unspsc:50202201 ;
           rdfs:label "sold under"@en ;
           rdfs:comment "The UNSPSC code for the wine."@en .

:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain :Vineyard ;
        rdfs:range agrovoc:Viticulture ;
        rdfs:label "part of"@en ;
        rdfs:comment "The vineyard is part of the broader domain of viticulture."@en .

:hasPreservative rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :containsCompound ;
                 rdfs:label "has preservative"@en ;
                 rdfs:comment "A preservative added to the wine."@en .

:hasAdditive rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :containsCompound ;
             rdfs:label "has additive"@en ;
             rdfs:comment "An additive in the wine."@en .

:hasFlavor rdf:type owl:ObjectProperty ;
           rdfs:domain :Wine ;
           rdfs:range :FlavorComponent ;
           rdfs:label "has flavor"@en ;
           rdfs:comment "The flavor profile of the wine."@en .

:hasAroma rdf:type owl:ObjectProperty ;
          rdfs:domain :Wine ;
          rdfs:range :AromaComponent ;
          rdfs:label "has aroma"@en ;
          rdfs:comment "The aroma profile of the wine."@en .

