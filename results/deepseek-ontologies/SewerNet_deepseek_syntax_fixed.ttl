    rdfs:subClassOf ns1:Property .

ns1:Blockage a owl:Class ;
    rdfs:label "Blockage" ;
    rdfs:comment "An event where a network component is obstructed." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:occursAt ;
            owl:someValuesFrom ns1:Pipe ],
        [ a owl:Restriction ;
            owl:onProperty ns1:occursAt ;
