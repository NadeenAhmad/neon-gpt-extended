    rdfs:range <http://www.semanticweb.org/swer-net#Blockage> .

<http://www.semanticweb.org/swer-net#targets> a owl:ObjectProperty ;
    rdfs:domain <http://www.semanticweb.org/swer-net#Inspection> ;
    rdfs:range <http://www.semanticweb.org/swer-net#NetworkComponent> .

ns1: a owl:Ontology ;
    rdfs:label "SewerNet Ontology" ;
    dcterms:created "2024-01-01"^^xsd:date ;
    dcterms:description "An ontology for representing wastewater and stormwater network components, properties, events, and geospatial information." ;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> ;
    dcterms:modified "2024-06-15"^^xsd:date ;
    dcterms:publisher "SewerNet Consortium" ;
    dcterms:title "SewerNet Ontology" ;
    vann:preferredNamespaceUri "http://www.semanticweb.org/swer-net-ontology#" ;
    rdfs:comment "A comprehensive semantic framework for representing wastewater and stormwater network infrastructure, including physical components, hydraulic properties, operational events, geospatial data, and compliance with international standards like RAEPA and INSPIRE." ;
    owl:imports <http://www.w3.org/2006/time> ;
    owl:versionInfo "1.0",
        "2.0" .

ns1:BoosterPumpingStation a owl:Class ;
    rdfs:label "Booster Pumping Station" ;
    rdfs:comment "A pumping station that increases pressure in the network." ;
    rdfs:subClassOf ns1:PumpingStation .

ns1:CCTVInspection a owl:Class ;
    rdfs:label "CCTV Inspection" ;
    rdfs:comment "Inspection using closed-circuit television cameras." ;
    rdfs:subClassOf ns1:Inspection .

ns1:CatchBasin a owl:Class ;
    rdfs:label "Catch Basin" ;
    rdfs:comment "An inlet with sediment collection capability." ;
    rdfs:subClassOf ns1:Inlet .

ns1:Cleaning a owl:Class ;
    rdfs:label "Cleaning" ;
    rdfs:comment "An event involving the removal of debris or sediment from network components." ;
    rdfs:subClassOf ns1:OperationalEvent .

ns1:CorrectiveMaintenance a owl:Class ;
    rdfs:label "Corrective Maintenance" ;
    rdfs:comment "Maintenance performed to correct existing defects." ;
    rdfs:subClassOf ns1:MaintenanceActivity .

ns1:CorrosionLevel a owl:Class ;
    rdfs:label "Corrosion Level" ;
    rdfs:comment "The extent of corrosion damage on a network component." ;
    rdfs:subClassOf ns1:StructuralProperty .

ns1:CrackSeverity a owl:Class ;
    rdfs:label "Crack Severity" ;
    rdfs:comment "The degree of cracking observed in a network component." ;
    rdfs:subClassOf ns1:StructuralProperty .

ns1:DropManhole a owl:Class ;
    rdfs:label "Drop Manhole" ;
    rdfs:comment "A manhole with vertical drop for flow energy dissipation." ;
    rdfs:subClassOf ns1:Manhole .

ns1:FlowVelocity a owl:Class ;
    rdfs:label "Flow Velocity" ;
    rdfs:comment "The speed at which water flows through a network component." ;
    rdfs:subClassOf ns1:HydraulicProperty .

ns1:GeospatialLine a owl:Class ;
    rdfs:label "Geospatial Line" ;
    rdfs:comment "A linear feature in geographic space." ;
    rdfs:subClassOf ns1:GeospatialLocation .

ns1:GeospatialPoint a owl:Class ;
    rdfs:label "Geospatial Point" ;
    rdfs:comment "A point location in geographic space." ;
    rdfs:subClassOf ns1:GeospatialLocation .

ns1:GeospatialPolygon a owl:Class ;
    rdfs:label "Geospatial Polygon" ;
    rdfs:comment "A polygonal area in geographic space." ;
    rdfs:subClassOf ns1:GeospatialLocation .

ns1:GravityPipe a owl:Class ;
    rdfs:label "Gravity Pipe" ;
    rdfs:comment "A pipe that relies on gravity for water flow." ;
    rdfs:subClassOf ns1:Pipe .

ns1:InspectionChamber a owl:Class ;
    rdfs:label "Inspection Chamber" ;
    rdfs:comment "A manhole designed specifically for inspection access." ;
    rdfs:subClassOf ns1:Manhole .

ns1:InspectionReport a owl:Class ;
    rdfs:label "Inspection Report" ;
    rdfs:comment "A documented record of an inspection event, including findings and recommendations." ;
    rdfs:subClassOf ns1:OperationalEvent .

ns1:Junction a owl:Class ;
    rdfs:label "Junction" ;
    rdfs:comment "A point where multiple pipes connect within the sewer network." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 3 ;
            owl:onProperty ns1:connectedTo ],
        [ a owl:Restriction ;
            owl:minCardinality 3 ;
            owl:onProperty ns1:connectedTo ],
        ns1:NetworkComponent .

ns1:LiftStation a owl:Class ;
    rdfs:label "Lift Station" ;
    rdfs:comment "A pumping station that lifts wastewater to higher elevations." ;
    rdfs:subClassOf ns1:PumpingStation .

ns1:Manhole123 a ns1:Manhole ;
    ns1:connectedTo ns1:MainSewerLine ;
    ns1:hasEvent ns1:RepairActivity456 ;
    ns1:hasID "MH-123" ;
    ns1:hasLocation ns1:Manhole123_Location ;
    ns1:hasMaterial "Precast Concrete" ;
    ns1:hasStructuralProperty ns1:StructuralProp1 ;
    rdfs:comment "Access point for inspection and maintenance on MainSewerLine." .

ns1:PressurePipe a owl:Class ;
    rdfs:label "Pressure Pipe" ;
    rdfs:comment "A pipe that uses pressure for water transport." ;
    rdfs:subClassOf ns1:Pipe .

ns1:PreventiveMaintenance a owl:Class ;
    rdfs:label "Preventive Maintenance" ;
    rdfs:comment "Maintenance performed to prevent future failures." ;
    rdfs:subClassOf ns1:MaintenanceActivity .

ns1:Rehabilitation a owl:Class ;
    rdfs:label "Rehabilitation" ;
    rdfs:comment "An event to restore or upgrade the functionality of network components." ;
    rdfs:subClassOf ns1:OperationalEvent .

ns1:RoughnessCoefficient a owl:Class ;
    rdfs:label "Roughness Coefficient" ;
    rdfs:comment "A measure of the internal surface roughness affecting flow resistance." ;
    rdfs:subClassOf ns1:HydraulicProperty .

ns1:StormwaterInlet a owl:Class ;
    rdfs:label "Stormwater Inlet" ;
    rdfs:comment "An inlet specifically for stormwater collection." ;
    rdfs:subClassOf ns1:Inlet .

ns1:StormwaterInlet45 a ns1:Inlet ;
    ns1:connectedTo ns1:MainSewerLine ;
    ns1:hasID "IN-045" ;
    ns1:hasLocation ns1:Inlet45_Location ;
    ns1:hasMaterial "Polymer Concrete" ;
    rdfs:comment "Stormwater inlet capturing runoff from Rue de Rivoli." .

ns1:StormwaterOutlet a owl:Class ;
    rdfs:label "Stormwater Outlet" ;
    rdfs:comment "An outlet for stormwater discharge." ;
    rdfs:subClassOf ns1:Outlet .

ns1:TreatmentOutlet a ns1:Outlet ;
    ns1:hasID "OUT-001" ;
    ns1:hasLocation ns1:Outlet_Location ;
    ns1:hasMaterial "Stainless Steel" ;
    rdfs:comment "Final discharge point to wastewater treatment plant." .

ns1:TreatmentPlant a owl:Class ;
    rdfs:label "Treatment Plant" ;
    rdfs:comment "A facility where wastewater is treated before discharge or reuse." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:hasDownstream ;
            owl:someValuesFrom ns1:Outlet ],
        [ a owl:Restriction ;
            owl:onProperty ns1:hasDownstream ;
            owl:someValuesFrom ns1:Outlet ],
        ns1:NetworkComponent .

ns1:Valve a owl:Class ;
    rdfs:label "Valve" ;
    rdfs:comment "A device that controls the flow of wastewater or stormwater in the network." ;
    rdfs:subClassOf ns1:NetworkComponent .

ns1:VisualInspection a owl:Class ;
    rdfs:label "Visual Inspection" ;
    rdfs:comment "Direct visual assessment of network components." ;
    rdfs:subClassOf ns1:Inspection .

ns1:WastewaterOutlet a owl:Class ;
    rdfs:label "Wastewater Outlet" ;
    rdfs:comment "An outlet for treated or untreated wastewater discharge." ;
    rdfs:subClassOf ns1:Outlet .

ns1:adjacentTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "adjacent to" ;
    rdfs:comment "Indicates spatial adjacency between network components." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range ns1:NetworkComponent .

ns1:alignsWith a owl:ObjectProperty ;
    rdfs:label "aligns with" ;
    rdfs:comment "Indicates alignment of a sewer network with the INSPIRE directive." ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range ns1:INSPIREDirective,
        ns1:INSPIREStandard .

ns1:compliesWithInverse a owl:ObjectProperty ;
    rdfs:label "is complied by" ;
    rdfs:comment "Relates a standard to the network components that comply with it." ;
    owl:inverseOf ns1:compliesWith .

ns1:connectedToInverse a owl:ObjectProperty ;
    rdfs:label "is connected to" ;
    rdfs:comment "Relates a network component to another that is hydraulically connected to it." ;
    owl:inverseOf ns1:connectedTo .

ns1:equivalentTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "equivalent to" ;
    rdfs:comment "Indicates semantic equivalence between standards." ;
    rdfs:domain ns1:Standard ;
    rdfs:range ns1:Standard .

ns1:hasAccuracy a owl:DatatypeProperty ;
    rdfs:label "has accuracy" ;
    rdfs:comment "The positional accuracy of the geographic coordinates in meters." ;
    rdfs:domain ns1:GeospatialLocation ;
    rdfs:range xsd:decimal .

ns1:hasBlockageCause a owl:DatatypeProperty ;
    rdfs:label "has blockage cause" ;
    rdfs:comment "The identified cause of the blockage (e.g., debris, roots, sediment)." ;
    rdfs:domain ns1:Blockage ;
    rdfs:range xsd:string .

ns1:hasBlockageSeverity a owl:DatatypeProperty ;
    rdfs:label "has blockage severity" ;
    rdfs:comment "The severity level of the blockage (e.g., partial, complete)." ;
    rdfs:domain ns1:Blockage ;
    rdfs:range xsd:string .

ns1:hasCatchmentArea a owl:DatatypeProperty ;
    rdfs:label "has catchment area" ;
    rdfs:comment "The surface area that drains to this inlet." ;
    rdfs:domain ns1:Inlet ;
    rdfs:range xsd:decimal .

ns1:hasCertificationBody a owl:DatatypeProperty ;
    rdfs:label "has certification body" ;
    rdfs:comment "The organization that certified compliance with the standard." ;
    rdfs:domain ns1:Standard ;
    rdfs:range xsd:string .

ns1:hasComplianceDate a owl:DatatypeProperty ;
    rdfs:label "has compliance date" ;
    rdfs:comment "The date when compliance with the standard was verified." ;
    rdfs:domain ns1:Standard ;
    rdfs:range xsd:date .

ns1:hasCondition a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "has condition" ;
    rdfs:comment "Each structural property has exactly one condition assessment.",
        "The structural condition of a network component.",
        "The structural condition rating of a network component." ;
    rdfs:domain ns1:StructuralCondition,
        ns1:StructuralProperty ;
    rdfs:range xsd:string .

ns1:hasConditionRating a owl:DatatypeProperty ;
    rdfs:label "has condition rating" ;
    rdfs:comment "A numerical rating of structural condition (1-5 scale)." ;
    rdfs:domain ns1:StructuralCondition ;
    rdfs:range xsd:integer .

ns1:hasCoordinateSystem a owl:DatatypeProperty ;
    rdfs:label "has coordinate system" ;
    rdfs:comment "The coordinate reference system used for spatial data (e.g., EPSG:4326)." ;
    rdfs:domain ns1:GeospatialLocation ;
    rdfs:range xsd:string .

ns1:hasCorrosionLevel a owl:DatatypeProperty ;
    rdfs:label "has corrosion level" ;
    rdfs:comment "The level of corrosion observed (e.g., low, medium, high)." ;
    rdfs:domain ns1:StructuralProperty ;
    rdfs:range xsd:string .

ns1:hasCoverType a owl:DatatypeProperty ;
    rdfs:label "has cover type" ;
    rdfs:comment "The type of cover installed on the manhole (e.g., solid, grated)." ;
    rdfs:domain ns1:Manhole ;
    rdfs:range xsd:string .

ns1:hasCrackWidth a owl:DatatypeProperty ;
    rdfs:label "has crack width" ;
    rdfs:comment "The maximum width of cracks observed in the component." ;
    rdfs:domain ns1:StructuralProperty ;
    rdfs:range xsd:decimal .

ns1:hasDeflection a owl:DatatypeProperty ;
    rdfs:label "has deflection" ;
    rdfs:comment "The degree of structural deflection from original shape." ;
    rdfs:domain ns1:StructuralProperty ;
    rdfs:range xsd:decimal .

ns1:hasDepth a owl:DatatypeProperty ;
    rdfs:label "has depth" ;
    rdfs:comment "The burial depth of the pipe from ground surface." ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description" ;
    rdfs:comment "A textual description of the network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:string .

ns1:hasDischargePoint a owl:DatatypeProperty ;
    rdfs:label "has discharge point" ;
    rdfs:comment "The description of where the outlet discharges (e.g., river, treatment plant)." ;
    rdfs:domain ns1:Outlet ;
    rdfs:range xsd:string .

ns1:hasElevation a owl:DatatypeProperty ;
    rdfs:label "has elevation" ;
    rdfs:comment "The elevation above sea level of the location." ;
    rdfs:domain ns1:GeospatialLocation ;
    rdfs:range xsd:decimal .

ns1:hasEndTimeInverse a owl:ObjectProperty ;
    rdfs:label "is end time of" ;
    rdfs:comment "Relates a time instant to the operational event it ends." ;
    owl:inverseOf ns1:hasEndTime .

ns1:hasEventID a owl:DatatypeProperty ;
    rdfs:label "has event ID" ;
    rdfs:comment "Unique identifier for the operational event." ;
    rdfs:domain ns1:OperationalEvent ;
    rdfs:range xsd:string .

ns1:hasEventInverse a owl:ObjectProperty ;
    rdfs:label "is event of" ;
    rdfs:comment "Relates an operational event to the network component it affects." ;
    owl:inverseOf ns1:hasEvent .

ns1:hasFlowCapacity a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "has flow capacity" ;
    rdfs:comment "Each pipe has exactly one flow capacity value.",
        "The maximum flow capacity of a pipe." ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:hasFlowVelocity a owl:DatatypeProperty ;
    rdfs:label "has flow velocity" ;
    rdfs:comment "The velocity of water flow in a network component.",
        "The velocity of water flow in the network component." ;
    rdfs:domain ns1:HydraulicCondition,
        ns1:HydraulicProperty ;
    rdfs:range xsd:decimal .

ns1:hasFroudeNumber a owl:DatatypeProperty ;
    rdfs:label "has Froude number" ;
    rdfs:comment "The dimensionless Froude number indicating flow regime." ;
    rdfs:domain ns1:HydraulicProperty ;
    rdfs:range xsd:decimal .

ns1:hasGrateType a owl:DatatypeProperty ;
    rdfs:label "has grate type" ;
    rdfs:comment "The type of grate covering the inlet." ;
    rdfs:domain ns1:Inlet ;
    rdfs:range xsd:string .

ns1:hasHydraulicCondition a owl:ObjectProperty ;
    rdfs:label "has hydraulic condition" ;
    rdfs:comment "Relates a network component to its hydraulic condition." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range ns1:HydraulicCondition ;
    rdfs:subPropertyOf ns1:hasHydraulicProperty .

ns1:hasHydraulicGradient a owl:DatatypeProperty ;
    rdfs:label "has hydraulic gradient" ;
    rdfs:comment "The slope of the hydraulic grade line." ;
    rdfs:domain ns1:HydraulicProperty ;
    rdfs:range xsd:decimal .

ns1:hasHydraulicPropertyInverse a owl:ObjectProperty ;
    rdfs:label "is hydraulic property of" ;
    rdfs:comment "Relates a hydraulic property to the network component it characterizes." ;
    owl:inverseOf ns1:hasHydraulicProperty .

ns1:hasID a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "has ID" ;
    rdfs:comment "Each network component has exactly one unique identifier.",
        "The unique identifier of a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:string .

ns1:hasInspectionDate a owl:DatatypeProperty ;
    rdfs:label "has inspection date" ;
    rdfs:comment "The date when an inspection was performed." ;
    rdfs:domain ns1:Inspection ;
    rdfs:range xsd:date .

ns1:hasInspectionFrequency a owl:DatatypeProperty ;
    rdfs:label "has inspection frequency" ;
    rdfs:comment "The recommended frequency of inspections for a network component in months." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:integer .

ns1:hasInspectionMethod a owl:DatatypeProperty ;
    rdfs:label "has inspection method" ;
    rdfs:comment "The method used for inspection (e.g., CCTV, sonar, visual)." ;
    rdfs:domain ns1:Inspection ;
    rdfs:range xsd:string .

ns1:hasInspectionScore a owl:DatatypeProperty ;
    rdfs:label "has inspection score" ;
    rdfs:comment "Numerical score representing the condition assessment from inspection." ;
    rdfs:domain ns1:Inspection ;
    rdfs:range xsd:integer .

ns1:hasInstallationDate a owl:DatatypeProperty ;
    rdfs:label "has installation date" ;
    rdfs:comment "The date when a network component was installed.",
        "The date when the network component was installed." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:date .

ns1:hasInvertElevation a owl:DatatypeProperty ;
    rdfs:label "has invert elevation" ;
    rdfs:comment "The elevation of the lowest internal point in the manhole." ;
    rdfs:domain ns1:Manhole ;
    rdfs:range xsd:decimal .

ns1:hasJointCondition a owl:DatatypeProperty ;
    rdfs:label "has joint condition" ;
    rdfs:comment "The condition of pipe joints (e.g., sealed, leaking, displaced)." ;
    rdfs:domain ns1:StructuralProperty ;
    rdfs:range xsd:string .

ns1:hasLastUpdated a owl:DatatypeProperty ;
    rdfs:label "has last updated" ;
    rdfs:comment "The date and time when the component information was last updated." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:dateTime .

ns1:hasLength a owl:DatatypeProperty ;
    rdfs:label "has length" ;
    rdfs:comment "The linear length of the pipe segment." ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:hasLocationInverse a owl:ObjectProperty ;
    rdfs:label "is location of" ;
    rdfs:comment "Relates a geospatial location to the network component it locates." ;
    owl:inverseOf ns1:hasLocation .

ns1:hasMaintenanceCost a owl:DatatypeProperty ;
    rdfs:label "has maintenance cost" ;
    rdfs:comment "The cost associated with the maintenance activity." ;
    rdfs:domain ns1:Maintenance ;
    rdfs:range xsd:decimal .

ns1:hasMaintenanceDate a owl:DatatypeProperty ;
    rdfs:label "has maintenance date" ;
    rdfs:comment "The date when maintenance was performed." ;
    rdfs:domain ns1:MaintenanceActivity ;
    rdfs:range xsd:date .

ns1:hasMaintenanceType a owl:DatatypeProperty ;
    rdfs:label "has maintenance type" ;
    rdfs:comment "The type of maintenance performed (e.g., preventive, corrective, emergency)." ;
    rdfs:domain ns1:Maintenance ;
    rdfs:range xsd:string .

ns1:hasMaterial a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "has material" ;
    rdfs:comment "Each network component has exactly one material composition.",
        "The material composition of a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:string .

ns1:hasMaterialGrade a owl:DatatypeProperty ;
    rdfs:label "has material grade" ;
    rdfs:comment "The quality grade of the material used in a network component." ;
    rdfs:domain ns1:MaterialProperty ;
    rdfs:range xsd:string .

ns1:hasMaterialProperty a owl:ObjectProperty ;
    rdfs:label "has material property" ;
    rdfs:comment "Relates a network component to its material properties." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range ns1:MaterialProperty ;
    rdfs:subPropertyOf ns1:hasProperty .

ns1:hasMaterialType a owl:DatatypeProperty ;
    rdfs:label "has material type" ;
    rdfs:comment "The specific type of material used in a network component." ;
    rdfs:domain ns1:MaterialProperty ;
    rdfs:range xsd:string .

ns1:hasNumberOfPumps a owl:DatatypeProperty ;
    rdfs:label "has number of pumps" ;
    rdfs:comment "The total number of pumps installed in the pumping station." ;
    rdfs:domain ns1:PumpingStation ;
    rdfs:range xsd:integer .

ns1:hasOverflowDuration a owl:DatatypeProperty ;
    rdfs:label "has overflow duration" ;
    rdfs:comment "The duration of the overflow event in hours." ;
    rdfs:domain ns1:Overflow ;
    rdfs:range xsd:decimal .

ns1:hasOverflowVolume a owl:DatatypeProperty ;
    rdfs:label "has overflow volume" ;
    rdfs:comment "The volume of water that overflowed during the event." ;
    rdfs:domain ns1:Overflow ;
    rdfs:range xsd:decimal .

ns1:hasOwner a owl:DatatypeProperty ;
    rdfs:label "has owner" ;
    rdfs:comment "The entity that owns the network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:string .

ns1:hasPriority a owl:DatatypeProperty ;
    rdfs:label "has priority" ;
    rdfs:comment "The priority level of the event (e.g., low, medium, high, critical)." ;
    rdfs:domain ns1:OperationalEvent ;
    rdfs:range xsd:string .

ns1:hasPropertyInverse a owl:ObjectProperty ;
    rdfs:label "is property of" ;
    rdfs:comment "Relates a property to the network component it characterizes." ;
    owl:inverseOf ns1:hasProperty .

ns1:hasPumpCapacity a owl:DatatypeProperty ;
    rdfs:label "has pump capacity" ;
    rdfs:comment "The maximum flow rate capacity of the pumping station." ;
    rdfs:domain ns1:PumpingStation ;
    rdfs:range xsd:decimal .

ns1:hasRepairDuration a owl:DatatypeProperty ;
    rdfs:label "has repair duration" ;
    rdfs:comment "The time taken to complete the repair in hours." ;
    rdfs:domain ns1:Repair ;
    rdfs:range xsd:decimal .

ns1:hasRepairMethod a owl:DatatypeProperty ;
    rdfs:label "has repair method" ;
    rdfs:comment "The method used for repair (e.g., patching, lining, replacement)." ;
    rdfs:domain ns1:Repair ;
    rdfs:range xsd:string .

ns1:hasReportedBy a owl:DatatypeProperty ;
    rdfs:label "has reported by" ;
    rdfs:comment "The person or entity that reported the event." ;
    rdfs:domain ns1:OperationalEvent ;
    rdfs:range xsd:string .

ns1:hasRiskLevel a owl:DatatypeProperty ;
    rdfs:label "has risk level" ;
    rdfs:comment "The assessed risk level of the component (e.g., low, medium, high)." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:string .

ns1:hasRiskScore a owl:DatatypeProperty ;
    rdfs:label "has risk score" ;
    rdfs:comment "A numerical score representing the failure risk of a network component." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:decimal .

ns1:hasRoughnessCoefficient a owl:DatatypeProperty ;
    rdfs:label "has roughness coefficient" ;
    rdfs:comment "The Manning's roughness coefficient for hydraulic calculations.",
        "The Manning's roughness coefficient of a pipe." ;
    rdfs:domain ns1:Pipe ;
    rdfs:range xsd:decimal .

ns1:hasServiceStatus a owl:DatatypeProperty ;
    rdfs:label "has service status" ;
    rdfs:comment "The operational status of the network component (e.g., active, inactive, decommissioned)." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range xsd:string .

ns1:hasStandardVersion a owl:DatatypeProperty ;
    rdfs:label "has standard version" ;
    rdfs:comment "The version number of the standard being complied with." ;
    rdfs:domain ns1:Standard ;
    rdfs:range xsd:string .

ns1:hasStartTimeInverse a owl:ObjectProperty ;
    rdfs:label "is start time of" ;
    rdfs:comment "Relates a time instant to the operational event it starts." ;
    owl:inverseOf ns1:hasStartTime .

ns1:hasStructuralCondition a owl:ObjectProperty ;
    rdfs:label "has structural condition" ;
    rdfs:comment "Relates a network component to its structural condition." ;
    rdfs:domain ns1:NetworkComponent ;
    rdfs:range ns1:StructuralCondition ;
    rdfs:subPropertyOf ns1:hasStructuralProperty .

ns1:hasStructuralPropertyInverse a owl:ObjectProperty ;
    rdfs:label "is structural property of" ;
    rdfs:comment "Relates a structural property to the network component it characterizes." ;
    owl:inverseOf ns1:hasStructuralProperty .

ns1:hasWKT a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "has WKT" ;
    rdfs:comment "Each geospatial location has exactly one Well-Known Text representation.",
        "The Well-Known Text representation of a geometry." ;
    rdfs:domain ns1:GeospatialLocation ;
    rdfs:range xsd:string .

ns1:hasWaterDepth a owl:DatatypeProperty ;
    rdfs:label "has water depth" ;
    rdfs:comment "The depth of water in a network component." ;
    rdfs:domain ns1:HydraulicCondition ;
    rdfs:range xsd:decimal .

ns1:interactsWith a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "interacts with" ;
    rdfs:comment "Indicates mutual influence between operational events." ;
