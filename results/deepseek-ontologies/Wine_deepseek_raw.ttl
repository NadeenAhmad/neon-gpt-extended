# --- Turtle block from step_12_inverse_properties ---
@prefix : <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wine: <http://www.wine-ontology.org/wine#> .

# Additional Object Properties with inverses
wine:hasProductionMethod rdf:type owl:ObjectProperty ;
    rdfs:label "has production method" ;
    rdfs:comment "Relates a wine to the specific production techniques used in its creation." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:ProductionMethod .

wine:methodUsedFor rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasProductionMethod ;
    rdfs:label "method used for" ;
    rdfs:comment "Relates a production method to the wines that utilize it." .

wine:hasAgingProcess rdf:type owl:ObjectProperty ;
    rdfs:label "has aging process" ;
    rdfs:comment "Relates a wine to the specific aging techniques applied during maturation." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:AgingProcess .

wine:agingProcessOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasAgingProcess ;
    rdfs:label "aging process of" ;
    rdfs:comment "Relates an aging process to the wines that undergo it." .

wine:hasClarificationMethod rdf:type owl:ObjectProperty ;
    rdfs:label "has clarification method" ;
    rdfs:comment "Relates a wine to the techniques used for clarification and stabilization." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:ClarificationMethod .

wine:clarificationMethodFor rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasClarificationMethod ;
    rdfs:label "clarification method for" ;
    rdfs:comment "Relates a clarification method to the wines it is applied to." .

wine:hasBottlingProcess rdf:type owl:ObjectProperty ;
    rdfs:label "has bottling process" ;
    rdfs:comment "Relates a wine to the specific bottling techniques and conditions." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:BottlingProcess .

wine:bottlingProcessOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasBottlingProcess ;
    rdfs:label "bottling process of" ;
    rdfs:comment "Relates a bottling process to the wines that undergo it." .

wine:hasLabelingInformation rdf:type owl:ObjectProperty ;
    rdfs:label "has labeling information" ;
    rdfs:comment "Relates a wine to its regulatory and descriptive labeling details." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:LabelingInformation .

wine:labelingInformationFor rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasLabelingInformation ;
    rdfs:label "labeling information for" ;
    rdfs:comment "Relates labeling information to the wine it describes." .

wine:hasQualityRating rdf:type owl:ObjectProperty ;
    rdfs:label "has quality rating" ;
    rdfs:comment "Relates a wine to its official quality assessment and rating." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:QualityRating .

wine:qualityRatingOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasQualityRating ;
    rdfs:label "quality rating of" ;
    rdfs:comment "Relates a quality rating to the wine it assesses." .

wine:hasSensoryProfile rdf:type owl:ObjectProperty ;
    rdfs:label "has sensory profile" ;
    rdfs:comment "Relates a wine to its comprehensive sensory characteristics and descriptors." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:SensoryProfile .

wine:sensoryProfileOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasSensoryProfile ;
    rdfs:label "sensory profile of" ;
    rdfs:comment "Relates a sensory profile to the wine it characterizes." .

wine:hasChemicalComposition rdf:type owl:ObjectProperty ;
    rdfs:label "has chemical composition" ;
    rdfs:comment "Relates a wine to its detailed chemical analysis and components." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:ChemicalComposition .

wine:chemicalCompositionOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasChemicalComposition ;
    rdfs:label "chemical composition of" ;
    rdfs:comment "Relates a chemical composition to the wine it analyzes." .

wine:hasViticulturalPractice rdf:type owl:ObjectProperty ;
    rdfs:label "has viticultural practice" ;
    rdfs:comment "Relates a grape variety to the cultivation techniques used in its production." ;
    rdfs:domain wine:GrapeVariety ;
    rdfs:range wine:ViticulturalPractice .

wine:practiceAppliedTo rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasViticulturalPractice ;
    rdfs:label "practice applied to" ;
    rdfs:comment "Relates a viticultural practice to the grape varieties it is used for." .

wine:hasSoilType rdf:type owl:ObjectProperty ;
    rdfs:label "has soil type" ;
    rdfs:comment "Relates a region to the predominant soil types found within it." ;
    rdfs:domain wine:Region ;
    rdfs:range wine:SoilType .

wine:soilTypeIn rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasSoilType ;
    rdfs:label "soil type in" ;
    rdfs:comment "Relates a soil type to the regions where it is found." .

wine:hasClimate rdf:type owl:ObjectProperty ;
    rdfs:label "has climate" ;
    rdfs:comment "Relates a region to its characteristic climate conditions." ;
    rdfs:domain wine:Region ;
    rdfs:range wine:Climate .

wine:climateOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasClimate ;
    rdfs:label "climate of" ;
    rdfs:comment "Relates a climate to the regions that experience it." .

wine:hasAppellation rdf:type owl:ObjectProperty ;
    rdfs:label "has appellation" ;
    rdfs:comment "Relates a wine to its official geographical designation and classification." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Appellation .

wine:appellationFor rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasAppellation ;
    rdfs:label "appellation for" ;
    rdfs:comment "Relates an appellation to the wines that carry it." .

wine:hasVintageCharacteristic rdf:type owl:ObjectProperty ;
    rdfs:label "has vintage characteristic" ;
    rdfs:comment "Relates a vintage to the specific climatic and growing conditions of that year." ;
    rdfs:domain wine:Vintage ;
    rdfs:range wine:VintageCharacteristic .

wine:characteristicOfVintage rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasVintageCharacteristic ;
    rdfs:label "characteristic of vintage" ;
    rdfs:comment "Relates vintage characteristics to the vintages they describe." .

wine:hasWinemakingTechnique rdf:type owl:ObjectProperty ;
    rdfs:label "has winemaking technique" ;
    rdfs:comment "Relates a winemaker to the specific techniques they employ in production." ;
    rdfs:domain wine:Winemaker ;
    rdfs:range wine:WinemakingTechnique .

wine:techniqueUsedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasWinemakingTechnique ;
    rdfs:label "technique used by" ;
    rdfs:comment "Relates a winemaking technique to the winemakers who use it." .

# Additional Classes for new properties
wine:ProductionMethod rdf:type owl:Class ;
