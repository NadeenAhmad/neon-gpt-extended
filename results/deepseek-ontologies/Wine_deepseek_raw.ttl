wine:LabelingInformation rdf:type owl:Class ;
    rdfs:label "Labeling Information" ;
    rdfs:comment "Regulatory and descriptive information found on wine labels." .

wine:QualityRating rdf:type owl:Class ;
    rdfs:label "Quality Rating" ;
    rdfs:comment "An official assessment or rating of wine quality." .

wine:SensoryProfile rdf:type owl:Class ;
    rdfs:label "Sensory Profile" ;
    rdfs:comment "The comprehensive sensory characteristics of a wine." .

wine:ChemicalComposition rdf:type owl:Class ;
    rdfs:label "Chemical Composition" ;
    rdfs:comment "The chemical components and analysis of a wine." .

wine:ViticulturalPractice rdf:type owl:Class ;
    rdfs:label "Viticultural Practice" ;
    rdfs:comment "Cultivation techniques used in grape growing." .

wine:SoilType rdf:type owl:Class ;
    rdfs:label "Soil Type" ;
    rdfs:comment "The geological composition of soil in a vineyard region." .

wine:Climate rdf:type owl:Class ;
    rdfs:label "Climate" ;
    rdfs:comment "The characteristic weather patterns of a geographical region." .

wine:Appellation rdf:type owl:Class ;
    rdfs:label "Appellation" ;
    rdfs:comment "An official geographical designation for wine production." .

wine:VintageCharacteristic rdf:type owl:Class ;
    rdfs:label "Vintage Characteristic" ;
    rdfs:comment "The specific growing conditions and climate of a vintage year." .

wine:WinemakingTechnique rdf:type owl:Class ;
    rdfs:label "Winemaking Technique" ;
    rdfs:comment "Specific methods and approaches used in wine production." .

# --- Turtle block from step_13_reflexive_properties ---
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Reflexive Properties
wine:isSimilarTo rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "is similar to" ;
    rdfs:comment "Relates a wine to another wine that shares similar characteristics, allowing for self-reference when a wine is considered similar to itself." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Wine .

wine:compatibleWith rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "compatible with" ;
    rdfs:comment "Relates a wine to another wine that can be paired or served together, including self-compatibility." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Wine .

wine:sharesTerroirWith rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "shares terroir with" ;
    rdfs:comment "Relates a region to another region that shares similar terroir characteristics, including self-reference." ;
    rdfs:domain wine:Region ;
    rdfs:range wine:Region .

wine:hasSimilarStyle rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "has similar style" ;
    rdfs:comment "Relates a wine style to another style that is similar, allowing for self-similarity." ;
    rdfs:domain wine:WineStyle ;
    rdfs:range wine:WineStyle .

wine:producedBySameWinemaker rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "produced by same winemaker" ;
    rdfs:comment "Relates a winemaker to themselves or other winemakers under the same entity, indicating self-production relationships." ;
    rdfs:domain wine:Winemaker ;
    rdfs:range wine:Winemaker .

# --- Turtle block from step_14_symmetric_properties ---
# Symmetric Properties
wine:pairedWith rdf:type owl:SymmetricProperty ;
    rdfs:label "paired with" ;
    rdfs:comment "Symmetric relationship indicating mutual pairing compatibility between wines and foods." ;
    rdfs:domain [ owl:unionOf ( wine:Wine wine:FoodPairing ) ] ;
    rdfs:range [ owl:unionOf ( wine:Wine wine:FoodPairing ) ] .

wine:compatibleWith rdf:type owl:SymmetricProperty ;
    rdfs:label "compatible with" ;
    rdfs:comment "Symmetric relationship indicating mutual compatibility between wine styles." ;
    rdfs:domain wine:WineStyle ;
    rdfs:range wine:WineStyle .

wine:blendedWith rdf:type owl:SymmetricProperty ;
    rdfs:label "blended with" ;
    rdfs:comment "Symmetric relationship indicating mutual blending relationship between grape varieties." ;
    rdfs:domain wine:GrapeVariety ;
    rdfs:range wine:GrapeVariety .

wine:adjacentTo rdf:type owl:SymmetricProperty ;
    rdfs:label "adjacent to" ;
    rdfs:comment "Symmetric relationship indicating geographical adjacency between wine regions." ;
    rdfs:domain wine:Region ;
    rdfs:range wine:Region .

wine:collaboratesWith rdf:type owl:SymmetricProperty ;
    rdfs:label "collaborates with" ;
    rdfs:comment "Symmetric relationship indicating mutual collaboration between winemakers." ;
    rdfs:domain wine:Winemaker ;
    rdfs:range wine:Winemaker .

# --- Turtle block from step_15_functional_properties ---
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Functional Object Properties
wine:hasWineType rdf:type owl:FunctionalProperty ;
    rdfs:comment "A wine can only have one primary classification type (e.g., it cannot be both red and white simultaneously)." .

wine:hasVintage rdf:type owl:FunctionalProperty ;
    rdfs:comment "A wine is produced from grapes harvested in a single specific year." .

wine:hasAlcoholContent rdf:type owl:FunctionalProperty ;
    rdfs:comment "A wine has exactly one alcohol content measurement by volume." .

wine:hasWinemaker rdf:type owl:FunctionalProperty ;
    rdfs:comment "A wine is typically produced by a single winemaker or producer entity." .

wine:producedInRegion rdf:type owl:FunctionalProperty ;
    rdfs:comment "A wine originates from one specific geographical region of production." .

# --- Turtle block from step_16_transitive_properties ---
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Transitive Properties
wine:hasSubRegion rdf:type owl:TransitiveProperty, owl:ObjectProperty ;
    rdfs:subPropertyOf wine:producedInRegion ;
    rdfs:label "has sub-region" ;
    rdfs:comment "Transitive property relating regions to their sub-regions, enabling hierarchical geographical reasoning." ;
    rdfs:domain wine:Region ;
    rdfs:range wine:Region .

wine:madeFromBlend rdf:type owl:TransitiveProperty, owl:ObjectProperty ;
    rdfs:subPropertyOf wine:madeFromGrape ;
    rdfs:label "made from blend" ;
    rdfs:comment "Transitive property relating wines to grape varieties through blending relationships." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:GrapeVariety .

wine:influencedByVintage rdf:type owl:TransitiveProperty, owl:ObjectProperty ;
    rdfs:subPropertyOf wine:hasVintage ;
    rdfs:label "influenced by vintage" ;
    rdfs:comment "Transitive property capturing how vintages influence wine characteristics across production chains." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Vintage .

wine:hasComponentProcess rdf:type owl:TransitiveProperty, owl:ObjectProperty ;
    rdfs:subPropertyOf wine:underwentFermentation ;
    rdfs:label "has component process" ;
    rdfs:comment "Transitive property relating winemaking processes to their sub-processes." ;
    rdfs:domain wine:Fermentation ;
    rdfs:range wine:Fermentation .

wine:producedBySubsidiary rdf:type owl:TransitiveProperty, owl:ObjectProperty ;
    rdfs:subPropertyOf wine:hasWinemaker ;
    rdfs:label "produced by subsidiary" ;
    rdfs:comment "Transitive property relating winemakers through corporate ownership hierarchies." ;
    rdfs:domain wine:Winemaker ;
    rdfs:range wine:Winemaker .

# --- Turtle block from step_17_individuals ---
@prefix : <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wine: <http://www.wine-ontology.org/wine#> .

# Additional Grape Varieties
wine:Merlot rdf:type wine:GrapeVariety ;
    rdfs:comment "A soft, fruity red grape variety known for its plum flavors and smooth tannins, widely used in Bordeaux blends." .

wine:Chardonnay rdf:type wine:GrapeVariety ;
    rdfs:comment "A versatile white grape variety producing wines ranging from crisp and mineral to rich and buttery, depending on winemaking techniques." .

wine:PinotNoir rdf:type wine:GrapeVariety ;
    rdfs:comment "A delicate red grape variety known for its light color, red fruit aromas, and sensitivity to terroir, prominent in Burgundy." .

wine:SauvignonBlanc rdf:type wine:GrapeVariety ;
    rdfs:comment "A crisp white grape variety characterized by herbaceous and citrus notes, thriving in cool climates like Loire Valley and Marlborough." .

wine:Riesling rdf:type wine:GrapeVariety ;
    rdfs:comment "An aromatic white grape variety producing wines with high acidity, ranging from dry to sweet, famous in Germany and Alsace." .

# Additional Regions
wine:Burgundy rdf:type wine:Region ;
    rdfs:comment "A historic French wine region renowned for Pinot Noir and Chardonnay, with complex terroir-driven classifications." .

wine:Tuscany rdf:type wine:Region ;
    rdfs:comment "An Italian wine region famous for Sangiovese-based wines like Chianti and Brunello di Montalcino, characterized by rolling hills and Mediterranean climate." .

wine:NapaValley rdf:type wine:Region ;
    rdfs:comment "A premier California wine region known for Cabernet Sauvignon, with diverse microclimates and volcanic soils." .

wine:Marlborough rdf:type wine:Region ;
    rdfs:comment "A New Zealand wine region celebrated for Sauvignon Blanc, featuring intense tropical and grassy flavors from its cool maritime climate." .

wine:Rioja rdf:type wine:Region ;
    rdfs:comment "A Spanish wine region specializing in Tempranillo-based wines, known for traditional oak aging classifications." .

# Additional Winemakers
wine:DomaineLeflaive rdf:type wine:Winemaker ;
    rdfs:comment "A prestigious Burgundy producer specializing in biodynamic Chardonnay from Puligny-Montrachet." .

wine:Antinori rdf:type wine:Winemaker ;
    rdfs:comment "A historic Tuscan wine family known for innovative Super Tuscan blends like Tignanello." .

wine:OpusOne rdf:type wine:Winemaker ;
    rdfs:comment "A Napa Valley winery partnership between Robert Mondavi and Baron Philippe de Rothschild, producing Bordeaux-style blends." .

wine:CloudyBay rdf:type wine:Winemaker ;
    rdfs:comment "A Marlborough winery famous for pioneering New Zealand Sauvignon Blanc with vibrant acidity and intense aromatics." .

wine:MarquesdeRiscal rdf:type wine:Winemaker ;
    rdfs:comment "A historic Rioja producer known for traditional Tempranillo blends with extended oak aging." .

# Additional Vintages
wine:Year2015 rdf:type wine:Vintage ;
    wine:hasYear "2015"^^xsd:gYear ;
    rdfs:comment "An exceptional vintage across many European regions, characterized by warm, dry conditions producing ripe, structured wines." .

wine:Year2020 rdf:type wine:Vintage ;
    wine:hasYear "2020"^^xsd:gYear ;
    rdfs:comment "A challenging but high-quality vintage with early harvests due to warm temperatures, producing fresh, balanced wines." .

wine:Year2016 rdf:type wine:Vintage ;
    wine:hasYear "2016"^^xsd:gYear ;
    rdfs:comment "A classic Bordeaux vintage with ideal growing conditions, yielding wines with excellent aging potential." .

wine:Year2019 rdf:type wine:Vintage ;
    wine:hasYear "2019"^^xsd:gYear ;
    rdfs:comment "A outstanding Burgundy vintage producing concentrated Pinot Noir and Chardonnay with perfect balance." .

wine:Year2017 rdf:type wine:Vintage ;
    wine:hasYear "2017"^^xsd:gYear ;
    rdfs:comment "A variable vintage affected by frost in some regions, but producing elegant wines in unaffected areas." .

# Additional Wine Styles
wine:ElegantStyle rdf:type wine:WineStyle ;
    rdfs:comment "A wine style characterized by subtle flavors, refined structure, and balanced acidity, typical of cool-climate wines." .

wine:FruityStyle rdf:type wine:WineStyle ;
    rdfs:comment "A wine style emphasizing fresh fruit flavors like berry and citrus, often found in young, unoaked wines." .

wine:MineralStyle rdf:type wine:WineStyle ;
    rdfs:comment "A wine style showing stony, flinty characteristics derived from specific soil types, common in Chablis and Mosel Rieslings." .

wine:OakAgedStyle rdf:type wine:WineStyle ;
    rdfs:comment "A wine style featuring vanilla, spice, and toast notes from barrel aging, typical of many premium red and white wines." .

wine:CrispStyle rdf:type wine:WineStyle ;
    rdfs:comment "A wine style marked by bright acidity and clean finish, ideal for refreshing white wines and sparkling wines." .

# Additional Food Pairings
wine:SeafoodPairing rdf:type wine:FoodPairing ;
    rdfs:comment "Pairs well with various seafood dishes including grilled fish, shellfish, and sushi, complementing with crisp acidity." .

wine:PastaPairing rdf:type wine:FoodPairing ;
    rdfs:comment "Pairs well with pasta dishes featuring tomato-based or cream sauces, matching the wine's acidity and body." .

wine:CheesePairing rdf:type wine:FoodPairing ;
    rdfs:comment "Pairs well with assorted cheeses, from soft brie to aged hard cheeses, creating complementary flavor interactions." .

wine:PoultryPairing rdf:type wine:FoodPairing ;
    rdfs:comment "Pairs well with chicken, turkey, and other poultry dishes, balancing the wine's tannins and fruit notes." .

wine:ChocolatePairing rdf:type wine:FoodPairing ;
    rdfs:comment "Pairs well with dark chocolate desserts, requiring wines with sufficient sweetness and intensity to match the richness." .

# Additional Alcohol Content
wine:Alc12Percent rdf:type wine:AlcoholContent ;
    wine:hasPercentage "12.0"^^xsd:decimal ;
    rdfs:comment "Lower alcohol content typical of cool-climate white wines and lighter red wines." .

wine:Alc14Percent rdf:type wine:AlcoholContent ;
    wine:hasPercentage "14.0"^^xsd:decimal ;
    rdfs:comment "Higher alcohol content common in warm-climate wines and full-bodied red varieties." .

wine:Alc11Percent rdf:type wine:AlcoholContent ;
    wine:hasPercentage "11.5"^^xsd:decimal ;
    rdfs:comment "Moderate alcohol content found in German Rieslings and other light-bodied wines." .

wine:Alc15Percent rdf:type wine:AlcoholContent ;
    wine:hasPercentage "15.0"^^xsd:decimal ;
    rdfs:comment "High alcohol content typical of fortified wines like Port and some Zinfandels." .

wine:Alc13Percent rdf:type wine:AlcoholContent ;
    wine:hasPercentage "13.0"^^xsd:decimal ;
    rdfs:comment "Medium alcohol content common in many traditional European red and white wines." .

# Additional Fermentation Processes
wine:MalolacticFermentation rdf:type wine:Fermentation ;
    rdfs:comment "A secondary fermentation converting malic acid to lactic acid, softening acidity and adding buttery notes to wines." .

wine:CarbonicMaceration rdf:type wine:Fermentation ;
    rdfs:comment "A fermentation technique where whole grapes ferment in carbon dioxide, producing fruity, low-tannin wines like Beaujolais Nouveau." .

wine:ChampagneMethod rdf:type wine:Fermentation ;
    rdfs:comment "A traditional method for sparkling wine production involving secondary fermentation in the bottle, creating fine bubbles and complex flavors." .

wine:BarrelFermentation rdf:type wine:Fermentation ;
    rdfs:comment "Fermentation occurring in oak barrels, integrating oak flavors and adding texture to the wine." .

wine:StainlessSteelFermentation rdf:type wine:Fermentation ;
    rdfs:comment "Fermentation in temperature-controlled stainless steel tanks, preserving fresh fruit character and crisp acidity." .

# Additional Wine Individuals
wine:ChateauMargauxGrandVin rdf:type wine:RedWine ;
    wine:madeFromGrape wine:CabernetSauvignon ;
    wine:hasWineType wine:RedWine ;
    wine:producedInRegion wine:Bordeaux ;
    wine:hasWinemaker wine:ChateauMargaux ;
    wine:hasVintage wine:Year2015 ;
    wine:hasWineStyle wine:ElegantStyle ;
    wine:hasFoodPairing wine:RedMeatPairing ;
    wine:hasAlcoholContent wine:Alc13Percent ;
    wine:underwentFermentation wine:BarrelFermentation ;
    rdfs:comment "First growth Bordeaux wine known for its complexity, aging potential, and refined tannin structure." .

