# Base URI declaration
<http://www.wine-ontology.org/wine> rdf:type owl:Ontology ;
    rdfs:label "Wine Ontology" ;
    rdfs:comment "An ontology for representing knowledge about wines, grape varieties, winemaking processes, and related concepts." ;
    owl:imports <http://purl.obolibrary.org/obo/foodon.owl> .

# Classes
wine:Wine rdf:type owl:Class ;
    rdfs:label "Wine" ;
    rdfs:comment "A beverage made from fermented grapes or other fruits." ;
    rdfs:subClassOf foodon:FOODON_00002452 .  # Alcoholic beverage

wine:GrapeVariety rdf:type owl:Class ;
    rdfs:label "Grape Variety" ;
    rdfs:comment "A specific cultivar of grape used in winemaking." ;
    rdfs:subClassOf agrovoc:c_3334 .  # Grape

wine:WineType rdf:type owl:Class ;
    rdfs:label "Wine Type" ;
    rdfs:comment "A classification of wine based on characteristics like color, sweetness, or effervescence." .

wine:Region rdf:type owl:Class ;
    rdfs:label "Region" ;
    rdfs:comment "A geographical area where grapes are grown and wine is produced." .

wine:Winemaker rdf:type owl:Class ;
    rdfs:label "Winemaker" ;
    rdfs:comment "An individual or entity responsible for producing wine." .

wine:Vintage rdf:type owl:Class ;
    rdfs:label "Vintage" ;
    rdfs:comment "The year in which the grapes were harvested to produce a wine." .

wine:WineStyle rdf:type owl:Class ;
    rdfs:label "Wine Style" ;
    rdfs:comment "A category describing the overall character of a wine, such as bold, light, or aromatic." .

wine:FoodPairing rdf:type owl:Class ;
    rdfs:label "Food Pairing" ;
    rdfs:comment "A recommendation for combining wine with specific foods." .

wine:AlcoholContent rdf:type owl:Class ;
    rdfs:label "Alcohol Content" ;
    rdfs:comment "The percentage of alcohol by volume in a wine." .

wine:Fermentation rdf:type owl:Class ;
    rdfs:label "Fermentation" ;
    rdfs:comment "The process by which sugars in grape juice are converted into alcohol and carbon dioxide by yeast." .

wine:Vineyard rdf:type owl:Class ;
    rdfs:label "Vineyard" ;
    rdfs:comment "A plantation of grape-bearing vines, grown mainly for winemaking." ;
    rdfs:subClassOf [ rdf:type owl:Restriction ;
                     owl:onProperty wine:partOf ;
                     owl:someValuesFrom agrovoc:c_8183 ] .  # Viticulture

wine:WineLabel rdf:type owl:Class ;
    rdfs:label "Wine Label" ;
    rdfs:comment "The label on a wine bottle containing information about the wine." .

wine:ChemicalCompound rdf:type owl:Class ;
    rdfs:label "Chemical Compound" ;
    rdfs:comment "A chemical substance present in wine." .

# Subclass relationships
wine:RedWine rdf:type owl:Class ;
    rdfs:subClassOf wine:Wine ;
    rdfs:label "Red Wine" ;
    rdfs:comment "A type of wine made from dark-colored grape varieties." .

wine:WhiteWine rdf:type owl:Class ;
    rdfs:subClassOf wine:Wine ;
    rdfs:label "White Wine" ;
    rdfs:comment "A type of wine made from green or yellow grapes, often with minimal skin contact." .

wine:RoséWine rdf:type owl:Class ;
