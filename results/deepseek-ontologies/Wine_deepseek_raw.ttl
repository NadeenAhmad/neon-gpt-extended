<http://www.wine-ontology.org/wine> owl:versionInfo "2.0" ;
    dcterms:created "2024-01-01"^^xsd:date ;
    dcterms:modified "2024-06-15"^^xsd:date ;
    owl:priorVersion <http://www.wine-ontology.org/wine/1.0> ;
    rdfs:comment "A comprehensive ontology for the wine domain covering viticulture, oenology, and wine appreciation. The ontology models 77 classes including wine types, grape varieties, production regions, and winemaking processes, with 657 logical axioms and 161 individuals representing real-world wine knowledge." .

# --- Turtle block from step_19_comments ---
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wine: <http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine#> .

wine:Wine rdfs:comment "A beverage produced through the fermentation of grapes or other fruits, typically characterized by its alcohol content, flavor profile, and aging potential." .
wine:WineType rdfs:comment "A classification category for wines based on shared characteristics such as color, sweetness, or production method." .
wine:RedWine rdfs:comment "A type of wine made from dark-colored grape varieties, characterized by its red hue and typically higher tannin content." .
wine:WhiteWine rdfs:comment "A type of wine made from green or yellow grapes, often with a lighter body and crisper acidity compared to red wines." .
wine:RoseWine rdfs:comment "A pink-colored wine produced by limited contact with grape skins during fermentation, offering characteristics between red and white wines." .
wine:SparklingWine rdfs:comment "A wine containing significant levels of carbon dioxide, resulting in effervescence, often produced through secondary fermentation." .
wine:FortifiedWine rdfs:comment "A wine to which a distilled spirit, usually brandy, has been added, increasing alcohol content and often sweetness." .
wine:DryWine rdfs:comment "A wine with minimal residual sugar, producing a non-sweet taste profile." .
wine:SweetWine rdfs:comment "A wine with noticeable residual sugar, creating a sweet or dessert-like flavor profile." .
wine:GrapeVariety rdfs:comment "A specific cultivar of grape used in winemaking, distinguished by genetic characteristics and flavor profiles." .
wine:Vineyard rdfs:comment "A plantation of grape-bearing vines, managed for the purpose of producing grapes for winemaking." .
wine:Region rdfs:comment "A geographical area recognized for its distinctive wine production characteristics, often defined by legal appellation systems." .
wine:Vintage rdfs:comment "The year in which the grapes were harvested, significantly influencing wine characteristics due to seasonal climatic conditions." .
wine:Winery rdfs:comment "An establishment where wine is produced, including facilities for fermentation, aging, and bottling." .
wine:WineMaker rdfs:comment "An individual or entity responsible for overseeing the winemaking process from grape selection to bottling." .
wine:WineStyle rdfs:comment "A categorization of wine based on production techniques, sensory characteristics, or regional traditions." .
wine:AlcoholContent rdfs:comment "The percentage of alcohol by volume in a wine, affecting body, flavor intensity, and preservation." .
wine:Acidity rdfs:comment "The level of acidic compounds in wine, contributing to freshness, balance, and aging potential." .
wine:Tannin rdfs:comment "Polyphenolic compounds derived from grape skins, seeds, and oak aging, contributing to bitterness, astringency, and structure." .
wine:Body rdfs:comment "The perceived weight and fullness of wine in the mouth, influenced by alcohol, sugar, and extract content." .
wine:Aroma rdfs:comment "The scent of wine derived from grape varieties, fermentation, and aging processes." .
wine:Flavor rdfs:comment "The combined taste and aroma sensations experienced when drinking wine." .
wine:FoodPairing rdfs:comment "The practice of matching wines with complementary foods to enhance dining experiences." .
wine:ViticulturalPractice rdfs:comment "Agricultural techniques applied in vineyard management to optimize grape quality and yield." .
wine:WinemakingProcess rdfs:comment "The sequence of operations from grape processing to bottling that transforms grapes into wine." .
wine:Harvest rdfs:comment "The process of gathering ripe grapes from the vineyard, timing of which critically affects wine quality." .
wine:Crushing rdfs:comment "The initial step in winemaking where grapes are crushed to release juice and begin fermentation." .
wine:Fermentation rdfs:comment "The biochemical process where yeast converts grape sugars into alcohol and carbon dioxide." .
wine:Aging rdfs:comment "The maturation period where wine develops complexity through chemical interactions in various containers." .
wine:Bottling rdfs:comment "The final process of transferring wine into bottles for storage and distribution." .
wine:Appellation rdfs:comment "A legally defined geographical indication used to identify where the grapes for a wine were grown." .
wine:Terroir rdfs:comment "The complete natural environment in which a particular wine is produced, including factors like soil, climate, and topography." .
wine:Quality rdfs:comment "An assessment of wine excellence based on sensory characteristics, balance, and typicity." .
wine:Label rdfs:comment "The informational marking on a wine bottle containing details about origin, producer, and composition." .
wine:Regulation rdfs:comment "Legal standards and requirements governing wine production, labeling, and marketing." .
wine:Color rdfs:comment "The visual hue of wine, primarily determined by grape variety and skin contact during winemaking." .
wine:Sweetness rdfs:comment "The perceived sugar content in wine, ranging from dry to sweet." .
wine:Effervescence rdfs:comment "The presence and intensity of bubbles in wine, characteristic of sparkling varieties." .
wine:VintageYear rdfs:comment "The specific calendar year associated with a wine's grape harvest." .
wine:Producer rdfs:comment "An entity responsible for growing grapes and/or producing wine." .
wine:WineExpert rdfs:comment "An individual with specialized knowledge in wine tasting, evaluation, and industry practices." .
wine:WineCritic rdfs:comment "A professional who assesses and rates wines, influencing consumer preferences and market values." .
wine:WineCompetition rdfs:comment "An organized event where wines are evaluated and awarded based on quality assessments." .
wine:WineAward rdfs:comment "A recognition given to wines for excellence in specific categories or competitions." .
wine:WineEducation rdfs:comment "Formal or informal training programs focused on wine knowledge, tasting, and production." .
wine:WineTourism rdfs:comment "Travel activities centered around visiting wineries, vineyards, and wine-producing regions." .
wine:WineStorage rdfs:comment "The practice of storing wine under controlled conditions to maintain or improve quality over time." .
wine:WineService rdfs:comment "The methods and etiquette of presenting and serving wine to consumers." .
wine:WineGlass rdfs:comment "A specially shaped vessel designed to enhance the tasting experience of different wine types." .
wine:Decanting rdfs:comment "The process of transferring wine from its bottle to another container to separate sediment and aerate the wine." .
wine:Breathing rdfs:comment "Allowing wine to interact with air before consumption to develop aromas and soften flavors." .
wine:Cork rdfs:comment "A traditional sealing material made from bark, used to close wine bottles and allow controlled aging." .
wine:Screwcap rdfs:comment "An alternative bottle closure made from metal, providing consistent sealing and preventing cork taint." .
wine:WineFault rdfs:comment "An undesirable characteristic in wine resulting from production errors, contamination, or improper storage." .
wine:Oxidation rdfs:comment "A chemical reaction where wine components interact with oxygen, potentially leading to spoilage." .
wine:Reduction rdfs:comment "A wine fault characterized by unpleasant sulfurous aromas due to lack of oxygen during winemaking." .
wine:Brettanomyces rdfs:comment "A yeast contamination that can produce barnyard or metallic flavors in wine." .
wine:CorkTaint rdfs:comment "A wine fault caused by trichloroanisole (TCA) contamination, resulting in musty odors and flavors." .
wine:WineChemistry rdfs:comment "The scientific study of chemical compounds and reactions involved in winemaking and aging." .
wine:WineAnalysis rdfs:comment "Laboratory testing of wine to determine chemical composition and ensure quality standards." .
wine:WineSensoryEvaluation rdfs:comment "The systematic assessment of wine using human senses to evaluate appearance, aroma, flavor, and mouthfeel." .
wine:WineTasting rdfs:comment "The practice of sampling and evaluating wines to understand their characteristics and quality." .
wine:WineJournal rdfs:comment "A recorded collection of tasting notes, observations, and experiences with different wines." .
wine:WineCollection rdfs:comment "An assembled group of wines, often curated for investment, consumption, or educational purposes." .
wine:WineInvestment rdfs:comment "The practice of purchasing wines with the expectation of increasing value over time." .
wine:WineMarket rdfs:comment "The economic system involving production, distribution, and consumption of wine products." .
wine:WinePrice rdfs:comment "The monetary value assigned to a wine based on quality, rarity, demand, and production costs." .
wine:WineDistribution rdfs:comment "The network and processes involved in moving wine from producers to consumers." .
wine:WineRetail rdfs:comment "The business of selling wine directly to consumers through various channels." .
wine:WineWholesale rdfs:comment "The bulk sale of wine to retailers, restaurants, or other distributors." .
wine:WineExport rdfs:comment "The international trade of wine across national borders." .
wine:WineImport rdfs:comment "The process of bringing foreign wines into a country for distribution and sale." .
wine:WineLaw rdfs:comment "Legal regulations governing wine production, labeling, advertising, and trade." .
wine:WineTax rdfs:comment "Government-imposed charges on wine production, import, or sale." .
wine:WineHistory rdfs:comment "The historical development of winemaking practices, regions, and cultural significance." .
wine:WineCulture rdfs:comment "The social traditions, rituals, and knowledge systems surrounding wine consumption and appreciation." .
wine:WineLiterature rdfs:comment "Written works including books, articles, and reviews dedicated to wine knowledge and criticism." .
wine:WineMedia rdfs:comment "Publications, broadcasts, and digital content focused on wine news, reviews, and education." .
wine:WineTechnology rdfs:comment "Innovative tools and methods applied to viticulture and winemaking to improve efficiency and quality." .
wine:OrganicWine rdfs:comment "Wine produced from grapes grown without synthetic pesticides or fertilizers, following organic farming standards." .
wine:BiodynamicWine rdfs:comment "Wine made using biodynamic agricultural principles that treat the vineyard as a self-sustaining ecosystem." .
wine:NaturalWine rdfs:comment "Wine produced with minimal intervention in both vineyard and cellar, avoiding additives and processing aids." .
wine:SustainableWine rdfs:comment "Wine made through practices that environmentally, socially, and economically sustainable." .
wine:VeganWine rdfs:comment "Wine processed without animal-derived fining agents, suitable for vegan diets." .
wine:LowAlcoholWine rdfs:comment "Wine with reduced alcohol content, often through specific viticultural or winemaking techniques." .
wine:NonAlcoholicWine rdfs:comment "Beverages resembling wine but with alcohol removed or reduced to negligible levels." .
wine:OrangeWine rdfs:comment "White wine made with extended skin contact, resulting in an orange hue and tannic structure." .
wine:madeFromGrape rdfs:comment "Indicates the grape variety or varieties used in the production of a particular wine." .
wine:hasVintage rdfs:comment "Links a wine to its specific harvest year, which influences character and quality." .
wine:producedIn rdfs:comment "Associates a wine with its geographical region of production, reflecting terroir influences." .
wine:hasWineStyle rdfs:comment "Connects a wine to its stylistic classification based on production methods or characteristics." .
wine:hasAlcoholContent rdfs:comment "Specifies the alcohol percentage present in a wine." .
wine:hasAcidity rdfs:comment "Describes the level of acidity in a wine, affecting taste and preservation." .
wine:hasTannin rdfs:comment "Indicates the tannin level in a wine, contributing to structure and aging potential." .
wine:hasBody rdfs:comment "Refers to the perceived weight and texture of a wine in the mouth." .
wine:hasAroma rdfs:comment "Describes the scent profile characteristic of a wine." .
wine:hasFlavor rdfs:comment "Captures the combined taste and aroma experience of a wine." .
wine:pairsWith rdfs:comment "Suggests complementary food items that enhance the wine drinking experience." .
wine:producedBy rdfs:comment "Identifies the winery or producer responsible for making the wine." .
wine:hasQuality rdfs:comment "Assesses the overall excellence and typicity of a wine." .

# --- Turtle block from step_20_refinement ---
@prefix agrovoc: <http://aims.fao.org/aos/agrovoc/> .
@prefix mesh: <http://id.nlm.nih.gov/mesh/> .
@prefix eurovoc: <http://eurovoc.europa.eu/> .
@prefix unspsc: <http://www.unspsc.org/> .

# Extended Class Hierarchy
wine:ViticulturePractice rdf:type owl:Class ;
    rdfs:label "Viticulture Practice" ;
    rdfs:comment "Agricultural practices specific to grape cultivation for winemaking." .

wine:Vineyard rdf:type owl:Class ;
    rdfs:subClassOf wine:Region ;
    rdfs:label "Vineyard" ;
    rdfs:comment "A specific plot of land where grapevines are cultivated for wine production." .

wine:WineComponent rdf:type owl:Class ;
    rdfs:label "Wine Component" ;
    rdfs:comment "Chemical compounds and substances present in wine." .

wine:ChemicalCompound rdf:type owl:Class ;
    rdfs:subClassOf wine:WineComponent ;
    rdfs:label "Chemical Compound" ;
    rdfs:comment "Specific chemical entities contributing to wine characteristics." .

wine:Acid rdf:type owl:Class ;
    rdfs:subClassOf wine:ChemicalCompound ;
    rdfs:label "Acid" ;
    rdfs:comment "Acidic compounds influencing wine pH and taste." .

wine:Preservative rdf:type owl:Class ;
    rdfs:subClassOf wine:ChemicalCompound ;
    rdfs:label "Preservative" ;
    rdfs:comment "Substances added to wine for stability and shelf-life extension." .

wine:Additive rdf:type owl:Class ;
    rdfs:subClassOf wine:ChemicalCompound ;
    rdfs:label "Additive" ;
    rdfs:comment "Substances added during winemaking to modify characteristics." .

wine:SensoryAttribute rdf:type owl:Class ;
    rdfs:label "Sensory Attribute" ;
    rdfs:comment "Perceptible qualities of wine experienced through senses." .

wine:ColorAttribute rdf:type owl:Class ;
    rdfs:subClassOf wine:SensoryAttribute ;
    rdfs:label "Color Attribute" ;
    rdfs:comment "Visual characteristics of wine color and hue." .

wine:FlavorAttribute rdf:type owl:Class ;
    rdfs:subClassOf wine:SensoryAttribute ;
    rdfs:label "Flavor Attribute" ;
    rdfs:comment "Taste and flavor profile characteristics." .

wine:AromaAttribute rdf:type owl:Class ;
    rdfs:subClassOf wine:SensoryAttribute ;
    rdfs:label "Aroma Attribute" ;
    rdfs:comment "Olfactory characteristics and bouquet." .

wine:BodyAttribute rdf:type owl:Class ;
    rdfs:subClassOf wine:SensoryAttribute ;
    rdfs:label "Body Attribute" ;
    rdfs:comment "Mouthfeel and texture characteristics." .

wine:SweetnessAttribute rdf:type owl:Class ;
    rdfs:subClassOf wine:SensoryAttribute ;
    rdfs:label "Sweetness Attribute" ;
    rdfs:comment "Perceived sugar content and sweetness level." .

wine:BitternessAttribute rdf:type owl:Class ;
    rdfs:subClassOf wine:SensoryAttribute ;
    rdfs:label "Bitterness Attribute" ;
    rdfs:comment "Bitterness intensity and character." .

wine:ViscosityAttribute rdf:type owl:Class ;
    rdfs:subClassOf wine:SensoryAttribute ;
    rdfs:label "Viscosity Attribute" ;
    rdfs:comment "Fluid thickness and flow characteristics." .

wine:WineLabel rdf:type owl:Class ;
    rdfs:label "Wine Label" ;
    rdfs:comment "Label information including regulatory compliance and marketing." .

wine:RegulatoryDesignation rdf:type owl:Class ;
    rdfs:label "Regulatory Designation" ;
    rdfs:comment "Legal classifications and protected designations for wine." .

# Extended Object Properties
wine:hasIngredient rdf:type owl:ObjectProperty ;
    rdfs:label "has ingredient" ;
    rdfs:comment "Relates a wine to its constituent ingredients beyond grape varieties." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:WineComponent .

wine:classifiedAs rdf:type owl:ObjectProperty ;
    rdfs:label "classified as" ;
    rdfs:comment "Relates a wine to its regulatory or quality classification." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:RegulatoryDesignation .

wine:containsCompound rdf:type owl:ObjectProperty ;
    rdfs:label "contains compound" ;
    rdfs:comment "Relates a wine to specific chemical compounds present in its composition." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:ChemicalCompound .

wine:hasComponent rdf:type owl:ObjectProperty ;
    rdfs:label "has component" ;
    rdfs:comment "Relates a wine to its various chemical and sensory components." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:WineComponent .

wine:soldUnder rdf:type owl:ObjectProperty ;
    rdfs:label "sold under" ;
    rdfs:comment "Relates a wine to its commercial classification or product code." ;
    rdfs:domain wine:Wine ;
    rdfs:range unspsc:ProductCategory .

wine:hasPreservative rdf:type owl:ObjectProperty ;
    rdfs:label "has preservative" ;
    rdfs:comment "Relates a wine to preservative compounds used in its production." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Preservative .

wine:hasAdditive rdf:type owl:ObjectProperty ;
    rdfs:label "has additive" ;
    rdfs:comment "Relates a wine to additives used during winemaking." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Additive .

wine:hasAcidity rdf:type owl:ObjectProperty ;
    rdfs:label "has acidity" ;
    rdfs:comment "Relates a wine to its acidic components." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Acid .

wine:hasColor rdf:type owl:ObjectProperty ;
    rdfs:label "has color" ;
    rdfs:comment "Relates a wine to its color characteristics and compounds." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:ColorAttribute .

wine:hasFlavor rdf:type owl:ObjectProperty ;
    rdfs:label "has flavor" ;
    rdfs:comment "Relates a wine to its flavor profile and compounds." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:FlavorAttribute .

wine:hasAroma rdf:type owl:ObjectProperty ;
    rdfs:label "has aroma" ;
    rdfs:comment "Relates a wine to its aromatic compounds and bouquet." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:AromaAttribute .

wine:hasBody rdf:type owl:ObjectProperty ;
    rdfs:label "has body" ;
    rdfs:comment "Relates a wine to its body and mouthfeel characteristics." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:BodyAttribute .

wine:hasSweetness rdf:type owl:ObjectProperty ;
    rdfs:label "has sweetness" ;
    rdfs:comment "Relates a wine to its sweetness characteristics and compounds." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:SweetnessAttribute .

wine:hasBitterness rdf:type owl:ObjectProperty ;
    rdfs:label "has bitterness" ;
    rdfs:comment "Relates a wine to its bitterness characteristics and compounds." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:BitternessAttribute .

wine:hasViscosity rdf:type owl:ObjectProperty ;
    rdfs:label "has viscosity" ;
    rdfs:comment "Relates a wine to its viscosity characteristics and compounds." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:ViscosityAttribute .

wine:compliesWith rdf:type owl:ObjectProperty ;
    rdfs:label "complies with" ;
    rdfs:comment "Relates a wine label to regulatory standards and directives." ;
    rdfs:domain wine:WineLabel ;
    rdfs:range eurovoc:Legislation .

wine:partOf rdf:type owl:ObjectProperty ;
    rdfs:label "part of" ;
    rdfs:comment "Relates a vineyard to broader viticultural practices and systems." ;
    rdfs:domain wine:Vineyard ;
    rdfs:range wine:ViticulturePractice .

# Inverse Properties
wine:ingredientOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasIngredient ;
    rdfs:label "ingredient of" ;
    rdfs:comment "Relates a wine component to the wines it is used in." .

wine:classificationFor rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:classifiedAs ;
    rdfs:label "classification for" ;
    rdfs:comment "Relates a regulatory designation to the wines it classifies." .

wine:compoundIn rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:containsCompound ;
    rdfs:label "compound in" ;
    rdfs:comment "Relates a chemical compound to the wines containing it." .

wine:componentOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasComponent ;
    rdfs:label "component of" ;
    rdfs:comment "Relates a wine component to the wines it is part of." .

wine:productCategoryFor rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:soldUnder ;
    rdfs:label "product category for" ;
    rdfs:comment "Relates a product category to the wines sold under it." .

wine:preservativeIn rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasPreservative ;
    rdfs:label "preservative in" ;
    rdfs:comment "Relates a preservative to the wines it preserves." .

wine:additiveIn rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasAdditive ;
    rdfs:label "additive in" ;
    rdfs:comment "Relates an additive to the wines it is added to." .

wine:acidityIn rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasAcidity ;
