@prefix : <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix wine: <http://www.wine-ontology.org/wine#> .

# Base URI declaration
<http://www.wine-ontology.org/wine> rdf:type owl:Ontology ;
    rdfs:label "Wine Ontology" ;
    rdfs:comment "An ontology for representing knowledge about wines, grape varieties, winemaking processes, and related concepts." .

# Classes
wine:Wine rdf:type owl:Class ;
    rdfs:label "Wine" ;
    rdfs:comment "A beverage made from fermented grapes or other fruits." .

wine:GrapeVariety rdf:type owl:Class ;
    rdfs:label "Grape Variety" ;
    rdfs:comment "A specific cultivar of grape used in winemaking." .

wine:WineType rdf:type owl:Class ;
    rdfs:label "Wine Type" ;
    rdfs:comment "A classification of wine based on characteristics like color, sweetness, or effervescence." .

wine:Region rdf:type owl:Class ;
    rdfs:label "Region" ;
    rdfs:comment "A geographical area where grapes are grown and wine is produced." .

wine:Winemaker rdf:type owl:Class ;
    rdfs:label "Winemaker" ;
    rdfs:comment "An individual or entity responsible for producing wine." .

wine:Vintage rdf:type owl:Class ;
    rdfs:label "Vintage" ;
    rdfs:comment "The year in which the grapes were harvested to produce a wine." .

wine:WineStyle rdf:type owl:Class ;
    rdfs:label "Wine Style" ;
    rdfs:comment "A category describing the overall character of a wine, such as bold, light, or aromatic." .

wine:FoodPairing rdf:type owl:Class ;
    rdfs:label "Food Pairing" ;
    rdfs:comment "A recommendation for combining wine with specific foods." .

wine:AlcoholContent rdf:type owl:Class ;
    rdfs:label "Alcohol Content" ;
    rdfs:comment "The percentage of alcohol by volume in a wine." .

wine:Fermentation rdf:type owl:Class ;
    rdfs:label "Fermentation" ;
    rdfs:comment "The process by which sugars in grape juice are converted into alcohol and carbon dioxide by yeast." .

# Subclass relationships
wine:RedWine rdf:type owl:Class ;
    rdfs:subClassOf wine:Wine ;
    rdfs:label "Red Wine" ;
    rdfs:comment "A type of wine made from dark-colored grape varieties." .

wine:WhiteWine rdf:type owl:Class ;
    rdfs:subClassOf wine:Wine ;
    rdfs:label "White Wine" ;
    rdfs:comment "A type of wine made from green or yellow grapes, often with minimal skin contact." .

wine:RoséWine rdf:type owl:Class ;
    rdfs:subClassOf wine:Wine ;
    rdfs:label "Rosé Wine" ;
    rdfs:comment "A type of wine that incorporates some color from the grape skins but not enough to qualify as red wine." .

wine:SparklingWine rdf:type owl:Class ;
    rdfs:subClassOf wine:Wine ;
    rdfs:label "Sparkling Wine" ;
    rdfs:comment "A wine with significant levels of carbon dioxide, making it fizzy." .

wine:FortifiedWine rdf:type owl:Class ;
    rdfs:subClassOf wine:Wine ;
    rdfs:label "Fortified Wine" ;
    rdfs:comment "A wine to which a distilled spirit, usually brandy, has been added." .

# Object Properties
wine:madeFromGrape rdf:type owl:ObjectProperty ;
    rdfs:label "made from grape" ;
    rdfs:comment "Relates a wine to the grape variety used in its production." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:GrapeVariety .

wine:hasWineType rdf:type owl:ObjectProperty ;
    rdfs:label "has wine type" ;
    rdfs:comment "Relates a wine to its classification type (e.g., red, white, sparkling)." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:WineType .

wine:producedInRegion rdf:type owl:ObjectProperty ;
    rdfs:label "produced in region" ;
    rdfs:comment "Relates a wine to the geographical region where it was produced." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Region .

wine:hasWinemaker rdf:type owl:ObjectProperty ;
    rdfs:label "has winemaker" ;
    rdfs:comment "Relates a wine to the winemaker responsible for its production." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Winemaker .

wine:hasVintage rdf:type owl:ObjectProperty ;
    rdfs:label "has vintage" ;
    rdfs:comment "Relates a wine to the specific vintage year." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Vintage .

wine:hasWineStyle rdf:type owl:ObjectProperty ;
    rdfs:label "has wine style" ;
    rdfs:comment "Relates a wine to its overall style (e.g., bold, light)." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:WineStyle .

wine:hasFoodPairing rdf:type owl:ObjectProperty ;
    rdfs:label "has food pairing" ;
    rdfs:comment "Relates a wine to recommended food pairings." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:FoodPairing .

wine:hasAlcoholContent rdf:type owl:ObjectProperty ;
    rdfs:label "has alcohol content" ;
    rdfs:comment "Relates a wine to its alcohol content measurement." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:AlcoholContent .

wine:underwentFermentation rdf:type owl:ObjectProperty ;
    rdfs:label "underwent fermentation" ;
    rdfs:comment "Relates a wine to the fermentation process it underwent." ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Fermentation .

# Data Properties
wine:hasName rdf:type owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:comment "The name of a wine, grape variety, region, or other entity." ;
    rdfs:domain [ owl:unionOf ( wine:Wine wine:GrapeVariety wine:Region wine:Winemaker wine:Vintage wine:WineStyle wine:FoodPairing wine:AlcoholContent wine:Fermentation ) ] ;
    rdfs:range xsd:string .

wine:hasDescription rdf:type owl:DatatypeProperty ;
    rdfs:label "has description" ;
    rdfs:comment "A textual description of a wine or related entity." ;
    rdfs:domain [ owl:unionOf ( wine:Wine wine:GrapeVariety wine:Region wine:Winemaker wine:Vintage wine:WineStyle wine:FoodPairing wine:AlcoholContent wine:Fermentation ) ] ;
    rdfs:range xsd:string .

wine:hasYear rdf:type owl:DatatypeProperty ;
    rdfs:label "has year" ;
    rdfs:comment "The year associated with a vintage." ;
    rdfs:domain wine:Vintage ;
    rdfs:range xsd:gYear .

wine:hasPercentage rdf:type owl:DatatypeProperty ;
    rdfs:label "has percentage" ;
    rdfs:comment "The percentage value for alcohol content." ;
    rdfs:domain wine:AlcoholContent ;
    rdfs:range xsd:decimal .

# Inverse Properties
wine:usedInWine rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:madeFromGrape ;
    rdfs:label "used in wine" ;
    rdfs:comment "Relates a grape variety to the wines it is used in." .

wine:typeOfWine rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasWineType ;
    rdfs:label "type of wine" ;
    rdfs:comment "Relates a wine type to the wines that belong to it." .

wine:regionProduces rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:producedInRegion ;
    rdfs:label "region produces" ;
    rdfs:comment "Relates a region to the wines produced there." .

wine:winemakerProduces rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasWinemaker ;
    rdfs:label "winemaker produces" ;
    rdfs:comment "Relates a winemaker to the wines they produce." .

wine:vintageOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasVintage ;
    rdfs:label "vintage of" ;
    rdfs:comment "Relates a vintage year to the wines from that year." .

wine:styleOfWine rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasWineStyle ;
    rdfs:label "style of wine" ;
    rdfs:comment "Relates a wine style to the wines that exhibit it." .

wine:pairingForWine rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasFoodPairing ;
    rdfs:label "pairing for wine" ;
    rdfs:comment "Relates a food pairing to the wines it is recommended for." .

wine:contentOfWine rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:hasAlcoholContent ;
    rdfs:label "content of wine" ;
    rdfs:comment "Relates an alcohol content measurement to the wine it belongs to." .

wine:fermentationOf rdf:type owl:ObjectProperty ;
    owl:inverseOf wine:underwentFermentation ;
    rdfs:label "fermentation of" ;
    rdfs:comment "Relates a fermentation process to the wine that underwent it." .

# Example Individuals
wine:CabernetSauvignon rdf:type wine:GrapeVariety ;
    wine:hasName "Cabernet Sauvignon" ;
    wine:hasDescription "A popular red wine grape variety known for its thick skin and robust flavor." .

wine:Bordeaux rdf:type wine:Region ;
    wine:hasName "Bordeaux" ;
    wine:hasDescription "A famous wine region in France known for its red blends." .

wine:ChateauMargaux rdf:type wine:Winemaker ;
    wine:hasName "Chateau Margaux" ;
    wine:hasDescription "A prestigious winery in the Bordeaux region." .

wine:Year2018 rdf:type wine:Vintage ;
    wine:hasYear "2018"^^xsd:gYear ;
    wine:hasDescription "A good vintage year with favorable weather conditions." .

wine:BoldStyle rdf:type wine:WineStyle ;
    wine:hasName "Bold" ;
    wine:hasDescription "A wine style characterized by intense flavors and high tannins." .

wine:RedMeatPairing rdf:type wine:FoodPairing ;
    wine:hasName "Red Meat" ;
    wine:hasDescription "Pairs well with red meats like steak and lamb." .

wine:Alc13Percent rdf:type wine:AlcoholContent ;
    wine:hasPercentage "13.5"^^xsd:decimal ;
    wine:hasDescription "Moderate alcohol content typical for many red wines." .

wine:PrimaryFermentation rdf:type wine:Fermentation ;
    wine:hasName "Primary Fermentation" ;
    wine:hasDescription "The initial fermentation where sugars are converted to alcohol." .

wine:ExampleRedWine rdf:type wine:RedWine ;
    wine:hasName "Example Red Wine" ;
    wine:hasDescription "A sample red wine for demonstration purposes." ;
    wine:madeFromGrape wine:CabernetSauvignon ;
    wine:hasWineType wine:RedWine ;
    wine:producedInRegion wine:Bordeaux ;
    wine:hasWinemaker wine:ChateauMargaux ;
    wine:hasVintage wine:Year2018 ;
    wine:hasWineStyle wine:BoldStyle ;
    wine:hasFoodPairing wine:RedMeatPairing ;
    wine:hasAlcoholContent wine:Alc13Percent ;
    wine:underwentFermentation wine:PrimaryFermentation .

