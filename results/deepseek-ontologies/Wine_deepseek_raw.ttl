# New Classes
wine:WineType rdf:type :Class ;
    rdfs:subClassOf wine:WineClassification .

wine:WineColor rdf:type :Class ;
    rdfs:subClassOf wine:WineSensoryProperty .

wine:WineRegion rdf:type :Class ;
    rdfs:subClassOf wine:GeographicalIndication .

wine:Winery rdf:type :Class ;
    rdfs:subClassOf wine:WineProducer .

wine:Vintage rdf:type :Class ;
    rdfs:subClassOf wine:HarvestYear .

wine:TastingNote rdf:type :Class ;
    rdfs:subClassOf wine:WineEvaluation .

wine:FoodPairing rdf:type :Class ;
    rdfs:subClassOf foodon:00003302 .  # FOODON:food_pairing

wine:WineClassification rdf:type :Class ;
    rdfs:subClassOf wine:WineRegulatoryStandard .

wine:WineSensoryProperty rdf:type :Class ;
    rdfs:subClassOf wine:WineCharacteristic .

wine:GeographicalIndication rdf:type :Class ;
    rdfs:subClassOf wine:WineRegulatoryStandard .

wine:WineProducer rdf:type :Class ;
    rdfs:subClassOf wine:WineBusinessEntity .

wine:HarvestYear rdf:type :Class ;
    rdfs:subClassOf wine:ViticulturalEvent .

wine:WineEvaluation rdf:type :Class ;
    rdfs:subClassOf wine:WineQualityAssessment .

# New Object Properties
wine:hasWineType rdf:type :ObjectProperty ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:WineType ;
    :inverseOf wine:wineTypeOf .

wine:wineTypeOf rdf:type :ObjectProperty ;
    rdfs:domain wine:WineType ;
    rdfs:range wine:Wine .

wine:hasColor rdf:type :ObjectProperty ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:WineColor ;
    :inverseOf wine:colorOf .

wine:colorOf rdf:type :ObjectProperty ;
    rdfs:domain wine:WineColor ;
    rdfs:range wine:Wine .

wine:producedIn rdf:type :ObjectProperty ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:WineRegion ;
    :inverseOf wine:regionProduces .

wine:regionProduces rdf:type :ObjectProperty ;
    rdfs:domain wine:WineRegion ;
    rdfs:range wine:Wine .

wine:producedBy rdf:type :ObjectProperty ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Winery ;
    :inverseOf wine:producesWine .

wine:producesWine rdf:type :ObjectProperty ;
    rdfs:domain wine:Winery ;
    rdfs:range wine:Wine .

wine:hasVintage rdf:type :ObjectProperty ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:Vintage ;
    :inverseOf wine:vintageOf .

wine:vintageOf rdf:type :ObjectProperty ;
    rdfs:domain wine:Vintage ;
    rdfs:range wine:Wine .

wine:hasTastingNote rdf:type :ObjectProperty ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:TastingNote ;
    :inverseOf wine:tastingNoteOf .

wine:tastingNoteOf rdf:type :ObjectProperty ;
    rdfs:domain wine:TastingNote ;
    rdfs:range wine:Wine .

wine:hasFoodPairing rdf:type :ObjectProperty ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:FoodPairing ;
    :inverseOf wine:foodPairingFor .

wine:foodPairingFor rdf:type :ObjectProperty ;
    rdfs:domain wine:FoodPairing ;
    rdfs:range wine:Wine .

wine:madeFrom rdf:type :ObjectProperty ;
    rdfs:domain wine:Wine ;
    rdfs:range wine:GrapeVariety ;
    :inverseOf wine:usedIn .

wine:usedIn rdf:type :ObjectProperty ;
    rdfs:domain wine:GrapeVariety ;
    rdfs:range wine:Wine .

wine:locatedIn rdf:type :ObjectProperty ;
    rdfs:domain wine:Winery ;
    rdfs:range wine:WineRegion ;
    :inverseOf wine:containsWinery .

wine:containsWinery rdf:type :ObjectProperty ;
    rdfs:domain wine:WineRegion ;
    rdfs:range wine:Winery .

wine:hasHarvestYear rdf:type :ObjectProperty ;
    rdfs:domain wine:Vintage ;
    rdfs:range xsd:gYear .

wine:describesWine rdf:type :ObjectProperty ;
    rdfs:domain wine:TastingNote ;
    rdfs:range wine:Wine .

wine:recommendedWith rdf:type :ObjectProperty ;
    rdfs:domain wine:FoodPairing ;
    rdfs:range foodon:00002453 .  # FOODON:ingredient

# Alignment with external ontologies
wine:WineType rdfs:subClassOf foodon:00003301 .  # FOODON:beverage_type

wine:WineColor rdfs:subClassOf foodon:00003303 .  # FOODON:color_property

wine:WineRegion rdfs:subClassOf eurovoc:GeographicalArea .

wine:Winery rdfs:subClassOf foodon:00003304 .  # FOODON:food_producer

wine:Vintage rdfs:subClassOf agrovoc:HarvestYear .

wine:TastingNote rdfs:subClassOf foodon:00003305 .  # FOODON:sensory_evaluation

wine:FoodPairing rdfs:subClassOf foodon:00003302 .  # FOODON:food_pairing

# Specific color mappings
wine:RedWine rdf:type wine:WineColor ;
    rdfs:subClassOf foodon:00003306 .  # FOODON:red_color

wine:WhiteWine rdf:type wine:WineColor ;
    rdfs:subClassOf foodon:00003307 .  # FOODON:white_color

wine:RoseWine rdf:type wine:WineColor ;
    rdfs:subClassOf foodon:00003308 .  # FOODON:pink_color

# Specific wine type mappings
wine:StillWine rdf:type wine:WineType ;
    rdfs:subClassOf foodon:00003309 .  # FOODON:still_beverage

wine:SparklingWine rdf:type wine:WineType ;
    rdfs:subClassOf foodon:00003310 .  # FOODON:sparkling_beverage

wine:FortifiedWine rdf:type wine:WineType ;
    rdfs:subClassOf foodon:00003311 .  # FOODON:fortified_beverage

# Property chains and additional axioms
wine:madeFrom o :propertyChainAxiom (wine:hasIngredient wine:ingredientOf) .

wine:producedIn o :propertyChainAxiom (wine:producedBy wine:locatedIn) .

wine:hasVintage o :propertyChainAxiom (wine:hasHarvestYear wine:harvestYearOf) .

