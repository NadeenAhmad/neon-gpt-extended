@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix raepa: <http://www.raepa.org/ont/1.2#> .
@prefix dolce: <http://www.loa.istc.cnr.it/ontologies/DOLCE-Lite.owl#> .

# Ontology Declaration
: a owl:Ontology ;
    dct:title "SewerNet Ontology" ;
    dct:description "An ontology for representing wastewater and stormwater network components, properties, events, and geospatial information." ;
    owl:imports <http://www.w3.org/2006/time> ;
    owl:versionInfo "1.0" .

# Classes

## Network Components
:Pipe a owl:Class ;
    rdfs:subClassOf :NetworkComponent ;
    rdfs:label "Pipe" ;
    rdfs:comment "A conduit for transporting wastewater or stormwater." .

:Manhole a owl:Class ;
    rdfs:subClassOf :NetworkComponent ;
    rdfs:label "Manhole" ;
    rdfs:comment "An access point to the sewer network for inspection and maintenance." .

:Inlet a owl:Class ;
    rdfs:subClassOf :NetworkComponent ;
    rdfs:label "Inlet" ;
    rdfs:comment "A point where water enters the sewer network." .

:Outlet a owl:Class ;
    rdfs:subClassOf :NetworkComponent ;
    rdfs:label "Outlet" ;
    rdfs:comment "A point where water exits the sewer network." .

:PumpingStation a owl:Class ;
    rdfs:subClassOf :NetworkComponent ;
    rdfs:label "Pumping Station" ;
    rdfs:comment "A facility that pumps wastewater or stormwater through the network." .

:NetworkComponent a owl:Class ;
    rdfs:subClassOf dolce:physical-object ;
    rdfs:label "Network Component" ;
    rdfs:comment "A physical element of the sewer network." .

## Hydraulic and Structural Properties
:HydraulicProperty a owl:Class ;
    rdfs:subClassOf :Property ;
    rdfs:label "Hydraulic Property" ;
    rdfs:comment "A property related to the flow of water in the network." .

:StructuralProperty a owl:Class ;
    rdfs:subClassOf :Property ;
    rdfs:label "Structural Property" ;
    rdfs:comment "A property related to the physical condition of network components." .

:Property a owl:Class ;
    rdfs:label "Property" ;
    rdfs:comment "A characteristic of a network component." .

## Operational Events
:Inspection a owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:label "Inspection" ;
    rdfs:comment "An event to assess the condition of a network component." .

:Maintenance a owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:label "Maintenance" ;
    rdfs:comment "An event to maintain or repair a network component." .

:Blockage a owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:label "Blockage" ;
    rdfs:comment "An event where a network component is obstructed." .

:Overflow a owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:label "Overflow" ;
    rdfs:comment "An event where water overflows from the network." .

:Repair a owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:label "Repair" ;
    rdfs:comment "An event to fix a damaged network component." .

:OperationalEvent a owl:Class ;
    rdfs:subClassOf dolce:event ;
    rdfs:label "Operational Event" ;
    rdfs:comment "An event related to the management of the sewer network." .

## Geospatial Information
:GeospatialLocation a owl:Class ;
    rdfs:subClassOf geo:Geometry ;
    rdfs:label "Geospatial Location" ;
    rdfs:comment "The geographic location of a network component." .

## Standards Compliance
:RAEPAStandard a owl:Class ;
    rdfs:subClassOf :Standard ;
    rdfs:label "RAEPA Standard" ;
    rdfs:comment "Compliance with the French RAEPA v1.2 geostandard." .

:INSPIREStandard a owl:Class ;
    rdfs:subClassOf :Standard ;
    rdfs:label "INSPIRE Standard" ;
    rdfs:comment "Compliance with the INSPIRE European directive." .

:Standard a owl:Class ;
    rdfs:label "Standard" ;
    rdfs:comment "A set of norms or guidelines for interoperability." .

# Object Properties

:hasPart a owl:ObjectProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :NetworkComponent ;
    rdfs:label "has part" ;
    rdfs:comment "Relates a network component to its constituent parts." .

:isPartOf a owl:ObjectProperty ;
    owl:inverseOf :hasPart ;
    rdfs:label "is part of" ;
    rdfs:comment "Relates a constituent part to the network component it belongs to." .

:hasProperty a owl:ObjectProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :Property ;
    rdfs:label "has property" ;
    rdfs:comment "Relates a network component to its properties." .

:hasHydraulicProperty a owl:ObjectProperty ;
    rdfs:subPropertyOf :hasProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :HydraulicProperty ;
    rdfs:label "has hydraulic property" ;
    rdfs:comment "Relates a network component to its hydraulic properties." .

:hasStructuralProperty a owl:ObjectProperty ;
    rdfs:subPropertyOf :hasProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :StructuralProperty ;
    rdfs:label "has structural property" ;
    rdfs:comment "Relates a network component to its structural properties." .

:hasEvent a owl:ObjectProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :OperationalEvent ;
    rdfs:label "has event" ;
    rdfs:comment "Relates a network component to operational events affecting it." .

:hasLocation a owl:ObjectProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :GeospatialLocation ;
    rdfs:label "has location" ;
    rdfs:comment "Relates a network component to its geographic location." .

:compliesWith a owl:ObjectProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :Standard ;
    rdfs:label "complies with" ;
    rdfs:comment "Indicates compliance of a network component with a standard." .

:hasStartTime a owl:ObjectProperty ;
    rdfs:domain :OperationalEvent ;
    rdfs:range time:Instant ;
    rdfs:label "has start time" ;
    rdfs:comment "Relates an operational event to its start time." .

:hasEndTime a owl:ObjectProperty ;
    rdfs:domain :OperationalEvent ;
    rdfs:range time:Instant ;
    rdfs:label "has end time" ;
    rdfs:comment "Relates an operational event to its end time." .

:connectedTo a owl:ObjectProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :NetworkComponent ;
    rdfs:label "connected to" ;
    rdfs:comment "Relates two network components that are hydraulically connected." .

# Data Properties

:hasDiameter a owl:DatatypeProperty ;
    rdfs:domain :Pipe ;
    rdfs:range xsd:decimal ;
    rdfs:label "has diameter" ;
    rdfs:comment "The diameter of a pipe." .

:hasMaterial a owl:DatatypeProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range xsd:string ;
    rdfs:label "has material" ;
    rdfs:comment "The material composition of a network component." .

:hasSlope a owl:DatatypeProperty ;
    rdfs:domain :Pipe ;
    rdfs:range xsd:decimal ;
    rdfs:label "has slope" ;
    rdfs:comment "The slope of a pipe." .

:hasFlowCapacity a owl:DatatypeProperty ;
    rdfs:domain :Pipe ;
    rdfs:range xsd:decimal ;
    rdfs:label "has flow capacity" ;
    rdfs:comment "The maximum flow capacity of a pipe." .

:hasCondition a owl:DatatypeProperty ;
    rdfs:domain :StructuralProperty ;
    rdfs:range xsd:string ;
    rdfs:label "has condition" ;
    rdfs:comment "The structural condition of a network component." .

:hasWKT a owl:DatatypeProperty ;
    rdfs:domain :GeospatialLocation ;
    rdfs:range xsd:string ;
    rdfs:label "has WKT" ;
    rdfs:comment "The Well-Known Text representation of a geometry." .

:hasID a owl:DatatypeProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range xsd:string ;
    rdfs:label "has ID" ;
    rdfs:comment "The unique identifier of a network component." .

# Axioms and Additional Triples

:Pipe rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasDiameter ;
    owl:someValuesFrom xsd:decimal
] .

:Pipe rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasSlope ;
    owl:someValuesFrom xsd:decimal
] .

:OperationalEvent rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasStartTime ;
    owl:someValuesFrom time:Instant
] .

:OperationalEvent rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasEndTime ;
    owl:someValuesFrom time:Instant
] .

:connectedTo a owl:SymmetricProperty .

:hasPart a owl:TransitiveProperty .

:isPartOf a owl:TransitiveProperty .

