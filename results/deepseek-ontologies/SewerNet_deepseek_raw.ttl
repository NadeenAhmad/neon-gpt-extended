@prefix : <http://www.semanticweb.org/swer-net-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .

: a owl:Ontology ;
    rdfs:label "SewerNet Ontology" ;
    rdfs:comment "A comprehensive semantic framework for representing wastewater and stormwater network infrastructure, including physical components, hydraulic properties, operational events, geospatial data, and compliance with international standards like RAEPA and INSPIRE." ;
    owl:versionInfo "2.0" ;
    vann:preferredNamespaceUri "http://www.semanticweb.org/swer-net-ontology#" ;
    dct:created "2024-01-01"^^xsd:date ;
    dct:modified "2024-06-15"^^xsd:date ;
    dct:license <http://creativecommons.org/licenses/by/4.0/> ;
    dct:publisher "SewerNet Consortium" .

# Additional Network Component Classes
:Junction a owl:Class ;
    rdfs:subClassOf :NetworkComponent ;
    rdfs:label "Junction" ;
    rdfs:comment "A point where multiple pipes connect within the sewer network." .

:Valve a owl:Class ;
    rdfs:subClassOf :NetworkComponent ;
    rdfs:label "Valve" ;
    rdfs:comment "A device that controls the flow of wastewater or stormwater in the network." .

:TreatmentPlant a owl:Class ;
    rdfs:subClassOf :NetworkComponent ;
    rdfs:label "Treatment Plant" ;
    rdfs:comment "A facility where wastewater is treated before discharge or reuse." .

# Additional Hydraulic Properties
:FlowVelocity a owl:Class ;
    rdfs:subClassOf :HydraulicProperty ;
    rdfs:label "Flow Velocity" ;
    rdfs:comment "The speed at which water flows through a network component." .

:RoughnessCoefficient a owl:Class ;
    rdfs:subClassOf :HydraulicProperty ;
    rdfs:label "Roughness Coefficient" ;
    rdfs:comment "A measure of the internal surface roughness affecting flow resistance." .

# Additional Structural Properties
:CrackSeverity a owl:Class ;
    rdfs:subClassOf :StructuralProperty ;
    rdfs:label "Crack Severity" ;
    rdfs:comment "The degree of cracking observed in a network component." .

:CorrosionLevel a owl:Class ;
    rdfs:subClassOf :StructuralProperty ;
    rdfs:label "Corrosion Level" ;
    rdfs:comment "The extent of corrosion damage on a network component." .

# Additional Operational Events
:Cleaning a owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:label "Cleaning" ;
    rdfs:comment "An event involving the removal of debris or sediment from network components." .

:Rehabilitation a owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:label "Rehabilitation" ;
    rdfs:comment "An event to restore or upgrade the functionality of network components." .

# Additional Object Properties
:hasUpstream a owl:ObjectProperty ;
    rdfs:subPropertyOf :connectedTo ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :NetworkComponent ;
    rdfs:label "has upstream" ;
    rdfs:comment "Relates a network component to its immediate upstream component in the flow direction." .

:hasDownstream a owl:ObjectProperty ;
    rdfs:subPropertyOf :connectedTo ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :NetworkComponent ;
    rdfs:label "has downstream" ;
    rdfs:comment "Relates a network component to its immediate downstream component in the flow direction." .

:triggers a owl:ObjectProperty ;
    rdfs:domain :OperationalEvent ;
    rdfs:range :OperationalEvent ;
    rdfs:label "triggers" ;
    rdfs:comment "Relates an operational event that causes another event to occur." .

:locatedIn a owl:ObjectProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :AdministrativeArea ;
    rdfs:label "located in" ;
    rdfs:comment "Relates a network component to the administrative area where it is located." .

# Additional Data Properties
:hasInstallationDate a owl:DatatypeProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range xsd:date ;
    rdfs:label "has installation date" ;
    rdfs:comment "The date when a network component was installed." .

:hasInspectionFrequency a owl:DatatypeProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range xsd:integer ;
    rdfs:label "has inspection frequency" ;
    rdfs:comment "The recommended frequency of inspections for a network component in months." .

:hasRiskScore a owl:DatatypeProperty ;
    rdfs:domain :NetworkComponent ;
    rdfs:range xsd:decimal ;
    rdfs:label "has risk score" ;
    rdfs:comment "A numerical score representing the failure risk of a network component." .

# Additional Classes for Standards and Administration
:AdministrativeArea a owl:Class ;
    rdfs:label "Administrative Area" ;
    rdfs:comment "A geographical area defined for administrative purposes, such as a municipality or district." .

:InspectionReport a owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:label "Inspection Report" ;
    rdfs:comment "A documented record of an inspection event, including findings and recommendations." .

# Property Chains and Additional Axioms
:hasUpstream owl:propertyChainAxiom (:hasPart :hasUpstream) .

:hasDownstream owl:propertyChainAxiom (:hasPart :hasDownstream) .

:Junction rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :connectedTo ;
    owl:minCardinality 3
] .

:TreatmentPlant rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasDownstream ;
    owl:someValuesFrom :Outlet
] .

