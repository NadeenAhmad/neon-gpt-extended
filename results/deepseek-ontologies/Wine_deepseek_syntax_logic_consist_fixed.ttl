ns1:Flavor rdfs:comment "The combined taste and aroma sensations experienced when drinking wine." .

ns1:Harvest rdfs:comment "The process of gathering ripe grapes from the vineyard, timing of which critically affects wine quality." .

ns1:Label rdfs:comment "The informational marking on a wine bottle containing details about origin, producer, and composition." .

ns1:LowAlcoholWine rdfs:comment "Wine with reduced alcohol content, often through specific viticultural or winemaking techniques." .

ns1:MalolacticFermentation a ns1:Fermentation ;
    rdfs:comment "A secondary fermentation converting malic acid to lactic acid, softening acidity and adding buttery notes to wines." .

ns1:MarquesdeRiscal a ns1:Winemaker ;
    rdfs:comment "A historic Rioja producer known for traditional Tempranillo blends with extended oak aging." .

ns1:Merlot a ns1:GrapeVariety ;
    rdfs:comment "A soft, fruity red grape variety known for its plum flavors and smooth tannins, widely used in Bordeaux blends." .

ns1:MontrachetGrandCru a ns1:WhiteWine ;
    rdfs:comment "Premier white Burgundy from the Montrachet vineyard, celebrated for its mineral complexity and aging ability." ;
    ns1:hasAlcoholContent ns1:Alc13Percent ;
    ns1:hasFoodPairing ns1:SeafoodPairing ;
    ns1:hasVintage ns1:Year2019 ;
    ns1:hasWineStyle ns1:MineralStyle ;
    ns1:hasWineType ns1:WhiteWine ;
    ns1:hasWinemaker ns1:DomaineLeflaive ;
    ns1:madeFromGrape ns1:Chardonnay ;
    ns1:producedInRegion ns1:Burgundy ;
    ns1:underwentFermentation ns1:BarrelFermentation .

ns1:NapaValley a ns1:Region ;
    rdfs:comment "A premier California wine region known for Cabernet Sauvignon, with diverse microclimates and volcanic soils." .

ns1:NaturalWine rdfs:comment "Wine produced with minimal intervention in both vineyard and cellar, avoiding additives and processing aids." .

ns1:NonAlcoholicWine rdfs:comment "Beverages resembling wine but with alcohol removed or reduced to negligible levels." .

ns1:OakAgedStyle a ns1:WineStyle ;
    rdfs:comment "A wine style featuring vanilla, spice, and toast notes from barrel aging, typical of many premium red and white wines." .

ns1:OpusOne a ns1:Winemaker ;
    rdfs:comment "A Napa Valley winery partnership between Robert Mondavi and Baron Philippe de Rothschild, producing Bordeaux-style blends." .

ns1:OrangeWine rdfs:comment "White wine made with extended skin contact, resulting in an orange hue and tannic structure." .

ns1:OrganicWine rdfs:comment "Wine produced from grapes grown without synthetic pesticides or fertilizers, following organic farming standards." .

ns1:Oxidation rdfs:comment "A chemical reaction where wine components interact with oxygen, potentially leading to spoilage." .

ns1:PinotNoir a ns1:GrapeVariety ;
    rdfs:comment "A delicate red grape variety known for its light color, red fruit aromas, and sensitivity to terroir, prominent in Burgundy." .

ns1:Producer rdfs:comment "An entity responsible for growing grapes and/or producing wine." .

ns1:Quality rdfs:comment "An assessment of wine excellence based on sensory characteristics, balance, and typicity." .

ns1:Reduction rdfs:comment "A wine fault characterized by unpleasant sulfurous aromas due to lack of oxygen during winemaking." .

ns1:Regulation rdfs:comment "Legal standards and requirements governing wine production, labeling, and marketing." .

ns1:Rioja a ns1:Region ;
    rdfs:comment "A Spanish wine region specializing in Tempranillo-based wines, known for traditional oak aging classifications." .

ns1:RoseWine a ns1:WineColor ;
    rdfs:comment "A pink-colored wine produced by limited contact with grape skins during fermentation, offering characteristics between red and white wines." ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/00003308> .

ns1:RoséWine a owl:Class ;
    rdfs:label "Rosé Wine" ;
    rdfs:comment "A type of wine that incorporates some color from the grape skins but not enough to qualify as red wine." ;
    rdfs:subClassOf ns1:Wine .

ns1:Screwcap rdfs:comment "An alternative bottle closure made from metal, providing consistent sealing and preventing cork taint." .

ns1:StillWine a ns1:WineType ;
    rdfs:subClassOf <http://purl.obolibrary.org/obo/00003309> .

ns1:SustainableWine rdfs:comment "Wine made through practices that environmentally, socially, and economically sustainable." .

ns1:SweetWine rdfs:comment "A wine with noticeable residual sugar, creating a sweet or dessert-like flavor profile." .

ns1:Sweetness rdfs:comment "The perceived sugar content in wine, ranging from dry to sweet." .

ns1:TaylorsPort a ns1:FortifiedWine ;
    rdfs:comment "Traditional Portuguese Port wine with rich dark fruit flavors, fortified with grape spirit during fermentation." ;
    ns1:hasAlcoholContent ns1:Alc20Percent ;
    ns1:hasFoodPairing ns1:ChocolatePairing ;
    ns1:hasVintage ns1:Year2017 ;
    ns1:hasWineStyle ns1:BoldStyle ;
    ns1:hasWineType ns1:FortifiedWine ;
    ns1:hasWinemaker ns1:TaylorFladgate ;
    ns1:madeFromGrape ns1:TourigaNacional ;
    ns1:producedInRegion ns1:Douro ;
    ns1:underwentFermentation ns1:PrimaryFermentation .

ns1:Terroir rdfs:comment "The complete natural environment in which a particular wine is produced, including factors like soil, climate, and topography." .

ns1:Tignanello a ns1:RedWine ;
    rdfs:comment "Pioneering Super Tuscan blend of Sangiovese with Cabernet Sauvignon, aged in French oak barrels." ;
    ns1:hasAlcoholContent ns1:Alc14Percent ;
    ns1:hasFoodPairing ns1:PastaPairing ;
    ns1:hasVintage ns1:Year2016 ;
    ns1:hasWineStyle ns1:BoldStyle ;
    ns1:hasWineType ns1:RedWine ;
    ns1:hasWinemaker ns1:Antinori ;
    ns1:madeFromGrape ns1:Sangiovese ;
    ns1:producedInRegion ns1:Tuscany ;
    ns1:underwentFermentation ns1:BarrelFermentation .

ns1:VeganWine rdfs:comment "Wine processed without animal-derived fining agents, suitable for vegan diets." .

ns1:VintageYear rdfs:comment "The specific calendar year associated with a wine's grape harvest." .

ns1:WineAnalysis rdfs:comment "Laboratory testing of wine to determine chemical composition and ensure quality standards." .

ns1:WineAward rdfs:comment "A recognition given to wines for excellence in specific categories or competitions." .

ns1:WineChemistry rdfs:comment "The scientific study of chemical compounds and reactions involved in winemaking and aging." .

ns1:WineCollection rdfs:comment "An assembled group of wines, often curated for investment, consumption, or educational purposes." .

ns1:WineCompetition rdfs:comment "An organized event where wines are evaluated and awarded based on quality assessments." .

ns1:WineCritic rdfs:comment "A professional who assesses and rates wines, influencing consumer preferences and market values." .

ns1:WineCulture rdfs:comment "The social traditions, rituals, and knowledge systems surrounding wine consumption and appreciation." .

ns1:WineDistribution rdfs:comment "The network and processes involved in moving wine from producers to consumers." .

ns1:WineEducation rdfs:comment "Formal or informal training programs focused on wine knowledge, tasting, and production." .

ns1:WineExpert rdfs:comment "An individual with specialized knowledge in wine tasting, evaluation, and industry practices." .

ns1:WineExport rdfs:comment "The international trade of wine across national borders." .

ns1:WineFault rdfs:comment "An undesirable characteristic in wine resulting from production errors, contamination, or improper storage." .

ns1:WineGlass rdfs:comment "A specially shaped vessel designed to enhance the tasting experience of different wine types." .

ns1:WineHistory rdfs:comment "The historical development of winemaking practices, regions, and cultural significance." .

ns1:WineImport rdfs:comment "The process of bringing foreign wines into a country for distribution and sale." .

ns1:WineInvestment rdfs:comment "The practice of purchasing wines with the expectation of increasing value over time." .

ns1:WineJournal rdfs:comment "A recorded collection of tasting notes, observations, and experiences with different wines." .

ns1:WineLaw rdfs:comment "Legal regulations governing wine production, labeling, advertising, and trade." .

ns1:WineLiterature rdfs:comment "Written works including books, articles, and reviews dedicated to wine knowledge and criticism." .

ns1:WineMaker rdfs:comment "An individual or entity responsible for overseeing the winemaking process from grape selection to bottling." .

ns1:WineMarket rdfs:comment "The economic system involving production, distribution, and consumption of wine products." .

ns1:WineMedia rdfs:comment "Publications, broadcasts, and digital content focused on wine news, reviews, and education." .

ns1:WinePrice rdfs:comment "The monetary value assigned to a wine based on quality, rarity, demand, and production costs." .

ns1:WineRetail rdfs:comment "The business of selling wine directly to consumers through various channels." .

ns1:WineSensoryEvaluation rdfs:comment "The systematic assessment of wine using human senses to evaluate appearance, aroma, flavor, and mouthfeel." .

ns1:WineService rdfs:comment "The methods and etiquette of presenting and serving wine to consumers." .

ns1:WineStorage rdfs:comment "The practice of storing wine under controlled conditions to maintain or improve quality over time." .

ns1:WineTasting rdfs:comment "The practice of sampling and evaluating wines to understand their characteristics and quality." .

ns1:WineTax rdfs:comment "Government-imposed charges on wine production, import, or sale." .

ns1:WineTechnology rdfs:comment "Innovative tools and methods applied to viticulture and winemaking to improve efficiency and quality." .

ns1:WineTourism rdfs:comment "Travel activities centered around visiting wineries, vineyards, and wine-producing regions." .

ns1:WineWholesale rdfs:comment "The bulk sale of wine to retailers, restaurants, or other distributors." .

ns1:WinemakingProcess rdfs:comment "The sequence of operations from grape processing to bottling that transforms grapes into wine." .

ns1:acidityIn a owl:ObjectProperty ;
    rdfs:label "acidity in" ;
    rdfs:comment "Relates an acid compound to the wines it contributes acidity to." ;
    owl:inverseOf ns1:hasAcidity .

ns1:adjacentTo a owl:SymmetricProperty ;
    rdfs:label "adjacent to" ;
    rdfs:comment "Symmetric relationship indicating geographical adjacency between wine regions." ;
    rdfs:domain ns1:Region ;
    rdfs:range ns1:Region .

ns1:agingProcessOf a owl:ObjectProperty ;
    rdfs:label "aging process of" ;
    rdfs:comment "Relates an aging process to the wines that undergo it." ;
    owl:inverseOf ns1:hasAgingProcess .

ns1:appellationFor a owl:ObjectProperty ;
    rdfs:label "appellation for" ;
    rdfs:comment "Relates an appellation to the wines that carry it." ;
    owl:inverseOf ns1:hasAppellation .

ns1:aromaOf a owl:ObjectProperty ;
    rdfs:label "aroma of" ;
    rdfs:comment "Relates an aroma attribute to the wines exhibiting it." ;
    owl:inverseOf ns1:hasAroma .

ns1:bitternessOf a owl:ObjectProperty ;
    rdfs:label "bitterness of" ;
    rdfs:comment "Relates a bitterness attribute to the wines exhibiting it." ;
    owl:inverseOf ns1:hasBitterness .

ns1:blendedWith a owl:SymmetricProperty ;
    rdfs:label "blended with" ;
    rdfs:comment "Symmetric relationship indicating mutual blending relationship between grape varieties." ;
    rdfs:domain ns1:GrapeVariety ;
    rdfs:range ns1:GrapeVariety .

ns1:bodyOf a owl:ObjectProperty ;
    rdfs:label "body of" ;
    rdfs:comment "Relates a body attribute to the wines exhibiting it." ;
    owl:inverseOf ns1:hasBody .

ns1:bottlingProcessOf a owl:ObjectProperty ;
    rdfs:label "bottling process of" ;
    rdfs:comment "Relates a bottling process to the wines that undergo it." ;
    owl:inverseOf ns1:hasBottlingProcess .

ns1:characteristicOfVintage a owl:ObjectProperty ;
    rdfs:label "characteristic of vintage" ;
    rdfs:comment "Relates vintage characteristics to the vintages they describe." ;
    owl:inverseOf ns1:hasVintageCharacteristic .

ns1:chemicalCompositionOf a owl:ObjectProperty ;
    rdfs:label "chemical composition of" ;
    rdfs:comment "Relates a chemical composition to the wine it analyzes." ;
    owl:inverseOf ns1:hasChemicalComposition .

ns1:clarificationMethodFor a owl:ObjectProperty ;
    rdfs:label "clarification method for" ;
    rdfs:comment "Relates a clarification method to the wines it is applied to." ;
    owl:inverseOf ns1:hasClarificationMethod .

ns1:classificationFor a owl:ObjectProperty ;
    rdfs:label "classification for" ;
    rdfs:comment "Relates a regulatory designation to the wines it classifies." ;
    owl:inverseOf ns1:classifiedAs .

ns1:climateOf a owl:ObjectProperty ;
    rdfs:label "climate of" ;
    rdfs:comment "Relates a climate to the regions that experience it." ;
    owl:inverseOf ns1:hasClimate .

ns1:collaboratesWith a owl:SymmetricProperty ;
    rdfs:label "collaborates with" ;
    rdfs:comment "Symmetric relationship indicating mutual collaboration between winemakers." ;
    rdfs:domain ns1:Winemaker ;
    rdfs:range ns1:Winemaker .

ns1:compatibleWith a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty ;
    rdfs:label "compatible with" ;
    rdfs:comment "Relates a wine to another wine that can be paired or served together, including self-compatibility.",
        "Symmetric relationship indicating mutual compatibility between wine styles." ;
    rdfs:domain ns1:Wine,
        ns1:WineStyle ;
    rdfs:range ns1:Wine,
        ns1:WineStyle .

ns1:complianceFor a owl:ObjectProperty ;
    rdfs:label "compliance for" ;
    rdfs:comment "Relates legislation to the wine labels that comply with it." ;
    owl:inverseOf ns1:compliesWith .

ns1:containsVineyard a owl:ObjectProperty ;
    rdfs:label "contains vineyard" ;
    rdfs:comment "Relates viticulture practices to the vineyards they encompass." ;
    owl:inverseOf ns1:partOf .

ns1:contentOfWine a owl:ObjectProperty ;
    rdfs:label "content of wine" ;
    rdfs:comment "Relates an alcohol content measurement to the wine it belongs to." ;
    owl:inverseOf ns1:hasAlcoholContent .

ns1:describesWine a owl:ObjectProperty ;
    rdfs:domain ns1:TastingNote ;
    rdfs:range ns1:Wine .

ns1:fermentationOf a owl:ObjectProperty ;
    rdfs:label "fermentation of" ;
    rdfs:comment "Relates a fermentation process to the wine that underwent it." ;
    owl:inverseOf ns1:underwentFermentation .

ns1:flavorOf a owl:ObjectProperty ;
    rdfs:label "flavor of" ;
    rdfs:comment "Relates a flavor attribute to the wines exhibiting it." ;
    owl:inverseOf ns1:hasFlavor .

ns1:hasAlcoholCompound a owl:ObjectProperty ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineAlcoholCompound ;
    owl:inverseOf ns1:alcoholCompoundIn .

ns1:hasAromaCompound a owl:ObjectProperty ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineAromaCompound ;
    owl:inverseOf ns1:aromaCompoundIn .

ns1:hasBitternessCompound a owl:ObjectProperty ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineBitternessCompound ;
    owl:inverseOf ns1:bitternessCompoundIn .

ns1:hasBodyCompound a owl:ObjectProperty ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineBodyCompound ;
    owl:inverseOf ns1:bodyCompoundIn .

ns1:hasComponentProcess a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:label "has component process" ;
    rdfs:comment "Transitive property relating winemaking processes to their sub-processes." ;
    rdfs:domain ns1:Fermentation ;
    rdfs:range ns1:Fermentation ;
    rdfs:subPropertyOf ns1:underwentFermentation .

ns1:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description" ;
    rdfs:comment "A textual description of a wine or related entity." ;
    rdfs:domain [ owl:unionOf ( ns1:Wine ns1:GrapeVariety ns1:Region ns1:Winemaker ns1:Vintage ns1:WineStyle ns1:FoodPairing ns1:AlcoholContent ns1:Fermentation ) ],
        [ owl:unionOf ( ns1:Wine ns1:GrapeVariety ns1:Region ns1:Winemaker ns1:Vintage ns1:WineStyle ns1:FoodPairing ns1:AlcoholContent ns1:Fermentation ns1:Vineyard ns1:WineLabel ) ],
        [ owl:unionOf ( ns1:Wine ns1:GrapeVariety ns1:Region ns1:Winemaker ns1:Vintage ns1:WineStyle ns1:FoodPairing ns1:AlcoholContent ns1:Fermentation ns1:Vineyard ns1:WineLabel ns1:ChemicalCompound ) ] ;
    rdfs:range xsd:string .

ns1:hasFlavorCompound a owl:ObjectProperty ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineFlavorCompound ;
    owl:inverseOf ns1:flavorCompoundIn .

ns1:hasName a owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:comment "The name of a wine, grape variety, region, or other entity." ;
