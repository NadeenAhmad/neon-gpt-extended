ns1:Acidity rdfs:comment "The level of acidic compounds in wine, contributing to freshness, balance, and aging potential." .

ns1:Aging rdfs:comment "The maturation period where wine develops complexity through chemical interactions in various containers." .

ns1:Alc11Percent a ns1:AlcoholContent ;
    rdfs:comment "Moderate alcohol content found in German Rieslings and other light-bodied wines." ;
    ns1:hasPercentage 11.5 .

ns1:Alc15Percent a ns1:AlcoholContent ;
    rdfs:comment "High alcohol content typical of fortified wines like Port and some Zinfandels." ;
    ns1:hasPercentage 15.0 .

ns1:Aroma rdfs:comment "The scent of wine derived from grape varieties, fermentation, and aging processes." .

ns1:BeaujolaisNouveau a ns1:RedWine ;
    rdfs:comment "Young, fruity red wine released shortly after harvest, known for its vibrant cherry and banana notes." ;
    ns1:hasAlcoholContent ns1:Alc12Percent ;
    ns1:hasFoodPairing ns1:PoultryPairing ;
    ns1:hasVintage ns1:Year2020 ;
    ns1:hasWineStyle ns1:FruityStyle ;
