    ns1:hasDescription "A sample vineyard for demonstration purposes.",
        "A specific vineyard site within a wine region." ;
    ns1:hasName "Example Vineyard" ;
    ns1:locatedIn ns1:Bordeaux ;
    ns1:partOf ns1:ViticulturePractice .

ns1:ExampleWineLabel a ns1:WineLabel ;
    ns1:compliesWith <http://eurovoc.europa.eu/WineLabelingDirective> ;
