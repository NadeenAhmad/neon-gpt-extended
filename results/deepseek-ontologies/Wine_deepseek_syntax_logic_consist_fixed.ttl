    ns1:producedInRegion ns1:Bordeaux ;
    ns1:underwentFermentation ns1:BarrelFermentation .

ns1:CheesePairing a ns1:FoodPairing ;
    rdfs:comment "Pairs well with assorted cheeses, from soft brie to aged hard cheeses, creating complementary flavor interactions." .

ns1:CloudyBaySauvignonBlanc a ns1:WhiteWine ;
    rdfs:comment "Iconic New Zealand Sauvignon Blanc with intense passionfruit, grapefruit, and herbaceous notes." ;
    ns1:hasAlcoholContent ns1:Alc13Percent ;
    ns1:hasFoodPairing ns1:SeafoodPairing ;
    ns1:hasVintage ns1:Year2020 ;
    ns1:hasWineStyle ns1:CrispStyle ;
    ns1:hasWineType ns1:WhiteWine ;
    ns1:hasWinemaker ns1:CloudyBay ;
    ns1:madeFromGrape ns1:SauvignonBlanc ;
    ns1:producedInRegion ns1:Marlborough ;
    ns1:underwentFermentation ns1:StainlessSteelFermentation .

ns1:Color rdfs:comment "The visual hue of wine, primarily determined by grape variety and skin contact during winemaking." .

ns1:Cork rdfs:comment "A traditional sealing material made from bark, used to close wine bottles and allow controlled aging." .

ns1:CorkTaint rdfs:comment "A wine fault caused by trichloroanisole (TCA) contamination, resulting in musty odors and flavors." .

ns1:Crushing rdfs:comment "The initial step in winemaking where grapes are crushed to release juice and begin fermentation." .

ns1:Decanting rdfs:comment "The process of transferring wine from its bottle to another container to separate sediment and aerate the wine." .

ns1:DomPerignon a ns1:SparklingWine ;
    rdfs:comment "Prestige cuvée Champagne known for its complexity, fine bubbles, and extended lees aging." ;
    ns1:hasAlcoholContent ns1:Alc12Percent ;
    ns1:hasFoodPairing ns1:SeafoodPairing ;
    ns1:hasVintage ns1:Year2012 ;
    ns1:hasWineStyle ns1:ElegantStyle ;
