    rdfs:label "contains sample" ;
    rdfs:comment "Relates a sampling environment to the samples located within it" .

:isTechniqueFor rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasMeasurementTechnique ;
    rdfs:label "is technique for" ;
    rdfs:comment "Relates a measurement technique to the samples it is used on" .

:isVariableOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasEnvironmentalVariable ;
    rdfs:label "is variable of" ;
    rdfs:comment "Relates an environmental variable to the sampling environment it characterizes" .

:isMediatedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :mediatesProcess ;
    rdfs:label "is mediated by" ;
    rdfs:comment "Relates a biogeochemical process to the microbial taxon that mediates it" .

:isSpatialContextOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasSpatialContext ;
    rdfs:label "is spatial context of" ;
    rdfs:comment "Relates a spatial context to the sample it describes" .

:isTemporalContextOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasTemporalContext ;
    rdfs:label "is temporal context of" ;
    rdfs:comment "Relates a temporal context to the sample it describes" .

:hasMember rdf:type owl:ObjectProperty ;
    owl:inverseOf :memberOfPopulation ;
    rdfs:label "has member" ;
    rdfs:comment "Relates a population to the organisms that are members of it" .

:hasParticipant rdf:type owl:ObjectProperty ;
    owl:inverseOf :participatesInInteraction ;
    rdfs:label "has participant" ;
    rdfs:comment "Relates an ecosystem interaction to the organisms that participate in it" .

:hasSample rdf:type owl:ObjectProperty ;
    owl:inverseOf :partOfStudy ;
    rdfs:label "has sample" ;
    rdfs:comment "Relates a study to the samples that are part of it" .

# --- Turtle block from step_13_reflexive_properties ---
@prefix : <http://aquadiva.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:isSimilarTo rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "is similar to" ;
    rdfs:comment "A reflexive property indicating that an entity is similar to itself or other entities of the same type" .

:isPartOf rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "is part of" ;
    rdfs:comment "A reflexive property indicating that an entity can be part of itself or other entities in a meronomic relationship" .

:isLocatedIn rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "is located in" ;
    rdfs:comment "A reflexive property indicating that an entity can be located within itself or other spatial entities" .

:interactsWith rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "interacts with" ;
    rdfs:comment "A reflexive property indicating that an entity can interact with itself or other entities in the ecosystem" .

:participatesIn rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "participates in" ;
    rdfs:comment "A reflexive property indicating that an entity can participate in itself or other processes or interactions" .

:hasMember rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "has member" ;
    rdfs:comment "A reflexive property indicating that a population or group can have itself or other organisms as members" .

:belongsTo rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "belongs to" ;
    rdfs:comment "A reflexive property indicating that an entity can belong to itself or other taxonomic or functional groups" .

:influences rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "influences" ;
    rdfs:comment "A reflexive property indicating that an entity can influence itself or other entities in environmental or biological contexts" .

:derivesFrom rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "derives from" ;
    rdfs:comment "A reflexive property indicating that an entity can derive from itself or other entities in evolutionary or process contexts" .

:overlapsWith rdf:type owl:ReflexiveProperty, owl:ObjectProperty ;
    rdfs:label "overlaps with" ;
    rdfs:comment "A reflexive property indicating that an entity can overlap with itself or other entities in spatial or functional domains" .

# --- Turtle block from step_14_symmetric_properties ---
:hasFunctionalRole rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a microbial taxon has a functional role, then that functional role is performed by the microbial taxon" .

:participatesInPathway rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a microbial taxon participates in a metabolic pathway, then that metabolic pathway involves the microbial taxon" .

:locatedIn rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a sample is located in a sampling environment, then that sampling environment contains the sample" .

:hasMeasurementTechnique rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a sample has a measurement technique, then that measurement technique is used on the sample" .

:hasEnvironmentalVariable rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a sampling environment has an environmental variable, then that environmental variable characterizes the sampling environment" .

:mediatesProcess rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a microbial taxon mediates a biogeochemical process, then that biogeochemical process is mediated by the microbial taxon" .

:hasSpatialContext rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a sample has a spatial context, then that spatial context describes the sample's location" .

:hasTemporalContext rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a sample has a temporal context, then that temporal context describes the sample's timing" .

:memberOfPopulation rdf:type owl:SymmetricProperty ;
    rdfs:comment "If an organism is a member of a population, then that population includes the organism" .

:participatesInInteraction rdf:type owl:SymmetricProperty ;
    rdfs:comment "If an organism participates in an ecosystem interaction, then that ecosystem interaction involves the organism" .

:partOfStudy rdf:type owl:SymmetricProperty ;
    rdfs:comment "If a sample is part of a study, then that study includes the sample" .

