# Extended Class Hierarchy
:MicrobialCommunity rdf:type owl:Class ;
    rdfs:subClassOf :Population ;
    rdfs:label "Microbial Community" ;
    rdfs:comment "A community of microorganisms inhabiting a particular environment" .

:Archaea rdf:type owl:Class ;
    rdfs:subClassOf :MicrobialTaxon ;
    rdfs:label "Archaea" ;
    rdfs:comment "A domain of single-celled microorganisms" .

:Bacteria rdf:type owl:Class ;
    rdfs:subClassOf :MicrobialTaxon ;
    rdfs:label "Bacteria" ;
    rdfs:comment "A domain of prokaryotic microorganisms" .

:MicrobialFunction rdf:type owl:Class ;
    rdfs:subClassOf :FunctionalRole ;
    rdfs:label "Microbial Function" ;
    rdfs:comment "A specific function performed by microorganisms" .

:EcosystemFunction rdf:type owl:Class ;
    rdfs:subClassOf :BiogeochemicalProcess ;
    rdfs:label "Ecosystem Function" ;
    rdfs:comment "A function that contributes to ecosystem processes" .

:SamplingSite rdf:type owl:Class ;
    rdfs:subClassOf :SamplingEnvironment ;
    rdfs:label "Sampling Site" ;
    rdfs:comment "A specific location where samples are collected" .

:EnvironmentalCondition rdf:type owl:Class ;
    rdfs:subClassOf :EnvironmentalVariable ;
    rdfs:label "Environmental Condition" ;
    rdfs:comment "A condition that describes the state of the environment" .

:LowOxygenCondition rdf:type owl:Class ;
    rdfs:subClassOf :EnvironmentalCondition ;
    rdfs:label "Low Oxygen Condition" ;
    rdfs:comment "An environmental condition characterized by low oxygen levels" .

:AnoxicCondition rdf:type owl:Class ;
    rdfs:subClassOf :LowOxygenCondition ;
    rdfs:label "Anoxic Condition" ;
    rdfs:comment "An environmental condition completely devoid of oxygen" .

:Microhabitat rdf:type owl:Class ;
    rdfs:subClassOf :SamplingEnvironment ;
    rdfs:label "Microhabitat" ;
    rdfs:comment "A small, specialized habitat within a larger environment" .

:PoreWater rdf:type owl:Class ;
    rdfs:subClassOf :Microhabitat ;
    rdfs:label "Pore Water" ;
    rdfs:comment "Water contained in pores of soil or rock" .

:Groundwater rdf:type owl:Class ;
    rdfs:subClassOf :Microhabitat ;
    rdfs:label "Groundwater" ;
    rdfs:comment "Water located beneath the ground surface" .

:Biofilm rdf:type owl:Class ;
    rdfs:subClassOf :MicrobialCommunity ;
    rdfs:label "Biofilm" ;
    rdfs:comment "A structured community of microorganisms encapsulated in a self-produced matrix" .

:TrophicInteraction rdf:type owl:Class ;
    rdfs:subClassOf :EcosystemInteraction ;
    rdfs:label "Trophic Interaction" ;
    rdfs:comment "An interaction involving energy transfer between trophic levels" .

:SymbioticInteraction rdf:type owl:Class ;
    rdfs:subClassOf :EcosystemInteraction ;
    rdfs:label "Symbiotic Interaction" ;
    rdfs:comment "A close and long-term biological interaction between different biological organisms" .

:Metagenome rdf:type owl:Class ;
    rdfs:subClassOf iao:0000100 ; # information content entity
    rdfs:label "Metagenome" ;
    rdfs:comment "The collective genetic material recovered directly from environmental samples" .

:MicrobialGenome rdf:type owl:Class ;
    rdfs:subClassOf :Metagenome ;
    rdfs:label "Microbial Genome" ;
    rdfs:comment "The complete set of genes or genetic material present in a microorganism" .

# Extended Object Properties
:inhabits rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialTaxon ;
    rdfs:range :SamplingEnvironment ;
    rdfs:label "inhabits" ;
    rdfs:comment "Relates a microbial taxon to the environment it inhabits" .

:performs rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialTaxon ;
    rdfs:range :MicrobialFunction ;
    rdfs:label "performs" ;
    rdfs:comment "Relates a microbial taxon to a function it performs" .

:hasFunction rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialCommunity ;
    rdfs:range :EcosystemFunction ;
    rdfs:label "has function" ;
    rdfs:comment "Relates a microbial community to an ecosystem function it carries out" .

:collectedUsing rdf:type owl:ObjectProperty ;
    rdfs:domain :Sample ;
    rdfs:range mixs:0000001 ; # MIxS sampling device
    rdfs:label "collected using" ;
    rdfs:comment "Relates a sample to the device used for collection" .

:hasEnvironmentalCondition rdf:type owl:ObjectProperty ;
    rdfs:domain :Sample ;
    rdfs:range :EnvironmentalCondition ;
    rdfs:label "has environmental condition" ;
    rdfs:comment "Relates a sample to its environmental conditions" .

:mediatedBy rdf:type owl:ObjectProperty ;
    rdfs:domain :BiogeochemicalProcess ;
    rdfs:range :MicrobialTaxon ;
    rdfs:label "mediated by" ;
    rdfs:comment "Relates a biogeochemical process to the microbial taxon that mediates it" .

:partOf rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialTaxon ;
    rdfs:range :MicrobialCommunity ;
    rdfs:label "part of" ;
    rdfs:comment "Relates a microbial taxon to the community it is part of" .

:associatedWith rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialTaxon ;
    rdfs:range :BiogeochemicalProcess ;
    rdfs:label "associated with" ;
    rdfs:comment "Relates a microbial taxon to a biogeochemical process it is associated with" .

:hasGenomicFeature rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialGenome ;
    rdfs:range obo:SO_0000110 ; # sequence feature
    rdfs:label "has genomic feature" ;
    rdfs:comment "Relates a microbial genome to its genomic features" .

:hasTaxonomicRank rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialTaxon ;
    rdfs:range obo:NCBITaxon_0000000 ; # taxonomic rank
    rdfs:label "has taxonomic rank" ;
    rdfs:comment "Relates a microbial taxon to its taxonomic rank" .

:hasMeasurementUnit rdf:type owl:ObjectProperty ;
    rdfs:domain :EnvironmentalVariable ;
    rdfs:range obo:UO_0000000 ; # unit of measurement
    rdfs:label "has measurement unit" ;
    rdfs:comment "Relates an environmental variable to its measurement unit" .

:hasQuality rdf:type owl:ObjectProperty ;
    rdfs:domain :EnvironmentalCondition ;
    rdfs:range pato:0000001 ; # quality
    rdfs:label "has quality" ;
    rdfs:comment "Relates an environmental condition to its quality" .

