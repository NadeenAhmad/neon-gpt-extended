:Aspirin_Provenance a prov:Entity ;
    rdfs:label "aspirin provenance" ;
    rdfs:comment "Provenance information for aspirin descriptors and representations." .

# Property Assertions
:Caffeine :hasDescriptor :Caffeine_MolecularWeight ;
    :hasDescriptor :Caffeine_LogP ;
    :hasDescriptor :Caffeine_TPSA ;
    :hasChemicalGraph :Caffeine_SMILES ;
    :hasChemicalGraph :Caffeine_InChI .

:Aspirin :hasDescriptor :Aspirin_MolecularWeight ;
    :hasDescriptor :Aspirin_LogP ;
    :hasDescriptor :Aspirin_TPSA ;
    :hasChemicalGraph :Aspirin_SMILES ;
    :hasChemicalGraph :Aspirin_InChI .

:Caffeine_MolecularWeight :isDescriptorOf :Caffeine ;
    :isCalculatedBy :RDKit_Descriptor_Algorithm ;
    :hasProvenance :Caffeine_Provenance .

:Aspirin_MolecularWeight :isDescriptorOf :Aspirin ;
    :isCalculatedBy :RDKit_Descriptor_Algorithm ;
    :hasProvenance :Aspirin_Provenance .

:Caffeine_LogP :isDescriptorOf :Caffeine ;
    :isCalculatedBy :RDKit_Descriptor_Algorithm ;
    :hasProvenance :Caffeine_Provenance .

:Aspirin_LogP :isDescriptorOf :Aspirin ;
    :isCalculatedBy :RDKit_Descriptor_Algorithm ;
    :hasProvenance :Aspirin_Provenance .

:Caffeine_TPSA :isDescriptorOf :Caffeine ;
    :isCalculatedBy :RDKit_Descriptor_Algorithm ;
    :hasProvenance :Caffeine_Provenance .

:Aspirin_TPSA :isDescriptorOf :Aspirin ;
    :isCalculatedBy :RDKit_Descriptor_Algorithm ;
    :hasProvenance :Aspirin_Provenance .

:Caffeine_SMILES :isChemicalGraphOf :Caffeine ;
    :hasFormat :SMILES_Format .

:Aspirin_SMILES :isChemicalGraphOf :Aspirin ;
    :hasFormat :SMILES_Format .

:Caffeine_InChI :isChemicalGraphOf :Caffeine ;
    :hasFormat :InChI_Format .

:Aspirin_InChI :isChemicalGraphOf :Aspirin ;
    :hasFormat :InChI_Format .

:OpenBabel_Algorithm :isImplementedBy :OpenBabel_Software ;
    :calculatesDescriptor :Caffeine_MolecularWeight, :Aspirin_MolecularWeight .

:RDKit_Descriptor_Algorithm :isImplementedBy :RDKit_Software ;
    :calculatesDescriptor :Caffeine_MolecularWeight, :Aspirin_MolecularWeight, :Caffeine_LogP, :Aspirin_LogP, :Caffeine_TPSA, :Aspirin_TPSA .

:CDK_Algorithm :isImplementedBy :CDK_Software ;
    :calculatesDescriptor :Caffeine_MolecularWeight, :Aspirin_MolecularWeight .

:SMILES_Format :isFormatOf :Caffeine_SMILES, :Aspirin_SMILES .

:InChI_Format :isFormatOf :Caffeine_InChI, :Aspirin_InChI .

:Caffeine_Provenance :isProvenanceOf :Caffeine_MolecularWeight, :Caffeine_LogP, :Caffeine_TPSA .

:Aspirin_Provenance :isProvenanceOf :Aspirin_MolecularWeight, :Aspirin_LogP, :Aspirin_TPSA .

# --- Turtle block from step_18_metadata ---
@prefix : <http://purl.obolibrary.org/obo/cheminf.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
