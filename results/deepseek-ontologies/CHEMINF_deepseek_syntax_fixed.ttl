:isImplementedBy rdf:type owl:SymmetricProperty ;
    rdfs:comment "A symmetric property linking an algorithm to its software implementation and vice versa." .

:calculatesDescriptor rdf:type owl:SymmetricProperty ;
    rdfs:comment "A symmetric property linking an algorithm to the descriptor it calculates and vice versa." .

:isCalculatedBy rdf:type owl:SymmetricProperty ;
    rdfs:comment "A symmetric property linking a descriptor to the algorithm that calculates it and vice versa." .

:hasFormat rdf:type owl:SymmetricProperty ;
    rdfs:comment "A symmetric property linking a chemical graph to its data format and vice versa." .

:isFormatOf rdf:type owl:SymmetricProperty ;
    rdfs:comment "A symmetric property linking a data format to its chemical graph and vice versa." .

:hasProvenance rdf:type owl:SymmetricProperty ;
