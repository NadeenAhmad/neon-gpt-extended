:implementsAlgorithm rdf:type owl:TransitiveProperty ;
    rdfs:comment "A transitive property linking software implementations to algorithms they implement through intermediate implementations." .

:isImplementedBy rdf:type owl:TransitiveProperty ;
    rdfs:comment "A transitive property indicating that an algorithm is implemented by software through intermediate implementations." .

:calculatesDescriptor rdf:type owl:TransitiveProperty ;
    rdfs:comment "A transitive property linking algorithms to descriptors they calculate through intermediate calculations." .

:isCalculatedBy rdf:type owl:TransitiveProperty ;
    rdfs:comment "A transitive property indicating that a descriptor is calculated by algorithms through intermediate calculations." .

:hasFormat rdf:type owl:TransitiveProperty ;
    rdfs:comment "A transitive property linking chemical graphs to their data formats through intermediate formats." .

:isFormatOf rdf:type owl:TransitiveProperty ;
    rdfs:comment "A transitive property indicating that a data format is used by chemical graphs through intermediate formats." .

:hasProvenance rdf:type owl:TransitiveProperty ;
    rdfs:comment "A transitive property linking chemical descriptors to their provenance information through intermediate provenance entities." .

:isProvenanceOf rdf:type owl:TransitiveProperty ;
    rdfs:comment "A transitive property indicating that provenance information applies to chemical descriptors through intermediate provenance entities." .

