# Inverse Object Properties
:isDescriptorOf rdf:type owl:ObjectProperty ;
    rdfs:label "is descriptor of" ;
    rdfs:comment "Inverse of hasDescriptor - links a chemical descriptor to the chemical entity it describes." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range sio:SIO_010004 ;
    owl:inverseOf :hasDescriptor .

:isChemicalGraphOf rdf:type owl:ObjectProperty ;
    rdfs:label "is chemical graph of" ;
    rdfs:comment "Inverse of hasChemicalGraph - links a chemical graph representation to the chemical entity it represents." ;
    rdfs:domain :ChemicalGraph ;
    rdfs:range sio:SIO_010004 ;
    owl:inverseOf :hasChemicalGraph .

:isImplementedBy rdf:type owl:ObjectProperty ;
    rdfs:label "is implemented by" ;
    rdfs:comment "Inverse of implementsAlgorithm - links an algorithm to the software implementation that executes it." ;
    rdfs:domain :Algorithm ;
    rdfs:range :SoftwareImplementation ;
    owl:inverseOf :implementsAlgorithm .

:isCalculatedBy rdf:type owl:ObjectProperty ;
    rdfs:label "is calculated by" ;
    rdfs:comment "Inverse of calculatesDescriptor - links a chemical descriptor to the algorithm that computes it." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :Algorithm ;
    owl:inverseOf :calculatesDescriptor .

:isFormatOf rdf:type owl:ObjectProperty ;
    rdfs:label "is format of" ;
    rdfs:comment "Inverse of hasFormat - links a data format to the chemical graph representations that use it." ;
    rdfs:domain :DataFormat ;
    rdfs:range :ChemicalGraph ;
