@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

# Ontology Header
:ChemicalInformationOntology rdf:type owl:Ontology ;
    rdfs:label "Chemical Information Ontology" ;
    rdfs:comment "An ontology for representing chemical information entities used in cheminformatics." .

# Classes
:ChemicalDescriptor rdf:type owl:Class ;
    rdfs:label "chemical descriptor" ;
    rdfs:comment "A quantitative or qualitative property of a chemical entity." ;
    rdfs:subClassOf sio:SIO_000614 .  # SIO:descriptor

:MolecularWeight rdf:type owl:Class ;
    rdfs:label "molecular weight" ;
    rdfs:comment "The mass of a molecule, typically in daltons." ;
    rdfs:subClassOf :ChemicalDescriptor .

:LogP rdf:type owl:Class ;
    rdfs:label "logP" ;
    rdfs:comment "The logarithm of the partition coefficient between octanol and water." ;
    rdfs:subClassOf :ChemicalDescriptor .

:TopologicalPolarSurfaceArea rdf:type owl:Class ;
    rdfs:label "topological polar surface area" ;
    rdfs:comment "The sum of the surface areas of polar atoms in a molecule." ;
    rdfs:subClassOf :ChemicalDescriptor .

:ChemicalGraph rdf:type owl:Class ;
    rdfs:label "chemical graph" ;
    rdfs:comment "A representation of molecular structure as a graph." ;
    rdfs:subClassOf sio:SIO_000113 .  # SIO:molecular entity

:SMILES rdf:type owl:Class ;
    rdfs:label "SMILES" ;
    rdfs:comment "Simplified Molecular Input Line Entry System representation." ;
    rdfs:subClassOf :ChemicalGraph .

:InChI rdf:type owl:Class ;
    rdfs:label "InChI" ;
    rdfs:comment "International Chemical Identifier representation." ;
    rdfs:subClassOf :ChemicalGraph .

:Algorithm rdf:type owl:Class ;
    rdfs:label "algorithm" ;
    rdfs:comment "A computational method for processing chemical information." ;
    rdfs:subClassOf sio:SIO_000089 .  # SIO:algorithm

:SoftwareImplementation rdf:type owl:Class ;
    rdfs:label "software implementation" ;
    rdfs:comment "A software tool that implements an algorithm." ;
    rdfs:subClassOf sio:SIO_000099 .  # SIO:software

:DataFormat rdf:type owl:Class ;
    rdfs:label "data format" ;
    rdfs:comment "A specification for representing chemical data." ;
    rdfs:subClassOf sio:SIO_000088 .  # SIO:data format

:MOL rdf:type owl:Class ;
    rdfs:label "MOL" ;
    rdfs:comment "MDL Molfile format for representing molecular structures." ;
    rdfs:subClassOf :DataFormat .

:SDF rdf:type owl:Class ;
    rdfs:label "SDF" ;
    rdfs:comment "Structure Data File format for multiple molecular structures." ;
    rdfs:subClassOf :DataFormat .

:ChemicalEntity rdf:type owl:Class ;
    rdfs:label "chemical entity" ;
    rdfs:comment "A chemical substance or molecular entity." ;
    rdfs:subClassOf sio:SIO_010004 .  # SIO:chemical entity

:DescriptorValue rdf:type owl:Class ;
    rdfs:label "descriptor value" ;
    rdfs:comment "A numerical or qualitative value of a chemical descriptor." .

:DataFormatSpecification rdf:type owl:Class ;
    rdfs:label "data format specification" ;
    rdfs:comment "A formal specification for a data format." ;
    rdfs:subClassOf sio:SIO_000088 .  # SIO:data format

:SMILESFormat rdf:type owl:Class ;
    rdfs:label "SMILES format" ;
    rdfs:comment "The data format specification for SMILES representations." ;
    rdfs:subClassOf :DataFormatSpecification .

:InChIFormat rdf:type owl:Class ;
    rdfs:label "InChI format" ;
    rdfs:comment "The data format specification for InChI representations." ;
    rdfs:subClassOf :DataFormatSpecification .

:ProvenanceInformation rdf:type owl:Class ;
    rdfs:label "provenance information" ;
    rdfs:comment "Information about the origin and processing history of data." ;
    rdfs:subClassOf prov:Entity .

:SoftwareVersion rdf:type owl:Class ;
    rdfs:label "software version" ;
    rdfs:comment "A specific version of a software implementation." .

# Object Properties
:hasDescriptor rdf:type owl:ObjectProperty ;
    rdfs:label "has descriptor" ;
    rdfs:comment "Links a chemical entity to its descriptor." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :ChemicalDescriptor .

:isDescriptorOf rdf:type owl:ObjectProperty ;
    rdfs:label "is descriptor of" ;
    rdfs:comment "Inverse of hasDescriptor." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :ChemicalEntity ;
    owl:inverseOf :hasDescriptor .

:hasChemicalGraph rdf:type owl:ObjectProperty ;
    rdfs:label "has chemical graph" ;
    rdfs:comment "Links a chemical entity to its graph representation." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :ChemicalGraph .

:isChemicalGraphOf rdf:type owl:ObjectProperty ;
    rdfs:label "is chemical graph of" ;
    rdfs:comment "Inverse of hasChemicalGraph." ;
    rdfs:domain :ChemicalGraph ;
    rdfs:range :ChemicalEntity ;
    owl:inverseOf :hasChemicalGraph .

:implementsAlgorithm rdf:type owl:ObjectProperty ;
    rdfs:label "implements algorithm" ;
    rdfs:comment "Links a software implementation to the algorithm it implements." ;
    rdfs:domain :SoftwareImplementation ;
    rdfs:range :Algorithm .

:isImplementedBy rdf:type owl:ObjectProperty ;
    rdfs:label "is implemented by" ;
    rdfs:comment "Inverse of implementsAlgorithm." ;
    rdfs:domain :Algorithm ;
    rdfs:range :SoftwareImplementation ;
    owl:inverseOf :implementsAlgorithm .

:calculatesDescriptor rdf:type owl:ObjectProperty ;
    rdfs:label "calculates descriptor" ;
    rdfs:comment "Links an algorithm to the descriptor it calculates." ;
    rdfs:domain :Algorithm ;
    rdfs:range :ChemicalDescriptor .

:isCalculatedBy rdf:type owl:ObjectProperty ;
    rdfs:label "is calculated by" ;
    rdfs:comment "Inverse of calculatesDescriptor." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :Algorithm ;
    owl:inverseOf :calculatesDescriptor .

:hasFormat rdf:type owl:ObjectProperty ;
    rdfs:label "has format" ;
    rdfs:comment "Links a chemical graph to its data format." ;
    rdfs:domain :ChemicalGraph ;
    rdfs:range :DataFormat .

:isFormatOf rdf:type owl:ObjectProperty ;
    rdfs:label "is format of" ;
    rdfs:comment "Inverse of hasFormat." ;
    rdfs:domain :DataFormat ;
    rdfs:range :ChemicalGraph ;
    owl:inverseOf :hasFormat .

:hasProvenance rdf:type owl:ObjectProperty ;
    rdfs:label "has provenance" ;
    rdfs:comment "Links a chemical descriptor to its provenance information." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :ProvenanceInformation .

:isProvenanceOf rdf:type owl:ObjectProperty ;
    rdfs:label "is provenance of" ;
    rdfs:comment "Inverse of hasProvenance." ;
    rdfs:domain :ProvenanceInformation ;
    rdfs:range :ChemicalDescriptor ;
    owl:inverseOf :hasProvenance .

:conformsTo rdf:type owl:ObjectProperty ;
    rdfs:label "conforms to" ;
    rdfs:comment "Links a chemical descriptor to the data format specification it conforms to." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :DataFormatSpecification .

:isConformedBy rdf:type owl:ObjectProperty ;
    rdfs:label "is conformed by" ;
    rdfs:comment "Inverse of conformsTo." ;
    rdfs:domain :DataFormatSpecification ;
    rdfs:range :ChemicalDescriptor ;
    owl:inverseOf :conformsTo .

:represents rdf:type owl:ObjectProperty ;
    rdfs:label "represents" ;
    rdfs:comment "Links a chemical graph to the chemical entity it represents." ;
    rdfs:domain :ChemicalGraph ;
    rdfs:range :ChemicalEntity .

:isRepresentedBy rdf:type owl:ObjectProperty ;
    rdfs:label "is represented by" ;
    rdfs:comment "Inverse of represents." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :ChemicalGraph ;
    owl:inverseOf :represents .

:encodedIn rdf:type owl:ObjectProperty ;
    rdfs:label "encoded in" ;
    rdfs:comment "Links a chemical graph to the specific format it is encoded in." ;
    rdfs:domain :ChemicalGraph ;
    rdfs:range :DataFormatSpecification .

:isEncodingOf rdf:type owl:ObjectProperty ;
    rdfs:label "is encoding of" ;
    rdfs:comment "Inverse of encodedIn." ;
    rdfs:domain :DataFormatSpecification ;
    rdfs:range :ChemicalGraph ;
    owl:inverseOf :encodedIn .

:hasVersion rdf:type owl:ObjectProperty ;
    rdfs:label "has version" ;
    rdfs:comment "Links a software implementation to its version." ;
    rdfs:domain :SoftwareImplementation ;
    rdfs:range :SoftwareVersion .

:isVersionOf rdf:type owl:ObjectProperty ;
    rdfs:label "is version of" ;
    rdfs:comment "Inverse of hasVersion." ;
    rdfs:domain :SoftwareVersion ;
    rdfs:range :SoftwareImplementation ;
    owl:inverseOf :hasVersion .

# Data Properties
:hasValue rdf:type owl:DatatypeProperty ;
    rdfs:label "has value" ;
    rdfs:comment "Links a descriptor to its numerical value." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range xsd:double .

:hasStringRepresentation rdf:type owl:DatatypeProperty ;
    rdfs:label "has string representation" ;
    rdfs:comment "Links a chemical graph to its string encoding." ;
    rdfs:domain :ChemicalGraph ;
    rdfs:range xsd:string .

:hasMIMEType rdf:type owl:DatatypeProperty ;
    rdfs:label "has MIME type" ;
    rdfs:comment "Links a data format to its MIME type." ;
    rdfs:domain :DataFormat ;
    rdfs:range xsd:string .

:hasVersionString rdf:type owl:DatatypeProperty ;
    rdfs:label "has version string" ;
    rdfs:comment "Links a software version to its version string." ;
    rdfs:domain :SoftwareVersion ;
    rdfs:range xsd:string .

