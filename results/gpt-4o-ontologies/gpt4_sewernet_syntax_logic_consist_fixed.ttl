        owl:TransitiveProperty ;
    rdfs:comment "A component may be recursively involved in a meta-event involving itself.",
        "If A is involved in B, and B is involved in C, then A is involved in C.",
        "Links a component to the event involving it." ;
    owl:inverseOf ns1:involvesComponent .

ns1:isMonitoredBy a owl:ObjectProperty ;
    rdfs:comment "Links a sewer component to the sensor monitoring it." ;
    owl:inverseOf ns1:monitors .

ns1:isProducedBy a owl:ObjectProperty ;
    rdfs:comment "Links a finding to the inspection producing it." ;
    owl:inverseOf ns1:producesFinding .

ns1:isRepairFor a owl:ObjectProperty ;
    rdfs:comment "Links a repair to the condition it corrects." ;
    owl:inverseOf ns1:requiresRepair .

ns1:isRestoredBy a owl:ObjectProperty ;
    rdfs:comment "Links a structural condition to the repair that restores it." ;
    owl:inverseOf ns1:restores .

ns1:isSimilarTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Indicates mutual similarity between two infrastructure elements." .

ns1:isStatusOf a owl:ObjectProperty ;
    rdfs:comment "Links a status back to the infrastructure component." ;
    owl:inverseOf ns1:hasStatus .

ns1:isTargetOf a owl:ObjectProperty ;
    rdfs:comment "Links a sewer component to the inspection targeting it." ;
    owl:inverseOf ns1:targets .

ns1:isZoneTypeOf a owl:ObjectProperty ;
    rdfs:comment "Links an urban zone type to its geospatial region." ;
    owl:inverseOf ns1:hasZoneType .

ns1:locatedInWatershed a owl:ObjectProperty ;
    rdfs:comment "Indicates the watershed in which a component is located." ;
    rdfs:domain ns1:SewerNetworkComponent ;
    rdfs:range ns1:Watershed .

ns1:locationOf a owl:ObjectProperty ;
    rdfs:comment "Links a geospatial point to the infrastructure component it describes." ;
    owl:inverseOf ns1:hasLocation .

ns1:monitoredBySensor a owl:ObjectProperty ;
    rdfs:comment "Relates a network component to the sensor monitoring it." ;
    rdfs:domain ns1:SewerNetworkComponent ;
    rdfs:range ns1:SensorDevice .

ns1:relatedToPermit a owl:ObjectProperty ;
    rdfs:comment "Associates an overflow event with a discharge permit." ;
    rdfs:domain ns1:Overflow ;
    rdfs:range ns1:DischargePermit .

ns1:schedules a owl:ObjectProperty ;
    rdfs:comment "Links a plan to the maintenance it schedules." ;
    owl:inverseOf ns1:scheduledBy .

ns1:sharesBoundaryWith a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:comment "Two zones share a common spatial boundary." .

ns1:AccessStructure rdfs:comment "A component providing access to the sewer network, such as a manhole." ;
    rdfs:subClassOf ns1:SewerNetworkComponent .

ns1:Conduit rdfs:comment "A generic class for linear flow channels like pipes." ;
    rdfs:subClassOf ns1:SewerNetworkComponent .

ns1:EmergencyResponsePlan a owl:Class ;
    rdfs:comment "A documented protocol for managing sewer-related emergencies." ;
    rdfs:subClassOf ns1:Plan .

ns1:EntryStructure rdfs:comment "A component that receives inflow, such as an inlet." ;
    rdfs:subClassOf ns1:SewerNetworkComponent .

ns1:ExitStructure rdfs:comment "A component that discharges outflow, such as an outlet." ;
    rdfs:subClassOf ns1:SewerNetworkComponent .

ns1:FloodRiskZone a owl:Class ;
    rdfs:comment "An area identified as being at risk of flooding." ;
    rdfs:subClassOf ns1:GeospatialRegion .

ns1:FlowRate a owl:Class ;
    rdfs:comment "The volumetric flow of wastewater or stormwater per unit of time." ;
    rdfs:subClassOf ns1:HydraulicProperty .

ns1:HydraulicHead a owl:Class ;
    rdfs:comment "A measure of potential energy in a flowing fluid." ;
    rdfs:subClassOf ns1:HydraulicProperty .

ns1:InfiltrationRate a owl:Class ;
    rdfs:comment "Rate at which water enters a sewer system from the ground." ;
    rdfs:subClassOf ns1:HydraulicProperty .

ns1:MechanicalStructure rdfs:comment "A mechanical component like a pumping station within the sewer network." ;
    rdfs:subClassOf ns1:SewerNetworkComponent .

ns1:OdorComplaint a owl:Class ;
    rdfs:comment "An event involving a reported odor issue from the sewer system." ;
    rdfs:subClassOf ns1:OperationalEvent .

ns1:OverflowAlarm a owl:Class ;
    rdfs:comment "Sensor device that detects overflow events." ;
    rdfs:subClassOf ns1:SensorDevice .

ns1:RainEvent a owl:Class ;
    rdfs:comment "A rainfall occurrence affecting the sewer system." ;
    rdfs:subClassOf ns1:EnvironmentalEvent .

ns1:SensorDataStream a owl:Class ;
    rdfs:comment "A data stream emitted by sensors monitoring sewer network conditions." ;
    rdfs:subClassOf ns1:InformationObject .

ns1:Watershed a owl:Class ;
    rdfs:comment "A geographic area draining to a common waterway." ;
    rdfs:subClassOf ns1:GeospatialRegion .

ns1:addresses a owl:ObjectProperty ;
    rdfs:comment "Indicates that maintenance addresses a particular structural condition." ;
    rdfs:domain ns1:MaintenanceActivity ;
    rdfs:range ns1:StructuralCondition .

ns1:affectsPerformanceOf a owl:ObjectProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "A condition might reflexively relate to itself for recursive degradation modeling.",
        "Structural condition influences performance." ;
    rdfs:domain ns1:StructuralCondition ;
    rdfs:range ns1:SewerNetworkComponent .

ns1:alignsWith a owl:ObjectProperty ;
    rdfs:comment "Relates a sewer network to the INSPIRE directive it aligns with." ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range ns1:INSPIREDirective .

ns1:classifiedBy a owl:FunctionalProperty,
        owl:ObjectProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "A region may be classified by a type that also classifies itself in abstract hierarchy.",
        "Each region is classified by one zone type for consistent planning.",
        "Relates a region to its urban classification." ;
    rdfs:domain ns1:GeospatialRegion ;
    rdfs:range ns1:UrbanZoneType .

ns1:compliesWith a owl:ObjectProperty ;
    rdfs:comment "Relates a sewer network to the RAEPA standard it complies with." ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range ns1:RAEPAStandard .

ns1:connectedTo a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:SymmetricProperty,
        owl:TransitiveProperty ;
    rdfs:comment "A component is always connected to itself by default.",
        "If component A is connected to B, and B is connected to C, then A is connected to C.",
        "If one component is connected to another, the reverse is also true.",
        "Indicates that two components are connected." ;
    rdfs:domain ns1:SewerNetworkComponent ;
    rdfs:range ns1:SewerNetworkComponent ;
    owl:SymmetricProperty true ;
    owl:inverseOf ns1:connectedTo .

ns1:detects a owl:ObjectProperty ;
    rdfs:comment "A sensor device detects a condition like corrosion." ;
    rdfs:domain ns1:SensorDevice ;
    rdfs:range ns1:StructuralCondition .

ns1:hasComponent a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:comment "Each network has unique references to its individual components (logically functional).",
        "If A has component B, and B has component C, then A has component C.",
        "Relates a sewer network to its components." ;
    rdfs:domain ns1:SewerNetwork ;
    rdfs:range ns1:SewerNetworkComponent .

ns1:hasLocation a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:comment "Defines the location of a network component.",
        "Each component has exactly one geospatial location." ;
    rdfs:domain ns1:SewerNetworkComponent ;
    rdfs:range ns1:GeospatialPoint ;
    owl:FunctionalProperty true .

ns1:hasStatus a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:comment "Each component has a single operational status at a time.",
        "The operational status of a component." ;
    rdfs:domain ns1:SewerNetworkComponent ;
    rdfs:range ns1:ComponentStatus .

ns1:hasZoneType a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:comment "Each geospatial region is classified by one urban zone type.",
        "Urban classification of a spatial region." ;
    rdfs:domain ns1:GeospatialRegion ;
    rdfs:range ns1:UrbanZoneType .

ns1:involvesComponent a owl:ObjectProperty,
        owl:ReflexiveProperty,
        owl:TransitiveProperty ;
    rdfs:comment "An event may involve a component which could be itself part of the cause.",
        "If event A involves B, and B involves C, then A involves C.",
        "Links events to involved components." ;
    rdfs:domain ns1:OperationalEvent ;
    rdfs:range ns1:SewerNetworkComponent .

ns1:monitors a owl:ObjectProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "A sensor device monitors a specific network component.",
        "A sensor system may self-monitor under redundancy or self-check schemes." ;
    rdfs:domain ns1:SensorDevice ;
    rdfs:range ns1:SewerNetworkComponent .

ns1:occursAt a owl:ObjectProperty ;
    rdfs:comment "Indicates where a blockage occurs." ;
    rdfs:domain ns1:Blockage ;
    rdfs:range ns1:Pipe .

ns1:producesFinding a owl:ObjectProperty ;
    rdfs:comment "An inspection results in one or more findings." ;
    rdfs:domain ns1:Inspection ;
    rdfs:range ns1:Finding .

ns1:requiresRepair a owl:ObjectProperty ;
    rdfs:comment "Indicates that a condition requires a specific repair." ;
    rdfs:domain ns1:StructuralCondition ;
    rdfs:range ns1:Repair .

ns1:restores a owl:ObjectProperty ;
    rdfs:comment "Relates a repair to the condition it restores." ;
    rdfs:domain ns1:Repair ;
    rdfs:range ns1:StructuralCondition .

ns1:resultsFrom a owl:ObjectProperty ;
    rdfs:comment "Indicates the blockage that resulted in an overflow." ;
    rdfs:domain ns1:Overflow ;
    rdfs:range ns1:Blockage .

ns1:scheduledBy a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:comment "Connects maintenance to its originating plan.",
        "Each maintenance activity is scheduled by only one plan." ;
    rdfs:domain ns1:MaintenanceActivity ;
    rdfs:range ns1:MaintenancePlan .

ns1:targets a owl:ObjectProperty ;
    rdfs:comment "Relates an inspection to the component being inspected." ;
    rdfs:domain ns1:Inspection ;
    rdfs:range ns1:SewerNetworkComponent .

ns1:DischargePermit a owl:Class ;
    rdfs:comment "A legal document specifying allowable discharge limits." ;
    rdfs:subClassOf ns1:RegulatoryStandard .

ns1:GeospatialData a owl:Class ;
    rdfs:comment "Data defining the spatial properties of components." .

ns1:Inlet a owl:Class ;
    rdfs:comment "An entry point for stormwater into the network." ;
    rdfs:subClassOf ns1:EntryStructure,
        ns1:SewerNetworkComponent .

ns1:MaintenancePlan a owl:Class ;
    rdfs:comment "A structured plan for performing maintenance." ;
    rdfs:subClassOf ns1:Plan .

ns1:Outlet a owl:Class ;
    rdfs:comment "A discharge point for wastewater from the network." ;
    rdfs:subClassOf ns1:ExitStructure,
        ns1:SewerNetworkComponent .

ns1:Plan a owl:Class ;
    rdfs:comment "A high-level document or strategy outlining planned activities.",
        "An informational entity capturing maintenance or inspection planning." ;
    rdfs:subClassOf ns1:InformationObject .

ns1:PumpingStation a owl:Class ;
    rdfs:comment "A station used to elevate and move wastewater." ;
    rdfs:subClassOf ns1:MechanicalStructure,
        ns1:SewerNetworkComponent .

ns1:Finding a owl:Class ;
    rdfs:comment "An observation made during inspection.",
        "An observation recorded during an inspection, used to evaluate the condition of infrastructure." ;
    rdfs:subClassOf ns1:InformationObject .

ns1:INSPIREDirective a ns1:INSPIREDirective,
        owl:Class ;
    rdfs:comment "A European Union directive for spatial data infrastructure known as INSPIRE.",
        "A European directive for spatial data infrastructure." .

ns1:Manhole a owl:Class ;
    ns1:hasLocation ns1:GeospatialPoint ;
    rdfs:comment "An access point to underground sewer systems." ;
    rdfs:subClassOf ns1:AccessStructure,
        ns1:SewerNetworkComponent .

ns1:RAEPAStandard a ns1:RAEPAStandard,
        owl:Class ;
    rdfs:comment "A regulatory or technical standard published by RAEPA for sewer infrastructure.",
        "A standard defining requirements for sanitation networks in France." .

ns1:SewerNetwork a owl:Class ;
    ns1:alignsWith ns1:INSPIREDirective ;
    ns1:compliesWith ns1:RAEPAStandard ;
    ns1:hasComponent ns1:Pipe ;
    rdfs:comment "A sewer system managing wastewater and stormwater." .

ns1:ComponentStatus a owl:Class ;
    rdfs:comment "Represents the operational condition of a sewer infrastructure component.",
        "The status classification of a component (e.g., operational, failed)." ;
    rdfs:subClassOf ns1:InformationObject .

ns1:Blockage a owl:Class ;
    ns1:occursAt ns1:Pipe ;
    rdfs:comment "An event where flow is impeded due to obstruction." ;
    rdfs:subClassOf ns1:OperationalEvent .

ns1:Inspection a owl:Class ;
    ns1:targets ns1:SewerNetworkComponent ;
    rdfs:comment "An event where sewer components are assessed." ;
    rdfs:subClassOf ns1:OperationalEvent .

ns1:Pipe a owl:Class ;
    ns1:connectedTo ns1:Manhole ;
    rdfs:comment "A linear conduit that conveys wastewater or stormwater." ;
    rdfs:subClassOf ns1:Conduit,
        ns1:SewerNetworkComponent .

ns1:Repair a owl:Class ;
    ns1:restores ns1:StructuralCondition ;
    rdfs:comment "A maintenance activity aimed at restoring structural condition." ;
    rdfs:subClassOf ns1:MaintenanceActivity .

ns1:UrbanZoneType a owl:Class ;
    rdfs:comment "A classification of urban land (e.g., residential, industrial) used for infrastructure planning.",
        "The classification of urban areas for infrastructure management." ;
    rdfs:subClassOf ns1:InformationObject .

ns1:Overflow a owl:Class ;
    ns1:resultsFrom ns1:Blockage ;
    rdfs:comment "An event where flow exceeds capacity and exits the system." ;
    rdfs:subClassOf ns1:OperationalEvent .

ns1:GeospatialPoint a owl:Class ;
    rdfs:comment "A specific geographic point using coordinates." ;
    rdfs:subClassOf ns1:GeospatialData .

ns1:HydraulicProperty a owl:Class ;
    rdfs:comment "A measurable property like flow or diameter that affects hydraulic behavior." .

ns1:GeospatialRegion a owl:Class ;
    rdfs:comment "A defined spatial area containing network components." ;
    rdfs:subClassOf ns1:GeospatialData .

ns1:MaintenanceActivity a owl:Class ;
    ns1:addresses ns1:StructuralCondition ;
    rdfs:comment "An action aimed at maintaining or repairing sewer components." ;
    rdfs:subClassOf ns1:OperationalEvent .

ns1:OperationalEvent a owl:Class ;
    rdfs:comment "An event that occurs in time affecting sewer operations." ;
    rdfs:subClassOf <http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#Perdurant> .

ns1:StructuralCondition a owl:Class ;
    rdfs:comment "A property describing physical wear or integrity of a component." .

ns1:SensorDevice a owl:Class ;
    rdfs:comment "A device installed in the sewer network to monitor conditions like flow, blockages, or structural issues.",
        "A device that monitors structural or hydraulic behavior." ;
    rdfs:subClassOf ns1:SewerNetworkComponent .

ns1:SewerNetworkComponent a owl:Class ;
    rdfs:comment "Abstract class for physical infrastructure elements in a sewer network." .

