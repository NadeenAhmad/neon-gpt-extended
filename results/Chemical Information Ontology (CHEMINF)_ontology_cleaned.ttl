    rdfs:label "is Represented By"@en ;
    rdfs:comment "Relates a chemical graph representation to the chemical entity it represents."@en ;
    owl:inverseOf ns1:hasRepresentation .

ns1:isUsedBy a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "is Used By"@en ;
    rdfs:comment "Relates an algorithm to the software implementation that uses it."@en ;
    owl:inverseOf ns1:usesAlgorithm .

ns1:represents a owl:ObjectProperty ;
    rdfs:label "represents"@en ;
    rdfs:comment "Relates a chemical graph to a chemical entity."@en,
