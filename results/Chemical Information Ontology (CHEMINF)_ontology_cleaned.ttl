    rdfs:range xsd:string .

ns1:stringValue a owl:DatatypeProperty ;
    rdfs:label "string Value"@en ;
    rdfs:comment "The string value of a descriptor or property."@en ;
    rdfs:range xsd:string .

ns1:value a owl:DatatypeProperty ;
    rdfs:label "value"@en ;
    rdfs:comment "The value of a descriptor or property."@en ;
    rdfs:range xsd:float .

ns1:version a owl:DatatypeProperty ;
    rdfs:label "version"@en ;
    rdfs:comment "The version of the software implementation."@en ;
