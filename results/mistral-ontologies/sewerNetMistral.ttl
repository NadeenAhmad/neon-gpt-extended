## Object Properties

:hasComponent rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a sewer network to its components." ;
    rdfs:domain :SewerNetwork ;
    rdfs:range :NetworkComponent .

:connectedTo rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a network component to another component it is connected to." ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :NetworkComponent .

:hasLocation rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a network component to its geospatial location." ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :GeospatialData .

:hasHydraulicProperty rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a network component to its hydraulic properties." ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :HydraulicProperty .

:hasStructuralCondition rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a network component to its structural condition." ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :StructuralCondition .

:targets rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates an inspection to the component it targets." ;
    rdfs:domain :Inspection ;
    rdfs:range :NetworkComponent .

:addresses rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a maintenance activity to the structural condition it addresses." ;
    rdfs:domain :MaintenanceActivity ;
    rdfs:range :StructuralCondition .

:occursAt rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a blockage to the component it occurs at." ;
    rdfs:domain :Blockage ;
    rdfs:range :Pipe .

:resultsFrom rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates an overflow to the blockage it results from." ;
    rdfs:domain :Overflow ;
    rdfs:range :Blockage .

:restores rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a repair to the structural condition it restores." ;
    rdfs:domain :Repair ;
    rdfs:range :StructuralCondition .

:compliesWith rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a sewer network to the RAEPA standard it complies with." ;
    rdfs:domain :SewerNetwork ;
    rdfs:range :RAEPACompliance .

:alignsWith rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a sewer network to the INSPIRE directive it aligns with." ;
    rdfs:domain :SewerNetwork ;
    rdfs:range :INSPIRECompliance .

:usesFoundationalOntology rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a sewer network to the foundational ontology it uses." ;
    rdfs:domain :SewerNetwork ;
    rdfs:range :FoundationalOntology .

:hasMaintenanceHistory rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a pumping station to its maintenance history." ;
    rdfs:domain :PumpingStation ;
    rdfs:range :MaintenanceActivity .

:requiresUpgradeToComplyWith rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a network component to the RAEPA standard it requires an upgrade to comply with." ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :RAEPACompliance .

:hasMaintenanceSchedule rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a network component to its maintenance schedule." ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :MaintenanceSchedule .

:hasInspectionSchedule rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a network component to its inspection schedule." ;
    rdfs:domain :NetworkComponent ;
    rdfs:range :InspectionSchedule .

:isPartOf rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates an inspection to the maintenance activity it is part of." ;
    rdfs:domain :Inspection ;
    rdfs:range :MaintenanceActivity .

:includes rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a maintenance activity to the repair it includes." ;
    rdfs:domain :MaintenanceActivity ;
    rdfs:range :Repair .

:causes rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a blockage to the overflow it causes." ;
    rdfs:domain :Blockage ;
    rdfs:range :Overflow .

:impacts rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates an overflow to the structural condition it impacts." ;
    rdfs:domain :Overflow ;
    rdfs:range :StructuralCondition .

:mitigates rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a repair to the blockage it mitigates." ;
    rdfs:domain :Repair ;
    rdfs:range :Blockage .

:affects rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a hydraulic property to the structural condition it affects." ;
    rdfs:domain :HydraulicProperty ;
    rdfs:range :StructuralCondition .

:influences rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates a structural condition to the hydraulic property it influences." ;
    rdfs:domain :StructuralCondition ;
    rdfs:range :HydraulicProperty .

:defines rdf:type owl:ObjectProperty ;
    rdfs:comment "Relates geospatial data to the administrative region it defines." ;
    rdfs:domain :GeospatialData ;
