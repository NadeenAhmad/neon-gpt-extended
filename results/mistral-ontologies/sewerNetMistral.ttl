### Equivalence Axioms
:SewerNetwork rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:WastewaterNetwork :StormwaterNetwork)
    ] .

:Pipe rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:MainPipe :LateralPipe :ServicePipe :DrainagePipe)
    ] .

:Manhole rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:AccessManhole :JunctionManhole :InspectionManhole :MaintenanceManhole)
    ] .

:Inlet rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:StormDrain :CatchBasin :RoofDrain :GroundInlet)
    ] .

:Outlet rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:DischargePoint :OverflowPoint :TreatmentPlantOutlet :StormwaterOutlet)
    ] .

:PumpingStation rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:PrimaryPumpingStation :SecondaryPumpingStation :EmergencyPumpingStation :BackupPumpingStation)
    ] .

:Inspection rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:RoutineInspection :EmergencyInspection :ComplianceInspection :PeriodicInspection)
    ] .

:MaintenanceActivity rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:PreventiveMaintenance :CorrectiveMaintenance :PredictiveMaintenance :EmergencyMaintenance)
    ] .

:Blockage rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:PartialBlockage :CompleteBlockage :RecurringBlockage :SevereBlockage)
    ] .

:Overflow rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:SanitarySewerOverflow :CombinedSewerOverflow :StormwaterOverflow :GroundwaterOverflow)
    ] .

:Repair rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:MinorRepair :MajorRepair :EmergencyRepair :TemporaryRepair)
    ] .

