### Equivalence Axioms
:SewerNetwork rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:WastewaterNetwork :StormwaterNetwork)
    ] .

:Pipe rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:MainPipe :LateralPipe :ServicePipe)
    ] .

:Manhole rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:AccessManhole :JunctionManhole :InspectionManhole)
    ] .

:Inlet rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:StormDrain :CatchBasin :RoofDrain)
    ] .

:Outlet rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:DischargePoint :OverflowPoint :TreatmentPlantOutlet)
    ] .

:PumpingStation rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:PrimaryPumpingStation :SecondaryPumpingStation :EmergencyPumpingStation)
    ] .

:Inspection rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:RoutineInspection :EmergencyInspection :ComplianceInspection)
    ] .

:MaintenanceActivity rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:PreventiveMaintenance :CorrectiveMaintenance :PredictiveMaintenance)
    ] .

:Blockage rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:PartialBlockage :CompleteBlockage :RecurringBlockage)
    ] .

:Overflow rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:SanitarySewerOverflow :CombinedSewerOverflow :StormwaterOverflow)
    ] .

:Repair rdf:type owl:Class ;
    owl:equivalentClass [
        owl:unionOf (:MinorRepair :MajorRepair :EmergencyRepair)
    ] .

