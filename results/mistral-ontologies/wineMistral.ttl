# Ontology Metadata
@prefix : <http://example.org/wine#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

<http://example.org/wine> rdf:type owl:Ontology ;
                          rdfs:label "Wine Ontology" ;
                          dc:description "An ontology describing the domain of wine, including its classification, characteristics, production methods, regions, producers, and more." ;
                          owl:versionInfo "1.0" .


# Maceration Processes and Wine Types
:MacerationProcess rdfs:subClassOf :Process .
:TotalMaceration rdfs:subClassOf :MacerationProcess .
:HalfMaceration rdfs:subClassOf :MacerationProcess .
:WithoutMaceration rdfs:subClassOf :MacerationProcess .

:RedWine rdfs:subClassOf :Wine .
:RedWine :usedToProduce :TotalMaceration .
:RedWine :hasVariety :RedVariety .
:RedWine :hasVariety :WhiteRedVarietyCombination .

:RoseWine rdfs:subClassOf :Wine .
:RoseWine :usedToProduce :HalfMaceration .
:RoseWine :hasVariety :RedVariety .
:RoseWine :hasVariety :WhiteRedVarietyCombination .

:WhiteWine rdfs:subClassOf :Wine .
:WhiteWine :usedToProduce :WithoutMaceration .
:WhiteWine :hasVariety :RedVariety .
:WhiteWine :hasVariety :WhiteVariety .

# Grape Maturity States
:GrapeMaturityState rdfs:subClassOf :State .
:Unripe rdfs:subClassOf :GrapeMaturityState .
:Mature rdfs:subClassOf :GrapeMaturityState .
:Overmature rdfs:subClassOf :GrapeMaturityState .

:Overmature :achievedBy :DehydratedGrapes .
:Overmature :achievedBy :FrozenGrapes .
:Overmature :achievedBy :NobleRot .

:GrapeMaturityState :affects :WineCharacteristic .

# Fermentation Processes and Wine Types
:FermentationProcess rdfs:subClassOf :Process .
:NormalFermentation rdfs:subClassOf :FermentationProcess .
:DoubleFermentation rdfs:subClassOf :FermentationProcess .
:InterruptedFermentation rdfs:subClassOf :FermentationProcess .

:TableWine rdfs:subClassOf :Wine .
:TableWine :producedBy :NormalFermentation .

:SparklingWine rdfs:subClassOf :Wine .
:SparklingWine :producedBy :DoubleFermentation .
:SparklingWine :distinguishedBy :Bubbles .

:SweetWine rdfs:subClassOf :Wine .
:SweetWine :producedBy :InterruptedFermentation .
:SweetWine :interruptedBy :Fortification .
:SweetWine :interruptedBy :SulphurousAddition .
:SweetWine :interruptedBy :Filtration .

# Wine Characteristics
:WineCharacteristic rdfs:subClassOf :Characteristic .
:AlcoholPercentage rdfs:subClassOf :WineCharacteristic .
:TanninLevel rdfs:subClassOf :WineCharacteristic .
:DegreeOfAcidity rdfs:subClassOf :WineCharacteristic .

:AmbiguousCharacteristic rdfs:subClassOf :Characteristic .
:Vision rdfs:subClassOf :AmbiguousCharacteristic .
:Aroma rdfs:subClassOf :AmbiguousCharacteristic .
:Flavour rdfs:subClassOf :AmbiguousCharacteristic .

:Vision :categorizedBy :Aspect .
:Vision :categorizedBy :Colour .
:Vision :categorizedBy :Intensity .
:Vision :categorizedBy :Foam .
:Vision :categorizedBy :Bubble .

:Aroma :categorizedBy :Condition .
:Aroma :categorizedBy :Intensity .
:Aroma :categorizedBy :Evolution .
:Aroma :categorizedBy :DominantCharacter .

:Flavour :categorizedBy :Sweetness .
:Flavour :categorizedBy :Acidity .
:Flavour :categorizedBy :Tannin .
:Flavour :categorizedBy :Structure .
:Flavour :categorizedBy :Intensity .
:Flavour :categorizedBy :Persistence .

:WineCharacteristic :contributesTo :WineEvaluation .

# Regional and Classification Systems
:ClassificationSystem rdfs:subClassOf :System .
:EuropeanClassification rdfs:subClassOf :ClassificationSystem .
:DOC rdfs:subClassOf :EuropeanClassification .
:VQPRD rdfs:subClassOf :EuropeanClassification .
:RegionalWine rdfs:subClassOf :EuropeanClassification .
:TableWine rdfs:subClassOf :EuropeanClassification .

:Region rdfs:subClassOf :GeographicalEntity .
:Region :partOf :Country .
:Region :influences :WineCharacteristic .

:Wine :producedIn :Region .

# Producer, Varieties, and Year of Production
:Producer rdfs:subClassOf :Entity .
:Wine :producedBy :Producer .
:Producer :hasReputation :Reputation .

:GrapeVariety rdfs:subClassOf :Variety .
:RedVariety rdfs:subClassOf :GrapeVariety .
:WhiteVariety rdfs:subClassOf :GrapeVariety .
:WhiteRedVarietyCombination rdfs:subClassOf :GrapeVariety .

:GrapeVariety :contributesTo :WineQuality .
:Wine :hasVariety :GrapeVariety .

:YearOfProduction rdfs:subClassOf :TimeEntity .
:Wine :producedInYear :YearOfProduction .
:YearOfProduction :hasCharacteristic :WineCharacteristic .

# Specific Wine Types and Production Methods
:PortWine rdfs:subClassOf :SweetWine .
:PortWine :producedBy :FortifiedWine .
:PortWine :hasVariety :TintaRoriz .
:PortWine :hasVariety :TourigaNacional .
:PortWine :hasVariety :TourigaFrancesa .

:BarcaVelha rdfs:subClassOf :Wine .
:BarcaVelha :producedInYear :YearOfProduction .
:BarcaVelha :hasSignificance :Significance .

# Query and Retrieval
:Wine :hasCharacteristic :AlcoholPercentage .
:Wine :hasCharacteristic :TanninLevel .
:Wine :producedIn :Region .
:Wine :producedBy :Producer .
:Wine :producedInYear :YearOfProduction .

# Interoperability and Knowledge Management
:WineOntology :facilitatesInteroperabilityWith :WineDatabase .
:WineOntology :facilitatesInteroperabilityWith :System .
:WineOntology :manages :WineKnowledge .
:WineOntology :retrieves :WineKnowledge .
:WineOntology :supportsIntegrationOf :Wine .
:WineOntology :supportsIntegrationOf :Region .
:WineOntology :supportsIntegrationOf :ClassificationSystem .

# User-Specific Queries
:Wine :recommendedBasedOn :UserPreference .
:UserPreference :hasPreference :Taste .
:UserPreference :hasPreference :Color .
:UserPreference :hasPreference :Smell .
