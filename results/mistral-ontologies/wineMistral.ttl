           rdfs:range :WineReview ;
           rdfs:comment "Relates a wine to its reviews." ;
           owl:inverseOf :isReviewOf .

:isReviewOf rdf:type owl:ObjectProperty ;
            rdfs:domain :WineReview ;
            rdfs:range :Wine ;
            rdfs:comment "Relates a review to the wine it evaluates." .
