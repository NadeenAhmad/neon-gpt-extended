# New Concepts and Relationships

# Wine Styles
:WineStyle rdfs:subClassOf :Wine ;
           rdfs:comment "A specific style of wine, such as dry, sweet, or sparkling." .

:DryWine rdfs:subClassOf :WineStyle ;
         rdfs:comment "A style of wine with low residual sugar, resulting in a dry taste." .

:SweetWine rdfs:subClassOf :WineStyle ;
           rdfs:comment "A style of wine with high residual sugar, resulting in a sweet taste." .

:SparklingWine rdfs:subClassOf :WineStyle ;
               rdfs:comment "A style of wine with carbonation, resulting in bubbles." .

# Wine Aging
:AgingProcess rdfs:subClassOf :Process ;
              rdfs:comment "A process by which wine is aged to develop its flavors and characteristics." .

:BarrelAging rdfs:subClassOf :AgingProcess ;
             rdfs:comment "Aging wine in barrels to impart additional flavors and complexity." .

:BottleAging rdfs:subClassOf :AgingProcess ;
             rdfs:comment "Aging wine in bottles to allow it to develop over time." .

:agedIn rdf:type owl:ObjectProperty ;
        rdfs:domain :Wine ;
        rdfs:range :AgingProcess ;
        rdfs:comment "Relates a wine to the aging process it undergoes." ;
        owl:inverseOf :agesWine .

:agesWine rdf:type owl:ObjectProperty ;
          rdfs:domain :AgingProcess ;
          rdfs:range :Wine ;
          rdfs:comment "Relates an aging process to the wine it ages." .

# Wine Awards
:WineAward rdfs:subClassOf :Award ;
            rdfs:comment "An award given to a wine for its quality or characteristics." .

:GoldMedal rdfs:subClassOf :WineAward ;
            rdfs:comment "A gold medal awarded to a wine for its high quality." .

:SilverMedal rdfs:subClassOf :WineAward ;
             rdfs:comment "A silver medal awarded to a wine for its quality." .

:BronzeMedal rdfs:subClassOf :WineAward ;
             rdfs:comment "A bronze medal awarded to a wine for its quality." .

:hasAward rdf:type owl:ObjectProperty ;
          rdfs:domain :Wine ;
          rdfs:range :WineAward ;
          rdfs:comment "Relates a wine to the awards it has received." ;
          owl:inverseOf :isAwardedTo .

:isAwardedTo rdf:type owl:ObjectProperty ;
             rdfs:domain :WineAward ;
             rdfs:range :Wine ;
             rdfs:comment "Relates an award to the wine it is awarded to." .

# Wine Tasting Notes
:TastingNote rdfs:subClassOf :Note ;
             rdfs:comment "A note describing the sensory characteristics of a wine during tasting." .

:AromaNote rdfs:subClassOf :TastingNote ;
            rdfs:comment "A note describing the aroma characteristics of a wine." .

:FlavorNote rdfs:subClassOf :TastingNote ;
            rdfs:comment "A note describing the flavor characteristics of a wine." .

:TextureNote rdfs:subClassOf :TastingNote ;
             rdfs:comment "A note describing the texture characteristics of a wine." .

:hasTastingNote rdf:type owl:ObjectProperty ;
               rdfs:domain :Wine ;
               rdfs:range :TastingNote ;
               rdfs:comment "Relates a wine to its tasting notes." ;
               owl:inverseOf :isTastingNoteOf .

:isTastingNoteOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :TastingNote ;
                 rdfs:range :Wine ;
                 rdfs:comment "Relates a tasting note to the wine it describes." .

# Wine and Food Pairing
:FoodPairing rdfs:subClassOf :Pairing ;
             rdfs:comment "A recommendation for pairing a wine with a specific type of food." .

:MeatPairing rdfs:subClassOf :FoodPairing ;
             rdfs:comment "A recommendation for pairing a wine with meat dishes." .

:SeafoodPairing rdfs:subClassOf :FoodPairing ;
               rdfs:comment "A recommendation for pairing a wine with seafood dishes." .

:VegetablePairing rdfs:subClassOf :FoodPairing ;
                 rdfs:comment "A recommendation for pairing a wine with vegetable dishes." .

:hasFoodPairing rdf:type owl:ObjectProperty ;
               rdfs:domain :Wine ;
               rdfs:range :FoodPairing ;
               rdfs:comment "Relates a wine to its recommended food pairings." ;
               owl:inverseOf :isFoodPairingOf .

:isFoodPairingOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :FoodPairing ;
                 rdfs:range :Wine ;
                 rdfs:comment "Relates a food pairing to the wine it is recommended for." .

# Wine Vintage
:Vintage rdfs:subClassOf :Year ;
         rdfs:comment "A specific year in which a wine is produced, often indicating its quality and characteristics." .

:hasVintage rdf:type owl:ObjectProperty ;
            rdfs:domain :Wine ;
            rdfs:range :Vintage ;
            rdfs:comment "Relates a wine to its vintage year." ;
            owl:inverseOf :isVintageOf .

:isVintageOf rdf:type owl:ObjectProperty ;
             rdfs:domain :Vintage ;
             rdfs:range :Wine ;
             rdfs:comment "Relates a vintage year to the wine produced in that year." .

# Wine Reviews
:WineReview rdfs:subClassOf :Review ;
            rdfs:comment "A review of a wine, providing an evaluation of its quality and characteristics." .

:ExpertReview rdfs:subClassOf :WineReview ;
              rdfs:comment "A review of a wine by an expert, providing a professional evaluation." .

:UserReview rdfs:subClassOf :WineReview ;
            rdfs:comment "A review of a wine by a user, providing a personal evaluation." .

