# Inverse Properties and Domain/Range Restrictions

# Maceration Processes and Wine Types
:usedToProduce rdfs:domain :Wine ;
               rdfs:range :MacerationProcess ;
               owl:inverseOf :producesWine .

:hasVariety rdfs:domain :Wine ;
            rdfs:range :GrapeVariety ;
            owl:inverseOf :isVarietyOf .

:producesWine rdfs:domain :MacerationProcess ;
              rdfs:range :Wine .

:isVarietyOf rdfs:domain :GrapeVariety ;
             rdfs:range :Wine .

# Grape Maturity States
:achievedBy rdfs:domain :Overmature ;
            rdfs:range :GrapeState ;
            owl:inverseOf :achieves .

:affects rdfs:domain :GrapeMaturityState ;
         rdfs:range :WineCharacteristic ;
         owl:inverseOf :isAffectedBy .

:achieves rdfs:domain :GrapeState ;
          rdfs:range :Overmature .

:isAffectedBy rdfs:domain :WineCharacteristic ;
              rdfs:range :GrapeMaturityState .

# Fermentation Processes and Wine Types
:producedBy rdfs:domain :Wine ;
            rdfs:range :FermentationProcess ;
            owl:inverseOf :produces .

:distinguishedBy rdfs:domain :SparklingWine ;
                 rdfs:range :Characteristic ;
                 owl:inverseOf :distinguishes .

:interruptedBy rdfs:domain :SweetWine ;
               rdfs:range :InterruptionMethod ;
               owl:inverseOf :interrupts .

:produces rdfs:domain :FermentationProcess ;
          rdfs:range :Wine .

:distinguishes rdfs:domain :Characteristic ;
               rdfs:range :SparklingWine .

:interrupts rdfs:domain :InterruptionMethod ;
            rdfs:range :SweetWine .

# Wine Characteristics
:categorizedBy rdfs:domain :AmbiguousCharacteristic ;
               rdfs:range :CharacteristicCategory ;
               owl:inverseOf :categorizes .

:contributesTo rdfs:domain :WineCharacteristic ;
               rdfs:range :WineEvaluation ;
               owl:inverseOf :isContributedBy .

:categorizes rdfs:domain :CharacteristicCategory ;
             rdfs:range :AmbiguousCharacteristic .

:isContributedBy rdfs:domain :WineEvaluation ;
                 rdfs:range :WineCharacteristic .

# Regional and Classification Systems
:partOf rdfs:domain :Region ;
        rdfs:range :Country ;
        owl:inverseOf :hasPart .

:influences rdfs:domain :Region ;
            rdfs:range :WineCharacteristic ;
            owl:inverseOf :isInfluencedBy .

:hasPart rdfs:domain :Country ;
         rdfs:range :Region .

:isInfluencedBy rdfs:domain :WineCharacteristic ;
                rdfs:range :Region .

# Producer, Varieties, and Year of Production
:hasReputation rdfs:domain :Producer ;
               rdfs:range :Reputation ;
               owl:inverseOf :isReputationOf .

:contributesTo rdfs:domain :GrapeVariety ;
               rdfs:range :WineQuality ;
               owl:inverseOf :isContributedByVariety .

:producedInYear rdfs:domain :Wine ;
                rdfs:range :YearOfProduction ;
                owl:inverseOf :producesWineInYear .

:isReputationOf rdfs:domain :Reputation ;
                rdfs:range :Producer .

:isContributedByVariety rdfs:domain :WineQuality ;
                        rdfs:range :GrapeVariety .

:producesWineInYear rdfs:domain :YearOfProduction ;
                    rdfs:range :Wine .

# Specific Wine Types and Production Methods
:hasSignificance rdfs:domain :BarcaVelha ;
                 rdfs:range :Significance ;
                 owl:inverseOf :isSignificanceOf .

:isSignificanceOf rdfs:domain :Significance ;
                  rdfs:range :BarcaVelha .

# Query and Retrieval
:hasCharacteristic rdfs:domain :Wine ;
                   rdfs:range :WineCharacteristic ;
                   owl:inverseOf :isCharacteristicOf .

:isCharacteristicOf rdfs:domain :WineCharacteristic ;
                    rdfs:range :Wine .

# Interoperability and Knowledge Management
:facilitatesInteroperabilityWith rdfs:domain :WineOntology ;
                                 rdfs:range :Entity ;
                                 owl:inverseOf :interoperatesWith .

:manages rdfs:domain :WineOntology ;
         rdfs:range :WineKnowledge ;
         owl:inverseOf :isManagedBy .

:retrieves rdfs:domain :WineOntology ;
           rdfs:range :WineKnowledge ;
           owl:inverseOf :isRetrievedBy .

:supportsIntegrationOf rdfs:domain :WineOntology ;
                       rdfs:range :Entity ;
                       owl:inverseOf :isIntegratedBy .

:interoperatesWith rdfs:domain :Entity ;
                   rdfs:range :WineOntology .

:isManagedBy rdfs:domain :WineKnowledge ;
             rdfs:range :WineOntology .

:isRetrievedBy rdfs:domain :WineKnowledge ;
               rdfs:range :WineOntology .

:isIntegratedBy rdfs:domain :Entity ;
                rdfs:range :WineOntology .

# User-Specific Queries
:recommendedBasedOn rdfs:domain :Wine ;
                    rdfs:range :UserPreference ;
                    owl:inverseOf :recommendsWine .

:hasPreference rdfs:domain :UserPreference ;
               rdfs:range :PreferenceCategory ;
               owl:inverseOf :isPreferenceOf .

:recommendsWine rdfs:domain :UserPreference ;
                rdfs:range :Wine .

:isPreferenceOf rdfs:domain :PreferenceCategory ;
                rdfs:range :UserPreference .
