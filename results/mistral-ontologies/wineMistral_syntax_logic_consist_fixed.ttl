@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ns1: <http://example.org/wine#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/wine> a owl:Ontology ;
    rdfs:label "Wine Ontology" ;
    dc:description "An ontology describing the domain of wine, including its classification, characteristics, production methods, regions, producers, and more." ;
    owl:versionInfo "1.0" .

ns1:AcidicFlavorNote rdfs:subClassOf ns1:FlavorNote .

ns1:AmateurExpertReview rdfs:subClassOf ns1:ExpertReview .

ns1:BoutiqueProducer rdfs:comment "A producer known for high-quality, limited-production wines." ;
    rdfs:subClassOf ns1:Producer .

ns1:CasualUserReview rdfs:subClassOf ns1:UserReview .

ns1:ChardonnayGrape a ns1:WhiteVariety ;
    rdfs:comment "A white grape variety known for its versatility in producing both crisp and full-bodied wines." .

ns1:ChateauMargaux a ns1:Producer ;
    ns1:hasReputationScore 95 ;
    ns1:producedBy ns1:BordeauxWine ;
    rdfs:comment "A renowned wine producer in the Bordeaux region, known for its high-quality red wines." .

ns1:CoolClimateRegion rdfs:comment "A wine region with a cool climate, producing wines with high acidity." ;
    rdfs:subClassOf ns1:Region .

ns1:CrispTextureNote rdfs:subClassOf ns1:TextureNote .

ns1:DOCClassification a ns1:DOC ;
    rdfs:comment "The highest quality classification for wines in the European Union." .

ns1:DeepColor a ns1:Vision ;
    ns1:categorizedBy ns1:ColorIntensity ;
    rdfs:comment "A deep and intense color in a wine, indicating high concentration and extraction." .

ns1:DomaineVacheron a ns1:Producer ;
    ns1:hasReputationScore 90 ;
    ns1:producedBy ns1:SancerreWine ;
    rdfs:comment "A wine producer in the Sancerre region, known for its crisp and refreshing white wines." .

ns1:DoubleFermentationProcess a ns1:DoubleFermentation ;
    ns1:produces ns1:ChampagneWine ;
    rdfs:comment "A fermentation process involving two stages, resulting in sparkling wines." .

ns1:DryRedWine rdfs:subClassOf ns1:DryWine,
        ns1:RedWine .

ns1:DryWhiteWine rdfs:subClassOf ns1:DryWine,
        ns1:WhiteWine .

ns1:EnthusiastUserReview rdfs:subClassOf ns1:UserReview .

ns1:FishPairing rdfs:subClassOf ns1:SeafoodPairing .

ns1:FloralAromaNote rdfs:subClassOf ns1:AromaNote .

ns1:FruityAroma a ns1:Aroma ;
    ns1:categorizedBy ns1:FruitCategory ;
    rdfs:comment "An aroma characterized by fruity notes, such as berries or citrus." .

ns1:FruityAromaNote rdfs:subClassOf ns1:AromaNote .

ns1:FullFlavour a ns1:Flavour ;
    ns1:categorizedBy ns1:FlavourIntensity ;
    rdfs:comment "A full and rich flavor in a wine, indicating high intensity and complexity." .

ns1:HighAcidGrapeVariety rdfs:comment "A grape variety known for high acidity." ;
    rdfs:subClassOf ns1:GrapeVariety .

ns1:HighAcidityWine rdfs:comment "A wine with high acidity levels." ;
    rdfs:subClassOf ns1:Wine .

ns1:HighAlcoholWine rdfs:comment "A wine with high alcohol content." ;
    rdfs:subClassOf ns1:Wine .

ns1:HighSugarGrapeVariety rdfs:comment "A grape variety known for high sugar content." ;
    rdfs:subClassOf ns1:GrapeVariety .

ns1:HighTanninWine rdfs:comment "A wine with high tannin levels." ;
    rdfs:subClassOf ns1:Wine .

ns1:HotClimateRegion rdfs:comment "A wine region with a hot climate, producing full-bodied wines." ;
    rdfs:subClassOf ns1:Region .

ns1:InternationalBronzeMedal rdfs:subClassOf ns1:BronzeMedal .

ns1:InternationalGoldMedal rdfs:subClassOf ns1:GoldMedal .

ns1:InternationalSilverMedal rdfs:subClassOf ns1:SilverMedal .

ns1:InterruptedFermentationProcess a ns1:InterruptedFermentation ;
    ns1:interrupts ns1:PortWineExample ;
    rdfs:comment "A fermentation process that is stopped before completion, resulting in sweet wines." .

ns1:LargeScaleProducer rdfs:comment "A producer known for large-scale wine production." ;
    rdfs:subClassOf ns1:Producer .

ns1:LeafyGreenPairing rdfs:subClassOf ns1:VegetablePairing .

ns1:LongTermBottleAging rdfs:subClassOf ns1:BottleAging .

ns1:LowAcidGrapeVariety rdfs:comment "A grape variety known for low acidity." ;
    rdfs:subClassOf ns1:GrapeVariety .

ns1:LowAcidityWine rdfs:comment "A wine with low acidity levels." ;
    rdfs:subClassOf ns1:Wine .

ns1:LowAlcoholWine rdfs:comment "A wine with low alcohol content." ;
    rdfs:subClassOf ns1:Wine .

ns1:LowSugarGrapeVariety rdfs:comment "A grape variety known for low sugar content." ;
    rdfs:subClassOf ns1:GrapeVariety .

ns1:LowTanninWine rdfs:comment "A wine with low tannin levels." ;
    rdfs:subClassOf ns1:Wine .

ns1:MoetChandon a ns1:Producer ;
    ns1:hasReputationScore 93 ;
    ns1:producedBy ns1:ChampagneWine ;
    rdfs:comment "A wine producer in the Champagne region, known for its high-quality sparkling wines." .

ns1:NapaValleyRegion a ns1:Region ;
    ns1:partOf ns1:USA ;
    rdfs:comment "A wine region in California, USA, known for its high-quality Cabernet Sauvignon and Chardonnay wines." .

ns1:NationalBronzeMedal rdfs:subClassOf ns1:BronzeMedal .

ns1:NationalGoldMedal rdfs:subClassOf ns1:GoldMedal .

ns1:NationalSilverMedal rdfs:subClassOf ns1:SilverMedal .

ns1:Niepoort a ns1:Producer ;
    ns1:hasReputationScore 97 ;
    ns1:producedBy ns1:BarcaVelhaWine ;
    rdfs:comment "A wine producer in the Alentejo region, known for its high-quality red wines." .

ns1:NormalFermentationProcess a ns1:NormalFermentation ;
    ns1:produces ns1:BordeauxWine ;
    rdfs:comment "A standard fermentation process resulting in table wines." .

ns1:OakBarrelAging rdfs:subClassOf ns1:BarrelAging .

ns1:PinotNoirGrape a ns1:RedVariety ;
    rdfs:comment "A red grape variety known for its light-bodied wines with delicate flavors and aromas." .

ns1:ProfessionalExpertReview rdfs:subClassOf ns1:ExpertReview .

ns1:RedMeatPairing rdfs:subClassOf ns1:MeatPairing .

ns1:RegionalBronzeMedal rdfs:subClassOf ns1:BronzeMedal .

ns1:RegionalGoldMedal rdfs:subClassOf ns1:GoldMedal .

ns1:RegionalSilverMedal rdfs:subClassOf ns1:SilverMedal .

ns1:RegionalWineClassification a ns1:RegionalWine ;
    rdfs:comment "A classification for wines that meet regional standards but are below VQPRD in quality." .

ns1:RichTextureNote rdfs:subClassOf ns1:TextureNote .

ns1:RootVegetablePairing rdfs:subClassOf ns1:VegetablePairing .

ns1:RoseWine ns1:hasVariety ns1:RedVariety,
        ns1:WhiteRedVarietyCombination ;
    ns1:usedToProduce ns1:HalfMaceration ;
    rdfs:comment "A type of wine produced from red grape varieties with half maceration." ;
    rdfs:subClassOf ns1:Wine .

ns1:ShellfishPairing rdfs:subClassOf ns1:SeafoodPairing .

ns1:ShortTermBottleAging rdfs:subClassOf ns1:BottleAging .

ns1:SmallScaleProducer rdfs:comment "A producer known for small-scale, artisanal wine production." ;
    rdfs:subClassOf ns1:Producer .

ns1:SmoothTextureNote rdfs:subClassOf ns1:TextureNote .

ns1:SparklingRedWine rdfs:subClassOf ns1:RedWine,
        ns1:SparklingWine .

ns1:SparklingWhiteWine rdfs:subClassOf ns1:SparklingWine,
        ns1:WhiteWine .

ns1:SpicyAromaNote rdfs:subClassOf ns1:AromaNote .

ns1:StainlessSteelAging rdfs:subClassOf ns1:BarrelAging .

ns1:SweetFlavorNote rdfs:subClassOf ns1:FlavorNote .

ns1:SweetRedWine rdfs:subClassOf ns1:RedWine,
        ns1:SweetWine .

ns1:SweetWhiteWine rdfs:subClassOf ns1:SweetWine,
        ns1:WhiteWine .

ns1:TableWineClassification a ns1:TableWine ;
    rdfs:comment "A classification for basic wines that do not meet higher quality standards." .

ns1:TannicFlavorNote rdfs:subClassOf ns1:FlavorNote .

ns1:TaylorFladgate a ns1:Producer ;
    ns1:hasReputationScore 92 ;
    ns1:producedBy ns1:PortWineExample ;
    rdfs:comment "A wine producer in the Alentejo region, known for its Port wines." .

ns1:User1Preference a ns1:UserPreference ;
    ns1:hasPreferredColor "red" ;
    ns1:hasPreferredSmell "earthy" ;
    ns1:hasPreferredTaste "dry" ;
    rdfs:comment "A user preference for dry, red wines with an earthy smell." .

ns1:User2Preference a ns1:UserPreference ;
    ns1:hasPreferredColor "white" ;
    ns1:hasPreferredSmell "floral" ;
    ns1:hasPreferredTaste "sweet" ;
    rdfs:comment "A user preference for sweet, white wines with a floral smell." .

ns1:VQPRDClassification a ns1:VQPRD ;
    rdfs:comment "A quality classification for wines in the European Union, below DOC." .

ns1:WarmClimateRegion rdfs:comment "A wine region with a warm climate, producing wines with high sugar content." ;
    rdfs:subClassOf ns1:Region .

ns1:WhiteMeatPairing rdfs:subClassOf ns1:MeatPairing .

ns1:WineDatabaseExample a ns1:WineDatabase ;
    ns1:interoperatesWith ns1:WineOntology ;
    rdfs:comment "A database containing information about wines, interoperable with the wine ontology." .

ns1:WineKnowledgeBase a ns1:WineKnowledge ;
    ns1:isManagedBy ns1:WineOntology ;
    ns1:isRetrievedBy ns1:WineOntology ;
    rdfs:comment "A knowledge base containing information about wines, managed and retrieved by the wine ontology." .

ns1:achievedBy a owl:FunctionalProperty ;
    rdfs:comment "Relates an overmature state to the method used to achieve it." ;
    rdfs:domain ns1:Overmature ;
    rdfs:range ns1:GrapeState ;
    owl:inverseOf ns1:achieves .

ns1:affects a owl:ReflexiveProperty ;
    rdfs:comment "Relates a grape maturity state to the wine characteristics it affects." ;
    rdfs:domain ns1:GrapeMaturityState ;
    rdfs:range ns1:WineCharacteristic ;
    owl:inverseOf ns1:isAffectedBy .

ns1:agedIn a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to the aging process it undergoes." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:AgingProcess ;
    owl:inverseOf ns1:agesWine .

ns1:categorizedBy a owl:FunctionalProperty ;
    rdfs:comment "Relates an ambiguous characteristic to the category it belongs to." ;
    rdfs:domain ns1:AmbiguousCharacteristic ;
    rdfs:range ns1:CharacteristicCategory ;
    owl:inverseOf ns1:categorizes .

ns1:contributesTo a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates a grape variety to the wine quality it contributes to.",
        "Relates a wine characteristic to the evaluation criteria it contributes to." ;
    rdfs:domain ns1:GrapeVariety,
        ns1:WineCharacteristic ;
    rdfs:range ns1:WineEvaluation,
        ns1:WineQuality ;
    owl:inverseOf ns1:isContributedBy,
        ns1:isContributedByVariety .

ns1:distinguishedBy a owl:FunctionalProperty ;
    rdfs:comment "Relates a sparkling wine to the characteristic that distinguishes it." ;
    rdfs:domain ns1:SparklingWine ;
    rdfs:range ns1:Characteristic ;
    owl:inverseOf ns1:distinguishes .

ns1:facilitatesInteroperabilityWith a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates the wine ontology to the databases and systems it facilitates interoperability with." ;
    rdfs:domain ns1:WineOntology ;
    rdfs:range ns1:Entity ;
    owl:inverseOf ns1:interoperatesWith .

ns1:hasAlcoholPercentage a owl:DatatypeProperty ;
    rdfs:comment "The percentage of alcohol in a wine." ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:decimal .

ns1:hasAromaIntensity a owl:DatatypeProperty ;
    rdfs:comment "The intensity of a wine's aroma, described as a string (e.g., light, medium, strong)." ;
    rdfs:domain ns1:Aroma ;
    rdfs:range xsd:string .

ns1:hasAward a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to the awards it has received." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineAward ;
    owl:inverseOf ns1:isAwardedTo .

ns1:hasCharacteristic a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates a wine to its characteristics." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineCharacteristic ;
    owl:inverseOf ns1:isCharacteristicOf .

ns1:hasDegreeOfAcidity a owl:DatatypeProperty ;
    rdfs:comment "The degree of acidity in a wine, expressed as a decimal value." ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:decimal .

ns1:hasFlavourIntensity a owl:DatatypeProperty ;
    rdfs:comment "The intensity of a wine's flavour, described as a string (e.g., light, medium, full)." ;
    rdfs:domain ns1:Flavour ;
    rdfs:range xsd:string .

ns1:hasFoodPairing a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its recommended food pairings." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:FoodPairing ;
    owl:inverseOf ns1:isFoodPairingOf .

ns1:hasPreference a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates a user preference to the category it belongs to." ;
    rdfs:domain ns1:UserPreference ;
    rdfs:range ns1:PreferenceCategory ;
    owl:inverseOf ns1:isPreferenceOf .

ns1:hasPreferredColor a owl:DatatypeProperty ;
    rdfs:comment "The preferred color of a user, described as a string (e.g., red, white)." ;
    rdfs:domain ns1:UserPreference ;
    rdfs:range xsd:string .

ns1:hasPreferredSmell a owl:DatatypeProperty ;
    rdfs:comment "The preferred smell of a user, described as a string (e.g., fruity, floral)." ;
    rdfs:domain ns1:UserPreference ;
    rdfs:range xsd:string .

ns1:hasPreferredTaste a owl:DatatypeProperty ;
    rdfs:comment "The preferred taste of a user, described as a string (e.g., sweet, dry)." ;
    rdfs:domain ns1:UserPreference ;
    rdfs:range xsd:string .

ns1:hasPriceValue a owl:DatatypeProperty ;
    rdfs:comment "The price of a wine, expressed as a positive integer." ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:positiveInteger .

ns1:hasProductionYear a owl:DatatypeProperty ;
    rdfs:comment "The year a specific wine (e.g., Barca Velha) was produced, expressed as a gYear." ;
    rdfs:domain ns1:BarcaVelha ;
    rdfs:range xsd:gYear .

ns1:hasReputation a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates a producer to its reputation." ;
    rdfs:domain ns1:Producer ;
    rdfs:range ns1:Reputation ;
    owl:inverseOf ns1:isReputationOf .

ns1:hasReputationScore a owl:DatatypeProperty ;
    rdfs:comment "The reputation score of a producer, expressed as an integer." ;
    rdfs:domain ns1:Producer ;
    rdfs:range xsd:integer .

ns1:hasReview a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its reviews." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineReview ;
    owl:inverseOf ns1:isReviewOf .

ns1:hasSignificance a owl:AnnotationProperty ;
    rdfs:comment "Relates a specific wine to its significance." ;
    rdfs:domain ns1:BarcaVelha ;
    rdfs:range ns1:Significance .

ns1:hasSugarLevel a owl:DatatypeProperty ;
    rdfs:comment "The level of sugar in a grape maturity state, expressed as a decimal value." ;
    rdfs:domain ns1:GrapeMaturityState ;
    rdfs:range xsd:decimal .

ns1:hasTanninLevel a owl:DatatypeProperty ;
    rdfs:comment "The level of tannins in a wine, described as a string (e.g., low, medium, high)." ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:string .

ns1:hasTastingNote a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its tasting notes." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:TastingNote ;
    owl:inverseOf ns1:isTastingNoteOf .

ns1:hasVariety a owl:ReflexiveProperty ;
    rdfs:comment "Relates a wine to the grape varieties used in its production." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:GrapeVariety ;
    owl:inverseOf ns1:isVarietyOf .

ns1:hasVintage a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its vintage year." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:Vintage ;
    owl:inverseOf ns1:isVintageOf .

ns1:hasVintageYear a owl:DatatypeProperty ;
    rdfs:comment "The year a wine was produced, expressed as a gYear." ;
    rdfs:domain ns1:Wine ;
    rdfs:range xsd:gYear .

ns1:hasVisionIntensity a owl:DatatypeProperty ;
    rdfs:comment "The intensity of a wine's visual characteristics, described as a string (e.g., pale, medium, deep)." ;
    rdfs:domain ns1:Vision ;
    rdfs:range xsd:string .

ns1:influences a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates a region to the wine characteristics it influences." ;
    rdfs:domain ns1:Region ;
    rdfs:range ns1:WineCharacteristic ;
    owl:inverseOf ns1:isInfluencedBy .

ns1:interruptedBy a owl:FunctionalProperty ;
    rdfs:comment "Relates a sweet wine to the method used to interrupt its fermentation." ;
    rdfs:domain ns1:SweetWine ;
    rdfs:range ns1:InterruptionMethod ;
    owl:inverseOf ns1:interrupts .

ns1:isSignificanceOf rdfs:comment "Relates a significance to the specific wine it belongs to." ;
    rdfs:domain ns1:Significance ;
    rdfs:range ns1:BarcaVelha .

ns1:manages a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates the wine ontology to the knowledge it manages." ;
    rdfs:domain ns1:WineOntology ;
    rdfs:range ns1:WineKnowledge ;
    owl:inverseOf ns1:isManagedBy .

ns1:partOf a owl:FunctionalProperty ;
    rdfs:comment "Relates a region to the country it is part of." ;
    rdfs:domain ns1:Region ;
    rdfs:range ns1:Country ;
    owl:inverseOf ns1:hasPart .

ns1:primaryAgingProcess a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its primary aging process." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:AgingProcess ;
    owl:inverseOf ns1:isPrimaryAgingProcessOf .

ns1:primaryAward a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its primary award." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineAward ;
    owl:inverseOf ns1:isPrimaryAwardOf .

ns1:primaryFoodPairing a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its primary food pairing." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:FoodPairing ;
    owl:inverseOf ns1:isPrimaryFoodPairingOf .

ns1:primaryProducer a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its primary producer." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:Producer ;
    owl:inverseOf ns1:isPrimaryProducerOf .

ns1:primaryRegion a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its primary region of production." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:Region ;
    owl:inverseOf ns1:isPrimaryRegionOf .

ns1:primaryReview a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its primary review." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineReview ;
    owl:inverseOf ns1:isPrimaryReviewOf .

ns1:primaryTastingNote a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its primary tasting note." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:TastingNote ;
    owl:inverseOf ns1:isPrimaryTastingNoteOf .

ns1:primaryVariety a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its primary grape variety." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:GrapeVariety ;
    owl:inverseOf ns1:isPrimaryVarietyOf .

ns1:producedBy a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates a wine to the fermentation process used to produce it." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:FermentationProcess ;
    owl:inverseOf ns1:produces .

ns1:producedInYear a owl:AnnotationProperty ;
    rdfs:comment "Relates a wine to the year it was produced in." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:YearOfProduction .

ns1:producesWineInYear rdfs:comment "Relates a year to the wine produced in it." ;
    rdfs:domain ns1:YearOfProduction ;
    rdfs:range ns1:Wine .

ns1:recommendedBasedOn a owl:FunctionalProperty ;
    rdfs:comment "Relates a wine to the user preferences it is recommended based on." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:UserPreference ;
    owl:inverseOf ns1:recommendsWine .

ns1:retrieves a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates the wine ontology to the knowledge it retrieves." ;
    rdfs:domain ns1:WineOntology ;
    rdfs:range ns1:WineKnowledge ;
    owl:inverseOf ns1:isRetrievedBy .

ns1:secondaryAgingProcess a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its secondary aging process." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:AgingProcess ;
    owl:inverseOf ns1:isSecondaryAgingProcessOf .

ns1:secondaryAward a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its secondary award." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineAward ;
    owl:inverseOf ns1:isSecondaryAwardOf .

ns1:secondaryFoodPairing a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its secondary food pairing." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:FoodPairing ;
    owl:inverseOf ns1:isSecondaryFoodPairingOf .

ns1:secondaryProducer a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its secondary producer." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:Producer ;
    owl:inverseOf ns1:isSecondaryProducerOf .

ns1:secondaryRegion a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its secondary region of production." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:Region ;
    owl:inverseOf ns1:isSecondaryRegionOf .

ns1:secondaryReview a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its secondary review." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:WineReview ;
    owl:inverseOf ns1:isSecondaryReviewOf .

ns1:secondaryTastingNote a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its secondary tasting note." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:TastingNote ;
    owl:inverseOf ns1:isSecondaryTastingNoteOf .

ns1:secondaryVariety a owl:ObjectProperty ;
    rdfs:comment "Relates a wine to its secondary grape variety." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:GrapeVariety ;
    owl:inverseOf ns1:isSecondaryVarietyOf .

ns1:supportsIntegrationOf a owl:FunctionalProperty,
        owl:ReflexiveProperty ;
    rdfs:comment "Relates the wine ontology to the entities it supports integration of." ;
    rdfs:domain ns1:WineOntology ;
    rdfs:range ns1:Entity ;
    owl:inverseOf ns1:isIntegratedBy .

ns1:usedToProduce a owl:FunctionalProperty ;
    rdfs:comment "Relates a wine to the maceration process used to produce it." ;
    rdfs:domain ns1:Wine ;
    rdfs:range ns1:MacerationProcess ;
    owl:inverseOf ns1:producesWine .

ns1:AlcoholPercentage rdfs:comment "The percentage of alcohol in a wine." ;
    rdfs:subClassOf ns1:WineCharacteristic .

ns1:BalancedSweetness a ns1:Sweetness ;
    ns1:isAffectedBy ns1:MatureGrapes ;
    rdfs:comment "A balanced level of sweetness in a wine, resulting from mature grapes." .

ns1:BarcaVelhaWine a ns1:RedWine ;
    ns1:hasAlcoholPercentage 14.0 ;
    ns1:hasDegreeOfAcidity 5.0 ;
    ns1:hasPriceValue "100"^^xsd:positiveInteger ;
    ns1:hasSignificance "High-quality wine produced only in exceptional years" ;
    ns1:hasTanninLevel "high" ;
    ns1:producedIn ns1:AlentejoRegion ;
    ns1:producedInYear "2018"^^xsd:gYear ;
    rdfs:comment "A high-quality red wine from the Alentejo region, produced only in exceptional years." .

ns1:BordeauxRegion a ns1:Region ;
    ns1:partOf ns1:France ;
    rdfs:comment "A wine region in France known for its full-bodied red wines made from Cabernet Sauvignon and Merlot." .

