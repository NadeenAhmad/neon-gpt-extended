@prefix : <http://purl.org/cheminf/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix sio: <http://semanticscience.org/resource/> .


<http://purl.org/cheminf/ontology>
    a owl:Ontology ;
    dc:title "Chemical Information Ontology (CHEMINF)" ;
    dc:description "The Chemical Information Ontology (CHEMINF) provides a formal, extensible, and semantically rich framework for representing chemical descriptors, molecular formats, algorithms, software, and provenance. It supports semantic reasoning, data integration, and reproducibility across cheminformatics and life sciences." ;
    owl:versionIRI <http://purl.org/cheminf/ontology/1.0.0> ;
    owl:versionInfo "1.0.0" ;
    rdfs:label "CHEMINF Ontology" .

:ChemicalEntity a owl:Class ;
    rdfs:label "ChemicalEntity" ;
    rdfs:comment "A discrete chemical compound or molecular species." .

:ChemicalDescriptor a owl:Class ;
    rdfs:label "ChemicalDescriptor" ;
    rdfs:comment "A measurable or calculated property of a chemical entity." .

:DescriptorValue a owl:Class ;
    rdfs:label "DescriptorValue" ;
    rdfs:comment "The numerical or categorical result of a descriptor calculation." .

:DataFormat a owl:Class ;
    rdfs:label "DataFormat" ;
    rdfs:comment "A data encoding standard used to represent chemical information." .

:ProvenanceInformation a owl:Class ;
    rdfs:label "ProvenanceInformation" ;
    rdfs:comment "Metadata about the source and method of descriptor calculation." .

:ChemicalGraph a owl:Class ;
    rdfs:label "ChemicalGraph" ;
    rdfs:comment "A graph-based representation of molecular structure." .

:SMILESFormat a owl:Class ;
    rdfs:label "SMILESFormat" ;
    rdfs:comment "A line notation format for representing chemical structures." .

:InChIFormat a owl:Class ;
    rdfs:label "InChIFormat" ;
    rdfs:comment "An IUPAC textual identifier for chemical substances." .

:Algorithm a owl:Class ;
    rdfs:label "Algorithm" ;
    rdfs:comment "A formal procedure for calculating descriptors or performing analyses." .

:SoftwareImplementation a owl:Class ;
    rdfs:label "SoftwareImplementation" ;
    rdfs:comment "A software tool or library implementing algorithms." .

:SoftwareTool a owl:Class ;
    rdfs:label "SoftwareTool" ;
    rdfs:comment "A specific type of SoftwareImplementation used in cheminformatics." .

:SoftwareVersion a owl:Class ;
    rdfs:label "SoftwareVersion" ;
    rdfs:comment "The version identifier of a software tool." .

:SimilarityScore a owl:Class ;
    rdfs:label "SimilarityScore" ;
    rdfs:comment "A numeric score representing structural similarity." .

:SubstructureFingerprint a owl:Class ;
    rdfs:label "SubstructureFingerprint" ;
    rdfs:comment "A binary or hashed pattern of structural features." .

:TanimotoCoefficient a owl:Class ;
    rdfs:label "TanimotoCoefficient" ;
    rdfs:comment "A metric used to quantify fingerprint-based similarity." .

:QSAR a owl:Class ;
    rdfs:label "QSAR" ;
    rdfs:comment "Quantitative structure–activity relationship model." .

:ToxicityPrediction a owl:Class ;
    rdfs:label "ToxicityPrediction" ;
    rdfs:comment "A predictive model for chemical toxicity." .

:ChemicalDataset a owl:Class ;
    rdfs:label "ChemicalDataset" ;
    rdfs:comment "A structured collection of chemical records." .

:OntologyAlignment a owl:Class ;
    rdfs:label "OntologyAlignment" ;
    rdfs:comment "A mapping between concepts in different ontologies." .

:DescriptorOntology a owl:Class ;
    rdfs:label "DescriptorOntology" ;
    rdfs:comment "An ontology describing chemical descriptors." .

:StructureRepresentation a owl:Class ;
    rdfs:label "StructureRepresentation" ;
    rdfs:comment "An abstract representation of a chemical structure." .

:hasDescriptor a owl:ObjectProperty ;
    rdfs:label "hasDescriptor" ;
    rdfs:comment "Relates a chemical entity to its descriptors." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :ChemicalDescriptor .

:hasValue a owl:ObjectProperty ;
    rdfs:label "hasValue" ;
    rdfs:comment "Links a descriptor to its evaluated value." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :DescriptorValue .

:isCalculatedBy a owl:ObjectProperty ;
    rdfs:label "isCalculatedBy" ;
    rdfs:comment "Links a descriptor to the algorithm used for calculation." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :Algorithm .

:isImplementedIn a owl:ObjectProperty ;
    rdfs:label "isImplementedIn" ;
    rdfs:comment "Links an algorithm to the software that implements it." ;
    rdfs:domain :Algorithm ;
    rdfs:range :SoftwareImplementation .

:represents a owl:ObjectProperty ;
    rdfs:label "represents" ;
    rdfs:comment "Links a graph to the entity it represents." ;
    rdfs:domain :ChemicalGraph ;
    rdfs:range :ChemicalEntity .

:encodedIn a owl:ObjectProperty ;
    rdfs:label "encodedIn" ;
    rdfs:comment "Indicates the format used to encode a structure." ;
    rdfs:domain :ChemicalGraph ;
    rdfs:range :DataFormat .

:hasProvenance a owl:ObjectProperty ;
    rdfs:label "hasProvenance" ;
    rdfs:comment "Associates a descriptor with its provenance metadata." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :ProvenanceInformation .

:hasVersion a owl:ObjectProperty ;
    rdfs:label "hasVersion" ;
    rdfs:comment "Specifies the version of a software implementation." ;
    rdfs:domain :SoftwareImplementation ;
    rdfs:range :SoftwareVersion .


:MolecularWeight a owl:Class ;
    rdfs:label "MolecularWeight" ;
    rdfs:comment "A chemical descriptor representing the molecular weight of a compound." .

:logP a owl:Class ;
    rdfs:label "logP" ;
    rdfs:comment "A chemical descriptor representing the octanol-water partition coefficient." .

:TopologicalPolarSurfaceArea a owl:Class ;
    rdfs:label "TopologicalPolarSurfaceArea" ;
    rdfs:comment "A descriptor estimating the surface area of polar atoms." .

:DescriptorCalculation a owl:Class ;
    rdfs:label "DescriptorCalculation" ;
    rdfs:comment "An event representing the calculation of a descriptor." .

:InferenceEngine a owl:Class ;
    rdfs:label "InferenceEngine" ;
    rdfs:comment "An algorithmic component capable of deriving new descriptor values." .

:RDFGraph a owl:Class ;
    rdfs:label "RDFGraph" ;
    rdfs:comment "An RDF representation of chemical knowledge." .

:SMILES a owl:Class ;
    rdfs:label "SMILES" ;
    rdfs:comment "A textual string in the SMILES format." .

:InChI a owl:Class ;
    rdfs:label "InChI" ;
    rdfs:comment "A textual string in the InChI format." .

:VersionIdentifier a owl:Class ;
    rdfs:label "VersionIdentifier" ;
    rdfs:comment "An identifier representing software version information." .

:Model a owl:Class ;
    rdfs:label "Model" ;
    rdfs:comment "A generalized class for computational predictive models." .

:Ontology a owl:Class ;
    rdfs:label "Ontology" ;
    rdfs:comment "A formal conceptualization of a knowledge domain." .

:Dataset a owl:Class ;
    rdfs:label "Dataset" ;
    rdfs:comment "A general collection of records." .

:GraphRepresentation a owl:Class ;
    rdfs:label "GraphRepresentation" ;
    rdfs:comment "A representation of knowledge as RDF triples." .

:AlgorithmExecution a owl:Class ;
    rdfs:label "AlgorithmExecution" ;
    rdfs:comment "A process in which an algorithm is executed." .

:RDKit a :SoftwareTool ;
    rdfs:comment "Open-source cheminformatics toolkit." .

:OpenBabel a :SoftwareTool ;
    rdfs:comment "Chemical toolbox for file format interconversion." .

:CDK a :SoftwareTool ;
    rdfs:comment "Chemistry Development Kit." .

:SMILES_v1 a :SMILESFormat ;
    rdfs:comment "Standard SMILES encoding." .

:InChI_v1 a :InChIFormat ;
    rdfs:comment "IUPAC InChI 1.0 format." .

:logP_calc_001 a :DescriptorCalculation ;
    rdfs:comment "A logP calculation process." .

:MolWeight_calc_002 a :DescriptorCalculation ;
    rdfs:comment "A molecular weight calculation process." .

:CHEMINF a :DescriptorOntology ;
    rdfs:comment "The Chemical Information Ontology." .

:CHEMINF_Alignment_ChEBI a :OntologyAlignment ;
    rdfs:comment "CHEMINF alignment with ChEBI." .

:QSAR_ToxModel a :QSAR ;
    rdfs:comment "QSAR model for toxicity prediction." .

:ToxicityPredictorX a :ToxicityPrediction ;
    rdfs:comment "Predictive tool for toxicity." .

:DS_001 a :ChemicalDataset ;
    rdfs:comment "Dataset containing environmental compounds." .

:FingerprintSim a :TanimotoCoefficient ;
    rdfs:comment "Fingerprint-based similarity metric." .

:Compound_A a :ChemicalEntity ;
    rdfs:comment "A test chemical compound." .

:Compound_B a :ChemicalEntity ;
    rdfs:comment "Another test chemical compound." .

:Desc_logP a :ChemicalDescriptor ;
    rdfs:comment "logP descriptor." .

:Desc_MW a :ChemicalDescriptor ;
    rdfs:comment "Molecular weight descriptor." .

:RDKit_3.10 a :SoftwareVersion ;
    rdfs:comment "Version 3.10 of RDKit." .

:DescVal_001 a :DescriptorValue ;
    rdfs:comment "logP value of Compound A." .

:DescVal_002 a :DescriptorValue ;
    rdfs:comment "MW value of Compound A." .

:hasConfidenceScore a owl:DatatypeProperty ;
    rdfs:label "hasConfidenceScore" ;
    rdfs:comment "Links a descriptor value to a numeric confidence score." ;
    rdfs:domain :DescriptorValue ;
    rdfs:range xsd:float .

:hasStringValue a owl:DatatypeProperty ;
    rdfs:label "hasStringValue" ;
    rdfs:comment "Literal value of a descriptor as a string." ;
    rdfs:domain :DescriptorValue ;
    rdfs:range xsd:string .

:hasNumericValue a owl:DatatypeProperty ;
    rdfs:label "hasNumericValue" ;
    rdfs:comment "Literal value of a descriptor as a number." ;
    rdfs:domain :DescriptorValue ;
    rdfs:range xsd:float .

:hasName a owl:DatatypeProperty ;
    rdfs:label "hasName" ;
    rdfs:comment "Name or label for a concept or resource." ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

:hasSMILES a owl:DatatypeProperty ;
    rdfs:label "hasSMILES" ;
    rdfs:comment "SMILES representation of a compound." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range xsd:string .

:hasInChI a owl:DatatypeProperty ;
    rdfs:label "hasInChI" ;
    rdfs:comment "InChI representation of a compound." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range xsd:string .

:calculationTimestamp a owl:DatatypeProperty ;
    rdfs:label "calculationTimestamp" ;
    rdfs:comment "Timestamp when the descriptor was calculated." ;
    rdfs:domain :DescriptorCalculation ;
    rdfs:range xsd:dateTime .

:usesDescriptor a owl:ObjectProperty ;
    rdfs:label "usesDescriptor" ;
    rdfs:comment "Associates a QSAR or toxicity model with descriptors it uses." ;
    rdfs:domain :Model ;
    rdfs:range :ChemicalDescriptor .

:usesModel a owl:ObjectProperty ;
    rdfs:label "usesModel" ;
    rdfs:comment "Indicates the model used in a predictive task." ;
    rdfs:domain :ToxicityPrediction ;
    rdfs:range :QSAR .

:trainedOn a owl:ObjectProperty ;
    rdfs:label "trainedOn" ;
    rdfs:comment "Associates a model with the dataset it was trained on." ;
    rdfs:domain :QSAR ;
    rdfs:range :ChemicalDataset .

:contains a owl:ObjectProperty ;
    rdfs:label "contains" ;
    rdfs:comment "Indicates that a dataset includes a particular chemical entity." ;
    rdfs:domain :ChemicalDataset ;
    rdfs:range :ChemicalEntity .

:annotatedWith a owl:ObjectProperty ;
    rdfs:label "annotatedWith" ;
    rdfs:comment "Indicates which ontology was used for annotation." ;
    rdfs:domain :ChemicalDataset ;
    rdfs:range :DescriptorOntology .

:describedIn a owl:ObjectProperty ;
    rdfs:label "describedIn" ;
    rdfs:comment "Indicates the RDF graph a chemical entity is described in." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :RDFGraph .

:reuses a owl:ObjectProperty ;
    rdfs:label "reuses" ;
    rdfs:comment "Indicates reuse of an ontology resource." ;
    rdfs:domain :DescriptorOntology ;
    rdfs:range :Ontology .

:alignedWith a owl:ObjectProperty ;
    rdfs:label "alignedWith" ;
    rdfs:comment "Links an ontology alignment to its target ontology." ;
    rdfs:domain :OntologyAlignment ;
    rdfs:range :Ontology .

:infers a owl:ObjectProperty ;
    rdfs:label "infers" ;
    rdfs:comment "Indicates that an inference engine can infer a descriptor value." ;
    rdfs:domain :InferenceEngine ;
    rdfs:range :DescriptorValue .

:operatesOn a owl:ObjectProperty ;
    rdfs:label "operatesOn" ;
    rdfs:comment "Specifies the dataset an inference engine operates on." ;
    rdfs:domain :InferenceEngine ;
    rdfs:range :ChemicalDataset .

:isUsedIn a owl:ObjectProperty ;
    rdfs:label "isUsedIn" ;
    rdfs:comment "Indicates where a fingerprint is used." ;
    rdfs:domain :SubstructureFingerprint ;
    rdfs:range :TanimotoCoefficient .

:generates a owl:ObjectProperty ;
    rdfs:label "generates" ;
    rdfs:comment "Indicates the generation of a score from a fingerprint." ;
    rdfs:domain :SubstructureFingerprint ;
    rdfs:range :SimilarityScore .

:hasFingerprint a owl:ObjectProperty ;
    rdfs:label "hasFingerprint" ;
    rdfs:comment "Relates a compound to its fingerprint." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :SubstructureFingerprint .

:hasSimilarityScore a owl:ObjectProperty ;
    rdfs:label "hasSimilarityScore" ;
    rdfs:comment "Relates a compound to its similarity score." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :SimilarityScore .

:computedUsing a owl:ObjectProperty ;
    rdfs:label "computedUsing" ;
    rdfs:comment "Indicates what fingerprint was used to compute a similarity score." ;
    rdfs:domain :SimilarityScore ;
    rdfs:range :SubstructureFingerprint .

:hasInput a owl:ObjectProperty ;
    rdfs:label "hasInput" ;
    rdfs:comment "Relates a calculation to its input." ;
    rdfs:domain :DescriptorCalculation ;
    rdfs:range :ChemicalEntity .

:hasOutput a owl:ObjectProperty ;
    rdfs:label "hasOutput" ;
    rdfs:comment "Relates a calculation to its output." ;
    rdfs:domain :DescriptorCalculation ;
    rdfs:range :DescriptorValue .

:usesAlgorithm a owl:ObjectProperty ;
    rdfs:label "usesAlgorithm" ;
    rdfs:comment "Links a descriptor calculation to the algorithm used." ;
    rdfs:domain :DescriptorCalculation ;
    rdfs:range :Algorithm .

:performedBy a owl:ObjectProperty ;
    rdfs:label "performedBy" ;
    rdfs:comment "Indicates which software tool performed a calculation." ;
    rdfs:domain :DescriptorCalculation ;
    rdfs:range :SoftwareTool .

:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:label "wasGeneratedBy" ;
    rdfs:comment "Indicates which tool generated provenance data." ;
    rdfs:domain :ProvenanceInformation ;
    rdfs:range :SoftwareTool .

:refersTo a owl:ObjectProperty ;
    rdfs:label "refersTo" ;
    rdfs:comment "Relates provenance metadata to the calculation it describes." ;
    rdfs:domain :ProvenanceInformation ;
    rdfs:range :DescriptorCalculation .

:wasDerivedFrom a owl:ObjectProperty ;
    rdfs:label "wasDerivedFrom" ;
    rdfs:comment "Indicates the source graph for provenance data." ;
    rdfs:domain :ProvenanceInformation ;
    rdfs:range :RDFGraph .

:hasStructure a owl:ObjectProperty ;
    rdfs:label "hasStructure" ;
    rdfs:comment "Links a chemical entity to its molecular structure." ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :MolecularStructure .

:encodedIn a owl:ObjectProperty ;
    rdfs:label "encodedIn" ;
    rdfs:comment "Indicates the format used to encode a structure." ;
    rdfs:domain :MolecularStructure ;
    rdfs:range :DataFormat .

:isFormatFor a owl:ObjectProperty ;
    rdfs:label "isFormatFor" ;
    rdfs:comment "States which object a format is used for." ;
    rdfs:domain :DataFormat ;
    rdfs:range :owl:Thing .

:logP rdfs:subClassOf :ChemicalDescriptor .
:MolecularWeight rdfs:subClassOf :ChemicalDescriptor .
:TopologicalPolarSurfaceArea rdfs:subClassOf :ChemicalDescriptor .
:TanimotoCoefficient rdfs:subClassOf :SimilarityScore .
:SubstructureFingerprint rdfs:subClassOf :ChemicalDescriptor .
:QSAR rdfs:subClassOf :Model .
:ToxicityPrediction rdfs:subClassOf :QSAR .
:SMILESFormat rdfs:subClassOf :DataFormat .
:InChIFormat rdfs:subClassOf :DataFormat .
:MolecularStructure rdfs:subClassOf :ChemicalGraph .
:StructureRepresentation rdfs:subClassOf :MolecularStructure .
:DescriptorCalculation rdfs:subClassOf :AlgorithmExecution .
:SoftwareTool rdfs:subClassOf :SoftwareImplementation .
:InferenceEngine rdfs:subClassOf :Algorithm .
:ChemicalDataset rdfs:subClassOf :Dataset .
:RDFGraph rdfs:subClassOf :GraphRepresentation .
:DescriptorOntology rdfs:subClassOf :Ontology .
:SoftwareVersion rdfs:subClassOf :VersionIdentifier .




# Data Properties for descriptors and structures

:hasLogPValue a owl:DatatypeProperty ;
    rdfs:label "hasLogPValue" ;
    rdfs:comment "The logP (partition coefficient) value of a compound, indicating its hydrophobicity." ;
    rdfs:domain :logP ;
    rdfs:range xsd:float .

:hasMolecularWeightValue a owl:DatatypeProperty ;
    rdfs:label "hasMolecularWeightValue" ;
    rdfs:comment "The molecular weight of a compound, expressed as a float in Daltons." ;
    rdfs:domain :MolecularWeight ;
    rdfs:range xsd:float .

:hasTopologicalPolarSurfaceAreaValue a owl:DatatypeProperty ;
    rdfs:label "hasTopologicalPolarSurfaceAreaValue" ;
    rdfs:comment "The topological polar surface area (TPSA) value of a compound in square angstroms." ;
    rdfs:domain :TopologicalPolarSurfaceArea ;
    rdfs:range xsd:float .

:hasSMILESString a owl:DatatypeProperty ;
    rdfs:label "hasSMILESString" ;
    rdfs:comment "SMILES string representation of a molecule." ;
    rdfs:domain :SMILESFormat ;
    rdfs:range xsd:string .

:hasInChIString a owl:DatatypeProperty ;
    rdfs:label "hasInChIString" ;
    rdfs:comment "InChI string representation of a molecule." ;
    rdfs:domain :InChIFormat ;
    rdfs:range xsd:string .

:hasVersionNumber a owl:DatatypeProperty ;
    rdfs:label "hasVersionNumber" ;
    rdfs:comment "Version number associated with a software implementation." ;
    rdfs:domain :SoftwareImplementation ;
    rdfs:range xsd:string .

:hasAlgorithmName a owl:DatatypeProperty ;
    rdfs:label "hasAlgorithmName" ;
    rdfs:comment "Name of an algorithm used for calculating descriptors or predictions." ;
    rdfs:domain :Algorithm ;
    rdfs:range xsd:string .

:hasDescriptorLabel a owl:DatatypeProperty ;
    rdfs:label "hasDescriptorLabel" ;
    rdfs:comment "Human-readable label or name for a descriptor." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range xsd:string .

:hasDescriptorUnit a owl:DatatypeProperty ;
    rdfs:label "hasDescriptorUnit" ;
    rdfs:comment "Unit of measurement for the descriptor value (e.g., Daltons, Å², logP)." ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range xsd:string .

:hasFormatName a owl:DatatypeProperty ;
    rdfs:label "hasFormatName" ;
    rdfs:comment "Name of the format used to represent chemical data (e.g., SMILES, InChI)." ;
    rdfs:domain :DataFormat ;
    rdfs:range xsd:string .

:hasSoftwareLabel a owl:DatatypeProperty ;
    rdfs:label "hasSoftwareLabel" ;
    rdfs:comment "Descriptive label for the software tool." ;
    rdfs:domain :SoftwareTool ;
    rdfs:range xsd:string .

:hasConfidenceValue a owl:DatatypeProperty ;
    rdfs:label "hasConfidenceValue" ;
    rdfs:comment "Numerical confidence value assigned to a descriptor prediction or similarity score." ;
    rdfs:domain :DescriptorValue ;
    rdfs:range xsd:float .

:hasCalculationTimestamp a owl:DatatypeProperty ;
    rdfs:label "hasCalculationTimestamp" ;
    rdfs:comment "Timestamp indicating when a descriptor calculation was performed." ;
    rdfs:domain :DescriptorCalculation ;
    rdfs:range xsd:dateTime .




:isDescriptorOf a owl:ObjectProperty ;
    rdfs:label "isDescriptorOf" ;
    rdfs:comment "Inverse of hasDescriptor. Indicates the chemical entity to which a descriptor applies." ;
    owl:inverseOf :hasDescriptor ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :ChemicalEntity .

:isValueOf a owl:ObjectProperty ;
    rdfs:label "isValueOf" ;
    rdfs:comment "Inverse of hasValue. Indicates which descriptor produced this value." ;
    owl:inverseOf :hasValue ;
    rdfs:domain :DescriptorValue ;
    rdfs:range :ChemicalDescriptor .

:isCalculatedFor a owl:ObjectProperty ;
    rdfs:label "isCalculatedFor" ;
    rdfs:comment "Inverse of isCalculatedBy. Indicates which descriptor is calculated by the algorithm." ;
    owl:inverseOf :isCalculatedBy ;
    rdfs:domain :Algorithm ;
    rdfs:range :ChemicalDescriptor .

:isImplementedBy a owl:ObjectProperty ;
    rdfs:label "isImplementedBy" ;
    rdfs:comment "Inverse of isImplementedIn. Indicates which software implements the algorithm." ;
    owl:inverseOf :isImplementedIn ;
    rdfs:domain :SoftwareImplementation ;
    rdfs:range :Algorithm .

:isRepresentedBy a owl:ObjectProperty ;
    rdfs:label "isRepresentedBy" ;
    rdfs:comment "Inverse of represents. Indicates which chemical graph represents the entity." ;
    owl:inverseOf :represents ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :ChemicalGraph .

:isEncodingOf a owl:ObjectProperty ;
    rdfs:label "isEncodingOf" ;
    rdfs:comment "Inverse of encodedIn. Indicates what structure format is used by a graph or structure." ;
    owl:inverseOf :encodedIn ;
    rdfs:domain :DataFormat ;
    rdfs:range :ChemicalGraph .

:isProvenanceFor a owl:ObjectProperty ;
    rdfs:label "isProvenanceFor" ;
    rdfs:comment "Inverse of hasProvenance. Indicates which descriptor is associated with provenance metadata." ;
    owl:inverseOf :hasProvenance ;
    rdfs:domain :ProvenanceInformation ;
    rdfs:range :ChemicalDescriptor .

:isVersionOf a owl:ObjectProperty ;
    rdfs:label "isVersionOf" ;
    rdfs:comment "Inverse of hasVersion. Indicates the software that a given version refers to." ;
    owl:inverseOf :hasVersion ;
    rdfs:domain :SoftwareVersion ;
    rdfs:range :SoftwareImplementation .

:isUsedBy a owl:ObjectProperty ;
    rdfs:label "isUsedBy" ;
    rdfs:comment "Inverse of usesDescriptor. Indicates the models that use a given descriptor." ;
    owl:inverseOf :usesDescriptor ;
    rdfs:domain :ChemicalDescriptor ;
    rdfs:range :Model .

:isModelOf a owl:ObjectProperty ;
    rdfs:label "isModelOf" ;
    rdfs:comment "Inverse of usesModel. Indicates the task that uses a model." ;
    owl:inverseOf :usesModel ;
    rdfs:domain :QSAR ;
    rdfs:range :ToxicityPrediction .

:isTrainingDataFor a owl:ObjectProperty ;
    rdfs:label "isTrainingDataFor" ;
    rdfs:comment "Inverse of trainedOn. Indicates the model trained on this dataset." ;
    owl:inverseOf :trainedOn ;
    rdfs:domain :ChemicalDataset ;
    rdfs:range :QSAR .

:isContainedIn a owl:ObjectProperty ;
    rdfs:label "isContainedIn" ;
    rdfs:comment "Inverse of contains. Indicates which dataset contains a given entity." ;
    owl:inverseOf :contains ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range :ChemicalDataset .

:isAnnotatedBy a owl:ObjectProperty ;
    rdfs:label "isAnnotatedBy" ;
    rdfs:comment "Inverse of annotatedWith. Indicates the dataset annotated by an ontology." ;
    owl:inverseOf :annotatedWith ;
    rdfs:domain :DescriptorOntology ;
    rdfs:range :ChemicalDataset .

:isDescribedBy a owl:ObjectProperty ;
    rdfs:label "isDescribedBy" ;
    rdfs:comment "Inverse of describedIn. Indicates the RDF graph that describes a chemical entity." ;
    owl:inverseOf :describedIn ;
    rdfs:domain :RDFGraph ;
    rdfs:range :ChemicalEntity .

:isReusedBy a owl:ObjectProperty ;
    rdfs:label "isReusedBy" ;
    rdfs:comment "Inverse of reuses. Indicates which ontology reused this ontology." ;
    owl:inverseOf :reuses ;
    rdfs:domain :Ontology ;
    rdfs:range :DescriptorOntology .

:isAlignedBy a owl:ObjectProperty ;
    rdfs:label "isAlignedBy" ;
    rdfs:comment "Inverse of alignedWith. Indicates the alignment that maps to this ontology." ;
    owl:inverseOf :alignedWith ;
    rdfs:domain :Ontology ;
    rdfs:range :OntologyAlignment .

:isInferredBy a owl:ObjectProperty ;
    rdfs:label "isInferredBy" ;
    rdfs:comment "Inverse of infers. Indicates the inference engine that inferred this value." ;
    owl:inverseOf :infers ;
    rdfs:domain :DescriptorValue ;
    rdfs:range :InferenceEngine .

:isOperatedBy a owl:ObjectProperty ;
    rdfs:label "isOperatedBy" ;
    rdfs:comment "Inverse of operatesOn. Indicates the engine that operates on a dataset." ;
    owl:inverseOf :operatesOn ;
    rdfs:domain :ChemicalDataset ;
    rdfs:range :InferenceEngine .

:isUseContextOf a owl:ObjectProperty ;
    rdfs:label "isUseContextOf" ;
    rdfs:comment "Inverse of isUsedIn. Indicates the fingerprint use context." ;
    owl:inverseOf :isUsedIn ;
    rdfs:domain :TanimotoCoefficient ;
    rdfs:range :SubstructureFingerprint .

:isGeneratedFrom a owl:ObjectProperty ;
    rdfs:label "isGeneratedFrom" ;
    rdfs:comment "Inverse of generates. Indicates the fingerprint that generated a score." ;
