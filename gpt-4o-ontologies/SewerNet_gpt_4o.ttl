:Cleaning rdf:type owl:Class ;
    rdfs:subClassOf :MaintenanceActivity ;
    rdfs:comment "Routine cleaning of sewer components." .
:Flushing rdf:type owl:Class ;
    rdfs:subClassOf :MaintenanceActivity ;
    rdfs:comment "Flushing operations to clear sediment or debris." .
:RootRemoval rdf:type owl:Class ;
    rdfs:subClassOf :MaintenanceActivity ;
    rdfs:comment "Removal of roots intruding into pipes." .
:OverflowAlarm rdf:type owl:Class ;
    rdfs:subClassOf :SensorDevice ;
    rdfs:comment "Sensor device that detects overflow events." .
:CorrosionSensor rdf:type owl:Class ;
    rdfs:subClassOf :SensorDevice ;
    rdfs:comment "Sensor device that detects corrosion levels." .
:OperationalStatus rdf:type owl:Class ;
    rdfs:subClassOf :ComponentStatus ;
    rdfs:comment "Indicates if the component is operational." .
:DegradedStatus rdf:type owl:Class ;
    rdfs:subClassOf :ComponentStatus ;
    rdfs:comment "Indicates reduced functionality." .
:FailedStatus rdf:type owl:Class ;
    rdfs:subClassOf :ComponentStatus ;
    rdfs:comment "Indicates a component has failed." .
:ResidentialZone rdf:type owl:Class ;
    rdfs:subClassOf :UrbanZoneType ;
    rdfs:comment "Urban region classified as residential." .
:IndustrialZone rdf:type owl:Class ;
    rdfs:subClassOf :UrbanZoneType ;
    rdfs:comment "Urban region classified as industrial." .
:isDetectedBy rdf:type owl:ObjectProperty ;
    rdfs:domain :Overflow ;
    rdfs:range :OverflowAlarm ;
    rdfs:comment "Links an overflow event to the alarm that detected it." .
:requiresRepair rdf:type owl:ObjectProperty ;
    rdfs:domain :StructuralCondition ;
    rdfs:range :Repair ;
    rdfs:comment "Indicates that a condition requires a specific repair." .
:scheduledBy rdf:type owl:ObjectProperty ;
    rdfs:domain :MaintenanceActivity ;
    rdfs:range :MaintenancePlan ;
    rdfs:comment "Connects maintenance to its originating plan." .
:involvesComponent rdf:type owl:ObjectProperty ;
    rdfs:domain :OperationalEvent ;
    rdfs:range :SewerNetworkComponent ;
    rdfs:comment "Links events to involved components." .
:classifiedBy rdf:type owl:ObjectProperty ;
    rdfs:domain :GeospatialRegion ;
    rdfs:range :UrbanZoneType ;
    rdfs:comment "Relates a region to its urban classification." .
:hasInspectionDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :Inspection ;
    rdfs:range xsd:date ;
    rdfs:comment "The date when an inspection was carried out." .
:hasRepairCost rdf:type owl:DatatypeProperty ;
    rdfs:domain :Repair ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The financial cost of a repair." .
:hasDetectionProbability rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorDevice ;
    rdfs:range xsd:float ;
    rdfs:comment "Probability that a sensor detects a fault." .
:hasResponseTime rdf:type owl:DatatypeProperty ;
    rdfs:domain :MaintenanceActivity ;
    rdfs:range xsd:duration ;
    rdfs:comment "Time taken to respond to a maintenance need." .
:hasZoneLabel rdf:type owl:DatatypeProperty ;
    rdfs:domain :UrbanZoneType ;
    rdfs:range xsd:string ;
    rdfs:comment "Label for the urban zone classification." .
:PipeDiameter rdf:type owl:DatatypeProperty ;
    rdfs:domain :Pipe ;
    rdfs:range xsd:float ;
    rdfs:comment "Represents the diameter of a pipe, relevant to hydraulic flow characteristics." .
:PipeMaterial rdf:type owl:DatatypeProperty ;
    rdfs:domain :Pipe ;
    rdfs:range xsd:string ;
    rdfs:comment "Denotes the material composition of the pipe, which impacts durability and load-bearing capacity." .
:InspectionDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :Inspection ;
    rdfs:range xsd:date ;
    rdfs:comment "Specifies the date on which an inspection was performed, important for tracking maintenance schedules." .
:MaintenanceFrequency rdf:type owl:DatatypeProperty ;
    rdfs:domain :MaintenanceActivity ;
    rdfs:range xsd:string ;
    rdfs:comment "Indicates how often maintenance is performed on the infrastructure component." .
:GeospatialCoordinates rdf:type owl:DatatypeProperty ;
    rdfs:domain :GeospatialPoint ;
    rdfs:range xsd:string ;
    rdfs:comment "Describes the geographic location of an infrastructure component using spatial coordinates." .
:PumpCapacity rdf:type owl:DatatypeProperty ;
    rdfs:domain :PumpingStation ;
    rdfs:range xsd:float ;
    rdfs:comment "The maximum volume a pumping station can handle per unit time." .
:ManholeDepth rdf:type owl:DatatypeProperty ;
    rdfs:domain :Manhole ;
    rdfs:range xsd:float ;
    rdfs:comment "Indicates the depth of the manhole from surface to base." .
:OutletDischargeRate rdf:type owl:DatatypeProperty ;
    rdfs:domain :Outlet ;
    rdfs:range xsd:float ;
    rdfs:comment "Specifies the rate at which wastewater is discharged from the outlet." .
:InletCatchmentArea rdf:type owl:DatatypeProperty ;
    rdfs:domain :Inlet ;
    rdfs:range xsd:float ;
    rdfs:comment "Defines the area served by an inlet in square meters." .
:SensorSensitivity rdf:type owl:DatatypeProperty ;
    rdfs:domain :SensorDevice ;
    rdfs:range xsd:float ;
    rdfs:comment "Defines the minimum threshold at which the sensor can detect anomalies." .
:ConditionSeverityLevel rdf:type owl:DatatypeProperty ;
    rdfs:domain :StructuralCondition ;
    rdfs:range xsd:string ;
    rdfs:comment "Describes the severity of a structural condition using qualitative terms." .
:RepairDuration rdf:type owl:DatatypeProperty ;
    rdfs:domain :Repair ;
    rdfs:range xsd:duration ;
    rdfs:comment "Time taken to complete the repair." .
:MaintenancePriority rdf:type owl:DatatypeProperty ;
    rdfs:domain :MaintenanceActivity ;
    rdfs:range xsd:string ;
    rdfs:comment "Indicates how critical a maintenance activity is." .
:OverflowVolume rdf:type owl:DatatypeProperty ;
    rdfs:domain :Overflow ;
    rdfs:range xsd:float ;
    rdfs:comment "The volume of fluid released during an overflow event." .
:BlockageType rdf:type owl:DatatypeProperty ;
    rdfs:domain :Blockage ;
    rdfs:range xsd:string ;
    rdfs:comment "The type of blockage encountered (e.g., grease, roots, debris)." .
:StatusLabel rdf:type owl:DatatypeProperty ;
    rdfs:domain :ComponentStatus ;
    rdfs:range xsd:string ;
    rdfs:comment "Textual label describing the status of the component." .
:ZoneCode rdf:type owl:DatatypeProperty ;
    rdfs:domain :UrbanZoneType ;
    rdfs:range xsd:string ;
    rdfs:comment "Alphanumeric code representing the urban zone classification." .
:RegionName rdf:type owl:DatatypeProperty ;
    rdfs:domain :GeospatialRegion ;
    rdfs:range xsd:string ;
    rdfs:comment "The official name of the geospatial region." .
:PlanIdentifier rdf:type owl:DatatypeProperty ;
    rdfs:domain :MaintenancePlan ;
    rdfs:range xsd:string ;
    rdfs:comment "Unique identifier for a maintenance plan." .
:FindingDescription rdf:type owl:DatatypeProperty ;
    rdfs:domain :Finding ;
    rdfs:range xsd:string ;
    rdfs:comment "Textual explanation of an inspection finding." .
:componentOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasComponent ;
    rdfs:comment "Links a component to the sewer network it belongs to." .
:locationOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasLocation ;
    rdfs:comment "Links a geospatial point to the infrastructure component it describes." .
:isTargetOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :targets ;
    rdfs:comment "Links a sewer component to the inspection targeting it." .
:isAddressedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :addresses ;
    rdfs:comment "Links a structural condition to the maintenance that addresses it." .
:hasBlockage rdf:type owl:ObjectProperty ;
    owl:inverseOf :occursAt ;
    rdfs:comment "Links a pipe to a blockage that occurs in it." .
:causesOverflow rdf:type owl:ObjectProperty ;
    owl:inverseOf :resultsFrom ;
    rdfs:comment "Links a blockage to the overflow it causes." .
:isRestoredBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :restores ;
    rdfs:comment "Links a structural condition to the repair that restores it." .
:isComplianceTargetOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :compliesWith ;
    rdfs:comment "Links a standard to the network complying with it." .
:isAlignedTo rdf:type owl:ObjectProperty ;
    owl:inverseOf :alignsWith ;
    rdfs:comment "Links a directive to the network aligned with it." .
:isMonitoredBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :monitors ;
    rdfs:comment "Links a sewer component to the sensor monitoring it." .
:isDetectedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :detects ;
    rdfs:comment "Links a condition to the sensor detecting it." .
:isProducedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :producesFinding ;
    rdfs:comment "Links a finding to the inspection producing it." .
:hasPerformanceAffectedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :affectsPerformanceOf ;
    rdfs:comment "Links a component to the condition affecting its performance." .
:isStatusOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasStatus ;
    rdfs:comment "Links a status back to the infrastructure component." .
:isZoneTypeOf rdf:type owl:ObjectProperty ;
