@prefix : <http://example.org/sewerNet#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dolce: <http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<http://example.org/sewerNet> rdf:type owl:Ontology .
<http://example.org/sewerNet> rdfs:label "SewerNet Ontology" .
<http://example.org/sewerNet> owl:versionInfo "1.0.0" .
<http://example.org/sewerNet> rdfs:comment "An ontology for modeling wastewater and stormwater infrastructure, events, properties, geospatial data, and regulatory compliance based on RAEPA and INSPIRE standards." .



:SewerNetwork rdf:type owl:Class ; rdfs:comment "A sewer system managing wastewater and stormwater." .
:Pipe rdf:type owl:Class ; rdfs:subClassOf :SewerNetworkComponent ; rdfs:comment "A linear conduit that conveys wastewater or stormwater." .
:Manhole rdf:type owl:Class ; rdfs:subClassOf :SewerNetworkComponent ; rdfs:comment "An access point to underground sewer systems." .
:Inlet rdf:type owl:Class ; rdfs:subClassOf :SewerNetworkComponent ; rdfs:comment "An entry point for stormwater into the network." .
:Outlet rdf:type owl:Class ; rdfs:subClassOf :SewerNetworkComponent ; rdfs:comment "A discharge point for wastewater from the network." .
:PumpingStation rdf:type owl:Class ; rdfs:subClassOf :SewerNetworkComponent ; rdfs:comment "A station used to elevate and move wastewater." .
:SewerNetworkComponent rdf:type owl:Class ; rdfs:comment "Abstract class for physical infrastructure elements in a sewer network." .
:Inspection rdf:type owl:Class ; rdfs:subClassOf :OperationalEvent ; rdfs:comment "An event where sewer components are assessed." .
:MaintenanceActivity rdf:type owl:Class ; rdfs:subClassOf :OperationalEvent ; rdfs:comment "An action aimed at maintaining or repairing sewer components." .
:Blockage rdf:type owl:Class ; rdfs:subClassOf :OperationalEvent ; rdfs:comment "An event where flow is impeded due to obstruction." .
:Overflow rdf:type owl:Class ; rdfs:subClassOf :OperationalEvent ; rdfs:comment "An event where flow exceeds capacity and exits the system." .
:Repair rdf:type owl:Class ; rdfs:subClassOf :MaintenanceActivity ; rdfs:comment "A maintenance activity aimed at restoring structural condition." .
:HydraulicProperty rdf:type owl:Class ; rdfs:comment "A measurable property like flow or diameter that affects hydraulic behavior." .
:StructuralCondition rdf:type owl:Class ; rdfs:comment "A property describing physical wear or integrity of a component." .
:GeospatialData rdf:type owl:Class ; rdfs:comment "Data defining the spatial properties of components." .
:GeospatialPoint rdf:type owl:Class ; rdfs:subClassOf :GeospatialData ; rdfs:comment "A specific geographic point using coordinates." .
:RAEPAStandard rdf:type owl:Class ; rdfs:comment "A standard defining requirements for sanitation networks in France." .
:INSPIREDirective rdf:type owl:Class ; rdfs:comment "A European directive for spatial data infrastructure." .
:OperationalEvent rdf:type owl:Class ; rdfs:subClassOf dolce:Perdurant ; rdfs:comment "An event that occurs in time affecting sewer operations." .
:Wastewater rdf:type owl:Class ; rdfs:comment "Used water from residential, commercial, and industrial sources." .
:Stormwater rdf:type owl:Class ; rdfs:comment "Rainwater or melted snow that runs off surfaces." .
:hasComponent rdf:type owl:ObjectProperty ; rdfs:domain :SewerNetwork ; rdfs:range :SewerNetworkComponent ; rdfs:comment "Relates a sewer network to its components." .
:connectedTo rdf:type owl:ObjectProperty ; owl:inverseOf :connectedTo ; owl:SymmetricProperty true ; rdfs:domain :SewerNetworkComponent ; rdfs:range :SewerNetworkComponent ; rdfs:comment "Indicates that two components are connected." .
:hasLocation rdf:type owl:ObjectProperty ; rdfs:domain :SewerNetworkComponent ; rdfs:range :GeospatialPoint ; owl:FunctionalProperty true ; rdfs:comment "Defines the location of a network component." .
:targets rdf:type owl:ObjectProperty ; rdfs:domain :Inspection ; rdfs:range :SewerNetworkComponent ; rdfs:comment "Relates an inspection to the component being inspected." .
:addresses rdf:type owl:ObjectProperty ; rdfs:domain :MaintenanceActivity ; rdfs:range :StructuralCondition ; rdfs:comment "Indicates that maintenance addresses a particular structural condition." .
:occursAt rdf:type owl:ObjectProperty ; rdfs:domain :Blockage ; rdfs:range :Pipe ; rdfs:comment "Indicates where a blockage occurs." .
:resultsFrom rdf:type owl:ObjectProperty ; rdfs:domain :Overflow ; rdfs:range :Blockage ; rdfs:comment "Indicates the blockage that resulted in an overflow." .
:restores rdf:type owl:ObjectProperty ; rdfs:domain :Repair ; rdfs:range :StructuralCondition ; rdfs:comment "Relates a repair to the condition it restores." .
:compliesWith rdf:type owl:ObjectProperty ; rdfs:domain :SewerNetwork ; rdfs:range :RAEPAStandard ; rdfs:comment "Relates a sewer network to the RAEPA standard it complies with." .
:alignsWith rdf:type owl:ObjectProperty ; rdfs:domain :SewerNetwork ; rdfs:range :INSPIREDirective ; rdfs:comment "Relates a sewer network to the INSPIRE directive it aligns with." .
:hasDiameter rdf:type owl:DatatypeProperty ; rdfs:domain :HydraulicProperty ; rdfs:range xsd:float ; rdfs:comment "The internal diameter of a pipe." .
:hasSlope rdf:type owl:DatatypeProperty ; rdfs:domain :HydraulicProperty ; rdfs:range xsd:float ; rdfs:comment "The gradient or slope of a pipe." .
:hasFlowCapacity rdf:type owl:DatatypeProperty ; rdfs:domain :HydraulicProperty ; rdfs:range xsd:float ; rdfs:comment "The maximum flow a component can handle." .
:hasMaterialType rdf:type owl:DatatypeProperty ; rdfs:domain :HydraulicProperty ; rdfs:range xsd:string ; rdfs:comment "The material from which a component is made." .
:hasCorrosionLevel rdf:type owl:DatatypeProperty ; rdfs:domain :StructuralCondition ; rdfs:range xsd:integer ; rdfs:comment "Corrosion level affecting structural integrity." .
:hasBlockageRisk rdf:type owl:DatatypeProperty ; rdfs:domain :StructuralCondition ; rdfs:range xsd:float ; rdfs:comment "Likelihood of blockage due to condition." .
:hasLatitude rdf:type owl:DatatypeProperty ; rdfs:domain :GeospatialPoint ; rdfs:range xsd:float ; rdfs:comment "Latitude coordinate of a point." .
:hasLongitude rdf:type owl:DatatypeProperty ; rdfs:domain :GeospatialPoint ; rdfs:range xsd:float ; rdfs:comment "Longitude coordinate of a point." .
:hasElevation rdf:type owl:DatatypeProperty ; rdfs:domain :GeospatialPoint ; rdfs:range xsd:float ; rdfs:comment "Elevation above sea level." .


:SensorDevice rdf:type owl:Class ;
    rdfs:subClassOf :SewerNetworkComponent ;
    rdfs:comment "A device that monitors structural or hydraulic behavior." .
:EnvironmentalImpact rdf:type owl:Class ;
    rdfs:subClassOf :dolce:Perdurant ;
    rdfs:comment "Consequences of overflow or pollution on the environment." .
:ComponentStatus rdf:type owl:Class ;
    rdfs:subClassOf :InformationObject ;
    rdfs:comment "The status classification of a component (e.g., operational, failed)." .
:FlowCapacity rdf:type owl:Class ;
    rdfs:subClassOf :InformationObject ;
    rdfs:comment "An information object representing hydraulic capacity." .
:UrbanZoneType rdf:type owl:Class ;
    rdfs:subClassOf :InformationObject ;
    rdfs:comment "The classification of urban areas for infrastructure management." .
:GeospatialRegion rdf:type owl:Class ;
    rdfs:subClassOf :GeospatialData ;
    rdfs:comment "A defined spatial area containing network components." .
:Maintenance rdf:type owl:Class ;
    rdfs:subClassOf :OperationalEvent ;
    rdfs:comment "A planned set of maintenance activities." .
:Plan rdf:type owl:Class ;
    rdfs:subClassOf :InformationObject ;
    rdfs:comment "An informational entity capturing maintenance or inspection planning." .
:MaintenancePlan rdf:type owl:Class ;
    rdfs:subClassOf :Plan ;
    rdfs:comment "A structured plan for performing maintenance." .
:Finding rdf:type owl:Class ;
    rdfs:subClassOf :InformationObject ;
    rdfs:comment "An observation made during inspection." .
:monitors rdf:type owl:ObjectProperty ;
    rdfs:domain :SensorDevice ;
    rdfs:range :SewerNetworkComponent ;
    rdfs:comment "A sensor device monitors a specific network component." .
:detects rdf:type owl:ObjectProperty ;
    rdfs:domain :SensorDevice ;
    rdfs:range :StructuralCondition ;
    rdfs:comment "A sensor device detects a condition like corrosion." .
:producesFinding rdf:type owl:ObjectProperty ;
    rdfs:domain :Inspection ;
    rdfs:range :Finding ;
    rdfs:comment "An inspection results in one or more findings." .
:indicatesNonCompliance rdf:type owl:DatatypeProperty ;
    rdfs:domain :Finding ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Indicates if a finding shows a regulation breach." .
:affectsPerformanceOf rdf:type owl:ObjectProperty ;
    rdfs:domain :StructuralCondition ;
    rdfs:range :SewerNetworkComponent ;
    rdfs:comment "Structural condition influences performance." .
:hasStatus rdf:type owl:ObjectProperty ;
    rdfs:domain :SewerNetworkComponent ;
    rdfs:range :ComponentStatus ;
    rdfs:comment "The operational status of a component." .
:hasZoneType rdf:type owl:ObjectProperty ;
    rdfs:domain :GeospatialRegion ;
    rdfs:range :UrbanZoneType ;
    rdfs:comment "Urban classification of a spatial region." .
