:hasStatus rdf:type owl:ObjectProperty ;
    rdf:type owl:FunctionalProperty ;
    rdfs:comment "Each component has a single operational status at a time." .
:hasZoneType rdf:type owl:ObjectProperty ;
    rdf:type owl:FunctionalProperty ;
